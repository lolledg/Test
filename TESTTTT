    -- SISTEMA DE EFECTO DE PANTALLA BLANCA
    local screenEffect = nil
    local currentEffectTween = nil
    
    function CreateWhiteScreenEffect(intensity, duration)
        if screenEffect then
            screenEffect:Destroy()
            if currentEffectTween then
                currentEffectTween:Cancel()
            end
        end
        
        screenEffect = Instance.new("ScreenGui")
        screenEffect.Name = "WhiteScreenEffect"
        screenEffect.ResetOnSpawn = false
        screenEffect.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
        screenEffect.Parent = game.CoreGui
        
        local frame = Instance.new("Frame")
        frame.Size = UDim2.new(1, 0, 1, 0)
        frame.Position = UDim2.new(0, 0, 0, 0)
        frame.BackgroundColor3 = Color3.new(1, 1, 1)
        frame.BackgroundTransparency = 1 - (intensity or 0.3)
        frame.BorderSizePixel = 0
        frame.ZIndex = 999
        frame.Parent = screenEffect
        
        if duration then
            task.delay(duration, function()
                if screenEffect then
                    local tween = TweenService:Create(
                        frame,
                        TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                        {BackgroundTransparency = 1}
                    )
                    tween:Play()
                    tween.Completed:Connect(function()
                        if screenEffect then
                            screenEffect:Destroy()
                            screenEffect = nil
                        end
                    end)
                end
            end)
        end
        
        return screenEffect
    end
    
    function RemoveScreenEffect()
        if screenEffect then
            local frame = screenEffect:FindFirstChildWhichIsA("Frame")
            if frame then
                local tween = TweenService:Create(
                    frame,
                    TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                    {BackgroundTransparency = 1}
                )
                tween:Play()
                tween.Completed:Connect(function()
                    if screenEffect then
                        screenEffect:Destroy()
                        screenEffect = nil
                    end
                end)
            else
                screenEffect:Destroy()
                screenEffect = nil
            end
        end
    end

    local function u27(p24, p25, p26)
        if not p24 or not p25 then return end
        p24.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
        p24.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
        p24.Velocity = Vector3.new(0, 0, 0)
        p24.CFrame = p25.CFrame * (p26 or CFrame.new())
        p24.CanCollide = false
        p25.CanCollide = false
    end

