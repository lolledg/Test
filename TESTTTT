    function stop(p41, p42)
        if not _LocalPlayer2.Character or not _LocalPlayer2.Character:FindFirstChild('LowerTorso') then return end
        coroutine.wrap(function()
            wait(_LocalPlayer2.Character.LowerTorso.BOOMBOXSOUND.TimeLength - 0.1)

            if _LocalPlayer2.Character.LowerTorso.BOOMBOXSOUND.SoundId == 'rbxassetid://' .. p41 and u40 == p42 then
                StopAudio()
            end
        end)()
    end
    
    -- SISTEMA MEJORADO DE ANIMACIONES CON SINCRONIZACI√ìN Y EFECTOS
    local activeAnimations = {}
    local currentSpeedMode = "normal"
    local speedEffectsActive = false
    
    function SyncAnimPlay(animId, speed, duration, priority)
        if not _LocalPlayer2.Character or not _LocalPlayer2.Character:FindFirstChild('Humanoid') then 
            return nil 
        end
        
        -- Detener animaciones similares
        for _, track in pairs(_LocalPlayer2.Character.Humanoid:GetPlayingAnimationTracks()) do
            if track.Animation.AnimationId:match('rbxassetid://' .. animId) then
                track:Stop()
            end
        end
        
        local anim = Instance.new('Animation')
        anim.AnimationId = 'rbxassetid://' .. animId
        
        local track = _LocalPlayer2.Character.Humanoid:LoadAnimation(anim)
        track.Priority = priority or Enum.AnimationPriority.Action4
        
        -- Ajustar velocidad seg√∫n el modo
        local actualSpeed = 1.0
        currentSpeedMode = speed or "normal"
        
        if speed == "ultrafast" then
            actualSpeed = 10.0  -- ¬°VELOCIDAD 10X!
            -- Aplicar efectos de velocidad extrema
            if not speedEffectsActive then
                ApplySpeedEffects("ultrafast")
                speedEffectsActive = true
            end
        elseif speed == "veryfast" then
            actualSpeed = 5.0
        elseif speed == "fast" then
            actualSpeed = 2.0
        elseif speed == "normal" then
            actualSpeed = 1.0
            RemoveSpeedEffects()
        elseif speed == "slow" then
            actualSpeed = 0.5
        elseif speed == "veryslow" then
            actualSpeed = 0.1
            -- Aplicar efecto de pantalla blanca en velocidad muy lenta
            CreateWhiteScreenEffect(0.4, duration)
        elseif speed == "extremeslow" then
            actualSpeed = 0.05
            -- Efecto de pantalla blanca m√°s intenso
            CreateWhiteScreenEffect(0.6, duration)
        else
            actualSpeed = tonumber(speed) or 1.0
        end
        
        track:AdjustSpeed(actualSpeed)
        track:Play()
        anim:Destroy()
        
        -- Aplicar efectos seg√∫n velocidad
        ApplySpeedVisualEffects(actualSpeed)
        
        -- Si hay duraci√≥n, detener despu√©s
        if duration then
            task.delay(duration, function()
                if track then
                    track:Stop()
                    RemoveSpeedEffects()
                    RemoveScreenEffect()
                    speedEffectsActive = false
                end
            end)
        end
        
        activeAnimations[animId] = track
        return track
    end
    
    function ApplySpeedVisualEffects(speed)
        if speed >= 5.0 then
            -- Efectos para velocidad extrema
            if not game:GetService("Workspace").CurrentCamera:FindFirstChild("SpeedBlur") then
                local blur = Instance.new("BlurEffect")
                blur.Name = "SpeedBlur"
                blur.Size = math.min(15, (speed - 5) * 3)
                blur.Parent = game:GetService("Workspace").CurrentCamera
            end
            
            -- Efecto de distorsi√≥n
            if not game:GetService("Workspace").CurrentCamera:FindFirstChild("SpeedColor") then
                local colorCorrection = Instance.new("ColorCorrectionEffect")
                colorCorrection.Name = "SpeedColor"
                colorCorrection.Contrast = 0.2
                colorCorrection.Saturation = 0.1
                colorCorrection.Parent = game:GetService("Workspace").CurrentCamera
            end
        elseif speed <= 0.2 then
            -- Efectos para velocidad lenta
            RemoveSpeedEffects()
        end
    end
    
    function ApplySpeedEffects(mode)
        if mode == "ultrafast" then
            -- Efecto de desenfoque de movimiento
            local blur = Instance.new("BlurEffect")
            blur.Name = "UltraFastBlur"
            blur.Size = 20
            blur.Parent = game:GetService("Workspace").CurrentCamera
            
            -- Efecto de color para velocidad
            local colorCorrection = Instance.new("ColorCorrectionEffect")
            colorCorrection.Name = "UltraFastColor"
            colorCorrection.TintColor = Color3.new(0.8, 0.9, 1)
            colorCorrection.Brightness = 0.1
            colorCorrection.Contrast = 0.3
            colorCorrection.Saturation = -0.2
            colorCorrection.Parent = game:GetService("Workspace").CurrentCamera
            
            -- Efecto de l√≠neas de velocidad
            local sunRays = Instance.new("SunRaysEffect")
            sunRays.Name = "SpeedRays"
            sunRays.Intensity = 0.1
            sunRays.Spread = 0.5
            sunRays.Parent = game:GetService("Workspace").CurrentCamera
        end
    end
    
    function RemoveSpeedEffects()
        local camera = game:GetService("Workspace").CurrentCamera
        
        if camera:FindFirstChild("SpeedBlur") then
            camera.SpeedBlur:Destroy()
        end
        if camera:FindFirstChild("SpeedColor") then
            camera.SpeedColor:Destroy()
        end
        if camera:FindFirstChild("UltraFastBlur") then
            camera.UltraFastBlur:Destroy()
        end
        if camera:FindFirstChild("UltraFastColor") then
            camera.UltraFastColor:Destroy()
        end
        if camera:FindFirstChild("SpeedRays") then
            camera.SpeedRays:Destroy()
        end
    end
    
    function SyncAnimStop(animId)
        if activeAnimations[animId] then
            activeAnimations[animId]:Stop()
            activeAnimations[animId] = nil
        end
        RemoveSpeedEffects()
        RemoveScreenEffect()
    end
    
    -- SISTEMA DE EFECTOS DE SANGRE √âPICOS
    local bloodEffects = {
        bloodSpray = "rbxassetid://8692474612",
        bloodSplat = "rbxassetid://8692475585", 
        bloodPool = "rbxassetid://8692476207",
        bloodDrip = "rbxassetid://8692476734"
    }
    
    function CreateBloodEffect(position, effectType, size, duration)
        local part = Instance.new("Part")
        part.Size = Vector3.new(size, size, size)
        part.Position = position
        part.Transparency = 1
        part.Anchored = true
        part.CanCollide = false
        part.Material = Enum.Material.Neon
        part.BrickColor = BrickColor.new("Really red")
        part.Parent = workspace
        
        local decal = Instance.new("Decal")
        decal.Face = Enum.NormalId.Top
        decal.Texture = bloodEffects[effectType] or bloodEffects.bloodSplat
        decal.Parent = part
        
        -- Efecto de desvanecimiento
        task.spawn(function()
            for i = 1, 10 do
                wait(duration/10)
                part.Transparency = i/10
            end
            part:Destroy()
        end)
        
        return part
    end
    
    function BloodSpray(position, intensity)
        for i = 1, intensity do
            local offset = Vector3.new(
                math.random(-1, 1),
                math.random(0, 2),
                math.random(-1, 1)
            ) * 0.5
            
            CreateBloodEffect(position + offset, "bloodSpray", 0.5, 3)
        end
    end
    
    function CreateBloodTrail(character)
        if not character then return end
        
        local trail = Instance.new("Trail")
        trail.Color = ColorSequence.new(Color3.fromRGB(150, 0, 0))
        trail.Transparency = NumberSequence.new(0.5)
        trail.Lifetime = 1
        trail.Parent = character:FindFirstChild("HumanoidRootPart")
        
        local attachment0 = Instance.new("Attachment")
        attachment0.Parent = character:FindFirstChild("HumanoidRootPart")
        
        local attachment1 = Instance.new("Attachment")
        attachment1.Parent = character:FindFirstChild("HumanoidRootPart")
        attachment1.Position = Vector3.new(0, -2, 0)
        
        trail.Attachment0 = attachment0
        trail.Attachment1 = attachment1
        
        return trail
    end

