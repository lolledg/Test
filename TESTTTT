    end

    -- SISTEMA MEJORADO DE ANIMACIONES CON VELOCIDADES CORREGIDAS
    local activeAnimations = {}
    local currentSpeedMode = "normal"
    local speedEffectsActive = false
    
    function SyncAnimPlay(animId, speed, duration, priority)
        if not _LocalPlayer2.Character or not _LocalPlayer2.Character:FindFirstChild('Humanoid') then 
            return nil 
        end
        
        -- Detener animaciones similares
        for _, track in pairs(_LocalPlayer2.Character.Humanoid:GetPlayingAnimationTracks()) do
            if track.Animation.AnimationId:match('rbxassetid://' .. animId) then
                track:Stop()
            end
        end
        
        local anim = Instance.new('Animation')
        anim.AnimationId = 'rbxassetid://' .. animId
        
        local track = _LocalPlayer2.Character.Humanoid:LoadAnimation(anim)
        track.Priority = priority or Enum.AnimationPriority.Action4
        
        -- VELOCIDADES CORREGIDAS SEG√öN TUS ESPECIFICACIONES
        local actualSpeed = 1.0
        currentSpeedMode = speed or "normal"
        
        if speed == "ultrafast" then
            actualSpeed = 100.0  -- ¬°VELOCIDAD 100X! (r√°pido)
            ApplySpeedEffects("ultrafast")
            speedEffectsActive = true
        elseif speed == "veryfast" then
            actualSpeed = 3.0    -- Velocidad normal predeterminada
        elseif speed == "fast" then
            actualSpeed = 2.0
        elseif speed == "normal" then
            actualSpeed = 1.0
            RemoveSpeedEffects()
        elseif speed == "slow" then
            actualSpeed = 0.5
        elseif speed == "veryslow" then
            actualSpeed = 0.1    -- Velocidad lenta
            -- Pantalla extremadamente blanca pero visible
            CreateWhiteScreenEffect(0.15, duration, false)  -- 15% blanco, no se desvanece
        elseif speed == "extremeslow" then
            actualSpeed = 0.05
            -- Pantalla m√°s blanca pero todav√≠a visible
            CreateWhiteScreenEffect(0.25, duration, false)  -- 25% blanco
        else
            actualSpeed = tonumber(speed) or 1.0
        end
        
        track:AdjustSpeed(actualSpeed)
        track:Play()
        anim:Destroy()
        
        -- Aplicar efectos seg√∫n velocidad
        ApplySpeedVisualEffects(actualSpeed)
        
        -- Si hay duraci√≥n, detener despu√©s
        if duration then
            task.delay(duration, function()
                if track then
                    track:Stop()
                    if speed == "veryslow" or speed == "extremeslow" then
                        -- No remover pantalla blanca inmediatamente
                        task.wait(1)
                    end
                    RemoveSpeedEffects()
                    if speed ~= "veryslow" and speed ~= "extremeslow" then
                        RemoveScreenEffect()
                    end
                    speedEffectsActive = false
                end
            end)
        end
        
        activeAnimations[animId] = track
        return track
    end
    
    function ApplySpeedVisualEffects(speed)
        local camera = game:GetService("Workspace").CurrentCamera
        
        -- Limpiar efectos antiguos primero
        RemoveSpeedEffects()
        
        if speed >= 50.0 then
            -- Efectos para velocidad 100x
            local blur = Instance.new("BlurEffect")
            blur.Name = "SpeedBlur"
            blur.Size = 35
            blur.Parent = camera
            
            local colorCorrection = Instance.new("ColorCorrectionEffect")
            colorCorrection.Name = "SpeedColor"
            colorCorrection.TintColor = Color3.new(0.7, 0.8, 1)
            colorCorrection.Brightness = 0.3
            colorCorrection.Contrast = 0.4
            colorCorrection.Saturation = -0.3
            colorCorrection.Parent = camera
            
            -- Efecto de l√≠neas de velocidad
            local sunRays = Instance.new("SunRaysEffect")
            sunRays.Name = "SpeedRays"
            sunRays.Intensity = 0.2
            sunRays.Spread = 1.0
            sunRays.Parent = camera
            
        elseif speed >= 2.0 and speed < 50.0 then
            -- Efectos para velocidad 3x
            local blur = Instance.new("BlurEffect")
            blur.Name = "SpeedBlur"
            blur.Size = 8
            blur.Parent = camera
            
        elseif speed <= 0.2 then
            -- Para velocidad lenta, solo pantalla blanca (ya aplicada)
            RemoveSpeedEffects()
        end
    end
    
    function ApplySpeedEffects(mode)
        local camera = game:GetService("Workspace").CurrentCamera
        
        if mode == "ultrafast" then
            -- Efecto de desenfoque extremo
            local blur = Instance.new("BlurEffect")
            blur.Name = "UltraFastBlur"
            blur.Size = 40
            blur.Parent = camera
            
            -- Efecto de color para velocidad extrema
            local colorCorrection = Instance.new("ColorCorrectionEffect")
            colorCorrection.Name = "UltraFastColor"
            colorCorrection.TintColor = Color3.new(0.6, 0.8, 1)
            colorCorrection.Brightness = 0.4
            colorCorrection.Contrast = 0.5
            colorCorrection.Saturation = -0.4
            colorCorrection.Parent = camera
            
            -- Efecto de rayos solares intensos
            local sunRays = Instance.new("SunRaysEffect")
            sunRays.Name = "SpeedRays"
            sunRays.Intensity = 0.3
            sunRays.Spread = 1.5
            sunRays.Parent = camera
            
            -- Efecto de bloom para brillo
            local bloom = Instance.new("BloomEffect")
            bloom.Name = "SpeedBloom"
            bloom.Intensity = 0.8
            bloom.Size = 24
            bloom.Threshold = 0.9
            bloom.Parent = camera
        end
    end
    
    function RemoveSpeedEffects()
        local camera = game:GetService("Workspace").CurrentCamera
        
        local effectsToRemove = {
            "SpeedBlur", "SpeedColor", "UltraFastBlur", 
            "UltraFastColor", "SpeedRays", "SpeedBloom"
        }
        
        for _, effectName in ipairs(effectsToRemove) do
            local effect = camera:FindFirstChild(effectName)
            if effect then
                effect:Destroy()
            end
        end
    end
    
    function SyncAnimStop(animId)
        if activeAnimations[animId] then
            activeAnimations[animId]:Stop()
            activeAnimations[animId] = nil
        end
        -- No remover pantalla blanca aqu√≠ si estamos en modo lento
        if currentSpeedMode ~= "veryslow" and currentSpeedMode ~= "extremeslow" then
            RemoveSpeedEffects()
            RemoveScreenEffect()
        end
    end
    
    -- SISTEMA DE ELIMINACI√ìN DE PARTES AL VAC√çO
    function SendPartsToVoid(character, partsToRemove, instant)
        if not character then return end
        
        local parts = {
            "LeftUpperArm", "RightUpperArm",
            "LeftLowerArm", "RightLowerArm", 
            "LeftUpperLeg", "RightUpperLeg",
            "LeftLowerLeg", "RightLowerLeg",
            "LowerTorso"
        }
        
        -- Si se especifican partes espec√≠ficas, usar esas
        if partsToRemove and #partsToRemove > 0 then
            parts = partsToRemove
        end
        
        for _, partName in ipairs(parts) do
            local part = character:FindFirstChild(partName)
            if part then
                -- Desconectar motores primero
                for _, motor in ipairs(part:GetChildren()) do
                    if motor:IsA("Motor6D") then
                        motor:Destroy()
                    end
                end
                
                -- Hacer la parte f√≠sica
                part.CanCollide = false
                part.Anchored = false
                part.Massless = false
                
                if instant then
                    -- Destrucci√≥n inmediata
                    part:Destroy()
                else
                    -- Enviar al vac√≠o con efecto
                    part.CFrame = CFrame.new(0, -10000, 0)
                    part.Transparency = 1
                    part.Anchored = true
                    
                    -- Destruir despu√©s de un tiempo
                    game.Debris:AddItem(part, 0.5)
                end
            end
        end
    end
    
    -- FUNCI√ìN PARA ELIMINACI√ìN COMPLETA DE LA V√çCTIMA
    function CompleteVictimElimination(victimChar, sendToVoid)
        if not victimChar then return end
        
        -- 1. Enviar todas las partes excepto cabeza al vac√≠o
        SendPartsToVoid(victimChar, {
            "LeftUpperArm", "RightUpperArm",
            "LeftLowerArm", "RightLowerArm", 
            "LeftUpperLeg", "RightUpperLeg",
            "LeftLowerLeg", "RightLowerLeg",
            "LowerTorso", "UpperTorso", "HumanoidRootPart"
        }, false)
        
        -- 2. Solo dejar la cabeza visible
        local head = victimChar:FindFirstChild("Head")
        if head then
            head.Transparency = 0
            head.CanCollide = true
            head.Anchored = false
            
            -- Aplicar textura de sangre
            head.BrickColor = BrickColor.new("Really red")
            
            -- Si es env√≠o al vac√≠o completo
            if sendToVoid then
                wait(2)  -- Esperar antes de enviar al vac√≠o
                head.CFrame = CFrame.new(0, -50000, 0)
                head.Anchored = true
                game.Debris:AddItem(head, 1)
            end
        end
        
        -- 3. Si hay Humanoid, matarlo
        local humanoid = victimChar:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.Health = 0
        end
        
        -- 4. Eliminar el personaje completo despu√©s de tiempo
        if sendToVoid then
            game.Debris:AddItem(victimChar, 3)
        end
    end

    game.Workspace.FallenPartsDestroyHeight = -100000

    -- [Funciones de alineaci√≥n y grab...]

    -- SISTEMA DE HERRAMIENTAS COMPLETAMENTE REESCRITO
    function ToolSSs()
        -- ESPERAR A QUE EL JUGADOR TENGA UN PERSONAJE V√ÅLIDO
        if not _LocalPlayer2 then
            _LocalPlayer2 = game:GetService('Players').LocalPlayer
        end
        
        if not _LocalPlayer2.Character then
            _LocalPlayer2.CharacterAdded:Wait()
        end
        
        repeat
            wait()
        until _LocalPlayer2.Character and _LocalPlayer2.Character:FindFirstChild('Humanoid') and _LocalPlayer2.Character:FindFirstChild('BodyEffects')

        -- TOOL: ACTIVATE (sin cambios)
        if _LocalPlayer2.Backpack:FindFirstChild('Activate') then
            _LocalPlayer2.Backpack:FindFirstChild('Activate'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('Activate') then
            _LocalPlayer2.Character:FindFirstChild('Activate'):Destroy()
        end

        task.wait(0.1)

        local _Tool = Instance.new('Tool')
        _Tool.RequiresHandle = false
        _Tool.Name = 'Activate'
        _Tool.Parent = _LocalPlayer2.Backpack

        _Tool.Activated:Connect(function()
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character:FindFirstChild('BodyEffects') then
                return
            end
            
            game:GetService('ReplicatedStorage').MainEvent:FireServer('Grabbing', true)
            wait(0.3)
            Grab()
        end)

        -- TOOL 1: GRAB KNIFE (ENV√çA AL VAC√çO COMPLETO)
        if _LocalPlayer2.Backpack:FindFirstChild('grab knife') then
            _LocalPlayer2.Backpack:FindFirstChild('grab knife'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('grab knife') then
            _LocalPlayer2.Character:FindFirstChild('grab knife'):Destroy()
        end
        
        wait(0.1)
        
        local toolGrabKnife = Instance.new('Tool')
        toolGrabKnife.RequiresHandle = false
        toolGrabKnife.Name = 'grab knife'
        toolGrabKnife.ToolTip = 'Grab Knife - Eliminaci√≥n completa con vac√≠o'
        toolGrabKnife.Parent = _LocalPlayer2.Backpack
        
        local isKnifeActive = false
        
        toolGrabKnife.Activated:Connect(function()
            if isKnifeActive then return end
            
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Grab Knife',
                    Text = 'Character not loaded',
                    Duration = 2
                })
                return
            end
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Grab Knife',
                    Text = 'No victim grabbed',
                    Duration = 2
                })
                return
            end
            
            isKnifeActive = true
            
            local knifeVictim = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
            local victimPlayer = game.Players:GetPlayerFromCharacter(knifeVictim)
            
            if not victimPlayer or victimPlayer == _LocalPlayer2 then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Grab Knife',
                    Text = 'Cannot use on yourself',
                    Duration = 2
                })
                isKnifeActive = false
                return
            end
            
            print('[Grab Knife] Eliminaci√≥n completa de:', victimPlayer.Name)
            
            -- OBTENER CUCHILLO
            local knife = _LocalPlayer2.Backpack:FindFirstChild('Knife') or 
                         _LocalPlayer2.Backpack:FindFirstChild('knife')
            if knife then
                knife.Parent = _LocalPlayer2.Character
            end
            
            -- FUNCI√ìN DE RISA
            local function PlayLaugh()
                if _LocalPlayer2.Character:FindFirstChild('BOOMBOX') then
                    _ReplicatedStorage.MainEvent:FireServer('Boombox', '4810729995')
                else
                    local laughSound = Instance.new("Sound")
                    laughSound.SoundId = "rbxassetid://4810729995"
                    laughSound.Volume = 1.0
                    laughSound.Parent = workspace
                    laughSound:Play()
                    game.Debris:AddItem(laughSound, 10)
                end
            end
            
            PlayLaugh()
            
            -- CONGELAR ANIMACI√ìN CON VELOCIDAD NORMAL (3x)
            local freezeTrack = SyncAnimPlay("74694286613254", "veryfast", 10, Enum.AnimationPriority.Action4)
            
            -- CONTROL DE LA V√çCTIMA
            local stopAlign = false
            local alignConnection
            
            alignConnection = RunService.Heartbeat:Connect(function()
                if stopAlign or not knifeVictim or not knifeVictim.Parent then
                    if alignConnection then alignConnection:Disconnect() end
                    return
                end
                
                local upperTorso = knifeVictim:FindFirstChild('UpperTorso')
                if not upperTorso then return end
                
                -- Crear controles si no existen
                local bodyPos = upperTorso:FindFirstChild('BodyPosition')
                if not bodyPos then
                    bodyPos = Instance.new('BodyPosition')
                    bodyPos.MaxForce = Vector3.new(900000, 900000, 900000)
                    bodyPos.P = 20000
                    bodyPos.D = 2000
                    bodyPos.Parent = upperTorso
                end
                
                local bodyGyro = upperTorso:FindFirstChild('BodyGyro')
                if not bodyGyro then
                    bodyGyro = Instance.new('BodyGyro')
                    bodyGyro.MaxTorque = Vector3.new(900000, 900000, 900000)
                    bodyGyro.P = 30000
                    bodyGyro.Parent = upperTorso
                end
                
                -- Posicionar frente al jugador
                local playerCF = _LocalPlayer2.Character.HumanoidRootPart.CFrame
                local frontPos = playerCF.Position + (playerCF.LookVector * 1.5) + Vector3.new(0, -0.5, 0)
                
                bodyPos.Position = frontPos
                bodyGyro.CFrame = CFrame.new(frontPos) * CFrame.Angles(math.rad(-90), math.pi, 0)
            end)
            
            -- ANIMACI√ìN DE CORTAR CON VELOCIDAD 3x
            local cutAnimId = "126796684932532"
            
            -- CORTAR MIEMBROS
            local limbs = {'RightUpperArm', 'LeftUpperArm', 'LeftUpperLeg', 'RightUpperLeg'}
            
            for i = 1, 4 do
                -- SONIDO DE CORTE
                local soundId = i == 4 and '91676414066995' or '125856818464345'
                
                if _LocalPlayer2.Character:FindFirstChild('BOOMBOX') then
                    _ReplicatedStorage.MainEvent:FireServer('Boombox', soundId)
                else
                    local cutSound = Instance.new("Sound")
                    cutSound.SoundId = "rbxassetid://" .. soundId
                    cutSound.Volume = 1.0
                    cutSound.Pitch = 1.5
                    cutSound.Parent = workspace
                    cutSound:Play()
                    game.Debris:AddItem(cutSound, 1.5)
                end
                
                -- ANIMACI√ìN DE CORTE 3x
                local cutTrack = SyncAnimPlay(cutAnimId, "veryfast", 1.2, Enum.AnimationPriority.Action)
                
                -- ELIMINAR MIEMBRO Y ENVIAR AL VAC√çO
                if knifeVictim and knifeVictim.Parent then
                    local part = knifeVictim:FindFirstChild(limbs[i])
                    if part then 
                        wait(0.2)
                        -- Enviar esta parte al vac√≠o
                        part.CFrame = CFrame.new(0, -10000, 0)
                        part.Transparency = 1
                        part.CanCollide = false
                        part.Anchored = true
                        game.Debris:AddItem(part, 0.5)
                    end
                end
                
                -- RISAS ALEATORIAS
                if math.random(1, 2) == 1 then
                    task.spawn(function()
                        PlayLaugh()
                    end)
                end
                
                wait(1.2)
                SyncAnimStop(cutAnimId)
            end
            
            -- ESPERAR Y PREPARAR PARA TIRAR CABEZA
            wait(1)
            
            -- ENVIAR TORSO Y PARTES RESTANTES AL VAC√çO
            SendPartsToVoid(knifeVictim, {"LowerTorso"}, false)
            
            -- DEJAR SOLO LA CABEZA VISIBLE
            if knifeVictim and knifeVictim.Parent and knifeVictim:FindFirstChild('Head') then
                local head = knifeVictim.Head
                
                -- Configurar cabeza visible
                head.Transparency = 0
                head.CanCollide = true
                head.Anchored = false
                head.BrickColor = BrickColor.new("Really red")
                
                -- Esconder UpperTorso bajo el suelo
                local upperTorso = knifeVictim:FindFirstChild('UpperTorso')
                if upperTorso then
                    upperTorso.CFrame = CFrame.new(0, -10, 0)  -- Justo bajo el suelo
                    upperTorso.Transparency = 1
                    upperTorso.CanCollide = false
                    upperTorso.Anchored = true
                end
                
                -- ANIMACI√ìN DE TIRAR CABEZA CON VELOCIDAD 100x
                local throwTrack = SyncAnimPlay("135931094841392", "ultrafast", 8, Enum.AnimationPriority.Action)
                
                -- TIRAR LA CABEZA
                local throwForce = (_LocalPlayer2.Character.HumanoidRootPart.CFrame.LookVector * 250) + Vector3.new(0, 60, 0)
                head.Velocity = throwForce
                
                PlayLaugh()
                
                -- NOTIFICACI√ìN
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = '‚ö° CABEZA LANZADA!',
                    Text = 'Velocidad 100x activada',
                    Duration = 4
                })
                
                -- ESPERAR A QUE LA CABEZA CAIGA
                wait(4)
                
                -- CAMBIAR A VELOCIDAD MUY LENTA (0.1x) CON PANTALLA BLANCA
                SyncAnimStop("135931094841392")
                local slowTrack = SyncAnimPlay("135931094841392", "veryslow", nil, Enum.AnimationPriority.Action)
                
                -- DETECTAR CUANDO EL JUGADOR SE ACERCA
                local startTime = tick()
                local playerNearHead = false
                
                while tick() - startTime < 20 do
                    if not head or not head.Parent then break end
                    
                    local distance = (_LocalPlayer2.Character.HumanoidRootPart.Position - head.Position).magnitude
                    
                    if distance <= 8 then
                        playerNearHead = true
                        break
                    end
                    task.wait(0.1)
                end
                
                if playerNearHead and head and head.Parent then
                    -- ANIMACI√ìN DE PISAR
                    local stompTrack = SyncAnimPlay("72406748336709", "normal", 2, Enum.AnimationPriority.Action)
                    
                    -- POSICIONAR SOBRE LA CABEZA
                    local headPos = head.Position
                    _LocalPlayer2.Character.HumanoidRootPart.CFrame = CFrame.new(
                        headPos.X, 
                        headPos.Y + 3.5, 
                        headPos.Z
                    )
                    
                    wait(0.5)
                    
                    -- APLASTAR LA CABEZA
                    local originalWalkSpeed = _LocalPlayer2.Character.Humanoid.WalkSpeed
                    _LocalPlayer2.Character.Humanoid.WalkSpeed = 0
                    
                    head.Velocity = Vector3.new(0, -150, 0)
                    
                    wait(1)
                    
                    -- ELIMINACI√ìN COMPLETA: ENVIAR V√çCTIMA AL VAC√çO
                    CompleteVictimElimination(knifeVictim, true)
                    
                    wait(1)
                    
                    -- RESTAURAR VELOCIDAD
                    _LocalPlayer2.Character.Humanoid.WalkSpeed = originalWalkSpeed
                    
                    stompTrack:Stop()
                    
                    game:GetService('StarterGui'):SetCore('SendNotification', {
                        Title = 'üíÄ ELIMINACI√ìN COMPLETA',
                        Text = victimPlayer.Name .. ' enviado al vac√≠o',
                        Duration = 5
                    })
                else
                    -- Si no se acerca, igual enviar al vac√≠o
                    CompleteVictimElimination(knifeVictim, true)
                    
                    game:GetService('StarterGui'):SetCore('SendNotification', {
                        Title = 'Grab Knife',
                        Text = 'V√≠ctima eliminada autom√°ticamente',
                        Duration = 3
                    })
                end
                
                -- LIMPIAR EFECTOS
                SyncAnimStop("135931094841392")
                RemoveScreenEffect()
            end
            
            -- LIMPIAR
            stopAlign = true
            if alignConnection then alignConnection:Disconnect() end
            
            -- SOLTAR V√çCTIMA
            game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Grabbing", false)
            
            -- DEVOLVER CUCHILLO
            if knife and knife.Parent == _LocalPlayer2.Character then
                knife.Parent = _LocalPlayer2.Backpack
            end
            
            -- DETENER ANIMACI√ìN
            if freezeTrack then
                freezeTrack:Stop()
            end
            
            RemoveSpeedEffects()
            RemoveScreenEffect()
            
            isKnifeActive = false
        end)
        
        -- TOOL 2: DISMEMBER (SOLO ELIMINA PARTES, NO ENV√çA AL VAC√çO)
        if _LocalPlayer2.Backpack:FindFirstChild('dismember') then
            _LocalPlayer2.Backpack:FindFirstChild('dismember'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('dismember') then
            _LocalPlayer2.Character:FindFirstChild('dismember'):Destroy()
        end
        
        wait(0.1)
        
        local toolDismember = Instance.new('Tool')
        toolDismember.RequiresHandle = false
        toolDismember.Name = 'dismember'
        toolDismember.ToolTip = 'Dismember - Solo elimina partes al vac√≠o'
        toolDismember.Parent = _LocalPlayer2.Backpack
        
        local isDismemberActive = false
        
        toolDismember.Activated:Connect(function()
            if isDismemberActive then return end
            
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then
                return
            end
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Dismember',
                    Text = 'No victim grabbed',
                    Duration = 2
                })
                return
            end
            
            isDismemberActive = true
            
            local victim = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
            local victimPlayer = game.Players:GetPlayerFromCharacter(victim)
            
            if not victimPlayer or victimPlayer == _LocalPlayer2 then
                isDismemberActive = false
                return
            end
            
            print('[Dismember] Eliminando partes de:', victimPlayer.Name)
            
            -- ANIMACI√ìN DE INICIO CON VELOCIDAD 3x
            local startTrack = SyncAnimPlay("126796684932532", "veryfast", 2, Enum.AnimationPriority.Action)
            
            -- LISTA DE PARTES A ELIMINAR (SOLO ESTAS)
            local partsToRemove = {
                "LeftUpperArm", "RightUpperArm",
                "LeftLowerArm", "RightLowerArm", 
                "LeftUpperLeg", "RightUpperLeg",
                "LeftLowerLeg", "RightLowerLeg",
                "LowerTorso"
            }
            
            -- ELIMINAR CADA PARTE CON EFECTO
            for i, partName in ipairs(partsToRemove) do
                local part = victim:FindFirstChild(partName)
                if part then
                    -- SONIDO DE DESMEMBRAMIENTO
                    local sound = Instance.new("Sound")
                    sound.SoundId = "rbxassetid://125856818464345"
                    sound.Volume = 0.7
                    sound.Parent = workspace
                    sound:Play()
                    game.Debris:AddItem(sound, 2)
                    
                    -- ANIMACI√ìN R√ÅPIDA
                    local animTrack = SyncAnimPlay("126796684932532", "veryfast", 0.5, Enum.AnimationPriority.Action)
                    
                    -- ENVIAR PARTE AL VAC√çO
                    task.wait(0.1)
                    part.CFrame = CFrame.new(0, -10000, 0)
                    part.Transparency = 1
                    part.CanCollide = false
                    part.Anchored = true
                    game.Debris:AddItem(part, 0.5)
                    
                    task.wait(0.4)
                    SyncAnimStop("126796684932532")
                end
            end
            
            -- DEJAR SOLO CABEZA Y UPPERTORSO VISIBLES
            local head = victim:FindFirstChild("Head")
            local upperTorso = victim:FindFirstChild("UpperTorso")
            
            if head then
                head.Transparency = 0
                head.BrickColor = BrickColor.new("Really red")
            end
            
            if upperTorso then
                -- Poner bajo el suelo pero no en el vac√≠o
                upperTorso.CFrame = CFrame.new(0, -5, 0)
                upperTorso.Transparency = 0.8  -- Semi-transparente
                upperTorso.CanCollide = false
                upperTorso.Anchored = true
            end
            
            -- NOTIFICACI√ìN
            game:GetService('StarterGui'):SetCore('SendNotification', {
                Title = 'ü©∏ DISMEMBER COMPLETADO',
                Text = 'Partes eliminadas al vac√≠o',
                Duration = 4
            })
            
            -- SOLTAR V√çCTIMA
            game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Grabbing", false)
            
            SyncAnimStop("126796684932532")
            isDismemberActive = false
        end)
        
        -- TOOL: IDK - CORREGIDO (SIN EFECTOS AUTOM√ÅTICOS)
        if _LocalPlayer2.Backpack:FindFirstChild('Idk') then
            _LocalPlayer2.Backpack:FindFirstChild('Idk'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('Idk') then
            _LocalPlayer2.Character:FindFirstChild('Idk'):Destroy()
        end

        wait(0.1)

        local toolIdk = Instance.new('Tool')
        toolIdk.RequiresHandle = false
        toolIdk.Name = 'Idk'
        toolIdk.ToolTip = 'Idk - Control simple sin efectos'
        toolIdk.Parent = _LocalPlayer2.Backpack

        local isIdkActive = false

        toolIdk.Activated:Connect(function()
            if isIdkActive then return end
            
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then
                return
            end
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Idk',
                    Text = 'No victim grabbed',
                    Duration = 2
                })
                return
            end
            
            isIdkActive = true
            
            -- ANIMACI√ìN SIMPLE SIN EFECTOS ESPECIALES
            local mainTrack = SyncAnimPlay("91859430011682", "veryfast", nil, Enum.AnimationPriority.Action)
            
            -- VARIABLE DE CONTROL
            getgenv().IdkActive = true
            
            -- CONEXI√ìN SIMPLE
            local checkConnection
            checkConnection = RunService.Heartbeat:Connect(function()
                if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects or 
                   not _LocalPlayer2.Character.BodyEffects.Grabbed.Value or getgenv().IdkActive == false then
                    
                    if mainTrack then
                        mainTrack:Stop()
                    end
                    
                    -- DESTRUIR CONTROLES SIN EFECTOS
                    if u19 and u19:FindFirstChild('UpperTorso') then
                        Destroy('UpperTorso')
                    end
                    
                    if checkConnection then
                        checkConnection:Disconnect()
                    end
                    
                    isIdkActive = false
                    getgenv().IdkActive = nil
                end
            end)
            
            -- LOOP SIMPLE DE ALINEACI√ìN
            task.spawn(function()
                while isIdkActive and _LocalPlayer2.Character and _LocalPlayer2.Character.BodyEffects and 
                      _LocalPlayer2.Character.BodyEffects.Grabbed.Value and getgenv().IdkActive == true do
                    task.wait()
                    
                    if u19 then
                        -- Limpiar velocidades
                        for _, part in ipairs(u19:GetDescendants()) do
                            if part:IsA('BasePart') then
                                part.Velocity = Vector3.zero
                                part.AssemblyAngularVelocity = Vector3.zero
                                part.AssemblyLinearVelocity = Vector3.zero
                            end
                        end

                        local timeOffset = tick() * 100
                        Align(20000, 500, 'UpperTorso', 'HumanoidRootPart', 
                              CFrame.new(0, -0.3, -1.2 + 0.1 * math.sin(timeOffset / 4)),
                              CFrame.Angles(math.pi * -0.3, 0, 0))
                    end
                end
            end)
        end)
        
        -- CONEXI√ìN PARA DETENER IDk AL SOLTAR
        local releaseConnection
        releaseConnection = _LocalPlayer2.Character.BodyEffects.Grabbed:GetPropertyChangedSignal("Value"):Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil then
                getgenv().IdkActive = false
            end
        end)
        
        -- GUARDAR CONEXI√ìN PARA LIMPIAR
        table.insert(u22, releaseConnection)
    end
        
    ToolSSs()
    game.Players.LocalPlayer.CharacterAdded:Connect(function()
        wait(2)
        ToolSSs()
    end)
else
    game:GetService('StarterGui'):SetCore('SendNotification', {
        Title = 'Your Not Buyer',
        Text = 'Please Buy Our Script Our Discord Server (Discord Server Set To Your CilpBoard)',
        Icon = 'rbxthumb://type=Asset&id=7257741890&w=150&h=150',
        Duration = 10,
    })
    setclipboard('https://discord.gg/mFnYMm7e')
end
