    function Align(p137, p138, p139, p140, p141, p142)
        if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects or not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
            return
        end
        if not u19 then return end
        
        if not (u19[p139]:FindFirstChildWhichIsA('BodyPosition') or u19[p139]:FindFirstChildWhichIsA('BodyGyro')) then
            Instance.new('BodyPosition', u19[p139]).MaxForce = Vector3.new(math.huge, math.huge, math.huge)

            local _BodyGyro = Instance.new('BodyGyro', u19[p139])

            _BodyGyro.maxTorque = Vector3.new(9000000000, 9000000000, 9000000000)
            _BodyGyro.P = 10000
        end

        if _LocalPlayer2.Character[p140] then
            u19[p139].BodyGyro.CFrame = _LocalPlayer2.Character[p140].CFrame * p142
            u19[p139].BodyPosition.Position = _LocalPlayer2.Character[p140].CFrame * p141.Position
            u19[p139].BodyPosition.P = p137
            u19[p139].BodyPosition.D = p138
        end
    end
    function Destroy(p144)
        if not u19 then return end
        if u19[p144]:FindFirstChildOfClass('BodyGyro') or u19[p144]:FindFirstChildOfClass('BodyPosition') then
            if u19[p144]:FindFirstChildOfClass('BodyGyro') then u19[p144]:FindFirstChildOfClass('BodyGyro'):Destroy() end
            if u19[p144]:FindFirstChildOfClass('BodyPosition') then u19[p144]:FindFirstChildOfClass('BodyPosition'):Destroy() end
        end
    end
    function Holding()
        repeat
            task.wait()
            Align(10000, 175, 'UpperTorso', 'RightUpperArm', CFrame.new(-0.2, -1.7, 0.5), CFrame.Angles(math.pi * -0.5, math.pi * 1, math.pi * 0.07))
        until not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects.Grabbed.Value or u20 == true
    end
    function Grab()
        if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then return end
        if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
            u19 = _LocalPlayer2.Character.BodyEffects.Grabbed.Value

            local _GRABBING_CONSTRAINT = u19:WaitForChild('GRABBING_CONSTRAINT')

            if not _GRABBING_CONSTRAINT then
                return
            end

            u23 = _GRABBING_CONSTRAINT:FindFirstChildOfClass('RopeConstraint')
            if u23 then
                u23.Length = 1 / 0
            end
            u20 = false

            local v146 = u19
            local v147, v148, v149 = pairs(v146:GetChildren())

            while true do
                local v150

                v149, v150 = v147(v148, v149)

                if v149 == nil then
                    break
                end
                if v150:IsA('BasePart') then
                    v150.CanCollide = false
                end
            end
            while true do
                task.wait()

                if not _LocalPlayer2.Character or not _LocalPlayer2.Character:FindFirstChild('Humanoid') then break end
                local v151, v152, v153 = pairs(_LocalPlayer2.Character.Humanoid:GetPlayingAnimationTracks())

                while true do
                    local v154

                    v153, v154 = v151(v152, v153)

                    if v153 == nil then
                        break
                    end
                    if v154.Animation.AnimationId:match('rbxassetid://11075367458') then
                        u20 = true
                        v154:Stop()
                    end
                end

                if u20 == true then
                    local v155 = u19
                    local v156, v157, v158 = pairs(v155:GetDescendants())

                    while true do
                        local v159

                        v158, v159 = v156(v157, v158)

                        if v158 == nil then
                            break
                        end
                        if v159:IsA('BasePart') then
                            v159.Velocity = Vector3.zero
                            v159.AssemblyAngularVelocity = Vector3.zero
                            v159.AssemblyLinearVelocity = Vector3.zero
                        end
                    end

                    u20 = false

                    AnimPlayvv2(3135389157, 0, 0.1, 0.3)
                    Holding()
                    AnimStopv2(3135389157)
                    Destroy('UpperTorso')
                end
            end
        else
            return
        end
    end
    
    -- FUNCI√ìN MEJORADA PARA DESACOPLAR CABEZA
    local function DetachHeadAndThrow(victimChar)
        if not victimChar or not victimChar:FindFirstChild("Head") then return nil end
        
        local head = victimChar.Head
        
        -- 1. Destruir TODOS los motores conectados a la cabeza
        for _, part in ipairs(victimChar:GetChildren()) do
            if part:IsA("BasePart") then
                for _, motor in ipairs(part:GetChildren()) do
                    if motor:IsA("Motor6D") and (motor.Part0 == head or motor.Part1 == head) then
                        motor:Destroy()
                    end
                end
            end
        end
        
        -- 2. Destruir motores dentro de la cabeza
        for _, motor in ipairs(head:GetChildren()) do
            if motor:IsA("Motor6D") then
                motor:Destroy()
            end
        end
        
        -- 3. Destruir GRABBING_CONSTRAINT si existe
        local grabbingConstraint = victimChar:FindFirstChild("GRABBING_CONSTRAINT")
        if grabbingConstraint then
            grabbingConstraint:Destroy()
        end
        
        -- 4. Destruir RopeConstraint si existe
        for _, constraint in ipairs(victimChar:GetDescendants()) do
            if constraint:IsA("RopeConstraint") then
                constraint:Destroy()
            end
        end
        
        -- 5. Configurar cabeza como f√≠sica independiente
        head.CanCollide = true
        head.Anchored = false
        head.Massless = false
        head.Transparency = 0
        
        -- 6. Aplicar efecto de sangre en la cabeza
        head.BrickColor = BrickColor.new("Really red")
        
        -- 7. Esconder el resto del cuerpo BAJO EL MAPA
        for _, part in ipairs(victimChar:GetChildren()) do
            if part:IsA("BasePart") and part ~= head then
                part.CFrame = CFrame.new(0, -10000, 0)
                part.Transparency = 1
                part.CanCollide = false
                part.Anchored = true
            end
        end
        
        return head
    end
    
    -- SISTEMA DE HERRAMIENTAS MEJORADO
    function ToolSSs()
        -- ESPERAR A QUE EL JUGADOR TENGA UN PERSONAJE V√ÅLIDO
        if not _LocalPlayer2 then
            _LocalPlayer2 = game:GetService('Players').LocalPlayer
        end
        
        if not _LocalPlayer2.Character then
            _LocalPlayer2.CharacterAdded:Wait()
        end
        
        repeat
            wait()
        until _LocalPlayer2.Character and _LocalPlayer2.Character:FindFirstChild('Humanoid') and _LocalPlayer2.Character:FindFirstChild('BodyEffects')

        -- TOOL: ACTIVATE
        if _LocalPlayer2.Backpack:FindFirstChild('Activate') then
            _LocalPlayer2.Backpack:FindFirstChild('Activate'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('Activate') then
            _LocalPlayer2.Character:FindFirstChild('Activate'):Destroy()
        end

        task.wait(0.1)

        local _Tool = Instance.new('Tool')
        _Tool.RequiresHandle = false
        _Tool.Name = 'Activate'
        _Tool.Parent = _LocalPlayer2.Backpack

        _Tool.Activated:Connect(function()
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character:FindFirstChild('BodyEffects') then
                return
            end
            
            game:GetService('ReplicatedStorage').MainEvent:FireServer('Grabbing', true)
            wait(0.3)
            Grab()
        end)

        -- TOOL: GRAB KNIFE - VERSI√ìN MEJORADA CON VELOCIDADES EXTREMAS
        if _LocalPlayer2.Backpack:FindFirstChild('grab knife') then
            _LocalPlayer2.Backpack:FindFirstChild('grab knife'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('grab knife') then
            _LocalPlayer2.Character:FindFirstChild('grab knife'):Destroy()
        end
        
        wait(0.1)
        
        local _ToolGrabKnife = Instance.new('Tool')
        _ToolGrabKnife.RequiresHandle = false
        _ToolGrabKnife.Name = 'grab knife'
        _ToolGrabKnife.ToolTip = 'Grab Knife - Bloody execution with laugh'
        _ToolGrabKnife.Parent = _LocalPlayer2.Backpack
        
        local isKnifeActive = false
        
        _ToolGrabKnife.Activated:Connect(function()
            if isKnifeActive then return end
            
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Grab Knife',
                    Text = 'Character not loaded',
                    Duration = 2
                })
                return
            end
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Grab Knife',
                    Text = 'No victim grabbed',
                    Duration = 2
                })
                return
            end
            
            isKnifeActive = true
            
            local knifeVictim = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
            local victimPlayer = game.Players:GetPlayerFromCharacter(knifeVictim)
            
            if not victimPlayer or victimPlayer == _LocalPlayer2 then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Grab Knife',
                    Text = 'Cannot use on yourself',
                    Duration = 2
                })
                isKnifeActive = false
                return
            end
            
            print('[Grab Knife] Starting execution on:', victimPlayer.Name)
            
            -- OBTENER CUCHILLO
            local knife = _LocalPlayer2.Backpack:FindFirstChild('Knife') or 
                         _LocalPlayer2.Backpack:FindFirstChild('knife')
            if knife then
                knife.Parent = _LocalPlayer2.Character
            end
            
            -- FUNCI√ìN DE RISA
            local function PlayLaugh()
                if _LocalPlayer2.Character:FindFirstChild('BOOMBOX') then
                    _ReplicatedStorage.MainEvent:FireServer('Boombox', '4810729995')
                else
                    local laughSound = Instance.new("Sound")
                    laughSound.SoundId = "rbxassetid://4810729995"
                    laughSound.Volume = 1.0
                    laughSound.Parent = workspace
                    laughSound:Play()
                    game.Debris:AddItem(laughSound, 10)
                end
            end
            
            PlayLaugh()
            
            -- CONGELAR ANIMACI√ìN CON EFECTO DE PANTALLA BLANCA (velocidad muy lenta)
            local freezeTrack = SyncAnimPlay("74694286613254", "extremeslow", 10, Enum.AnimationPriority.Action4)
            
            -- CONTROL DE LA V√çCTIMA MEJORADO
            local stopAlign = false
            local alignConnection
            
            alignConnection = RunService.Heartbeat:Connect(function()
                if stopAlign or not knifeVictim or not knifeVictim.Parent then
                    if alignConnection then alignConnection:Disconnect() end
                    return
                end
                
                local upperTorso = knifeVictim:FindFirstChild('UpperTorso')
                if not upperTorso then return end
                
                -- Crear controles si no existen
                local bodyPos = upperTorso:FindFirstChild('BodyPosition')
                if not bodyPos then
                    bodyPos = Instance.new('BodyPosition')
                    bodyPos.MaxForce = Vector3.new(900000, 900000, 900000)
                    bodyPos.P = 20000
                    bodyPos.D = 2000
                    bodyPos.Parent = upperTorso
                end
                
                local bodyGyro = upperTorso:FindFirstChild('BodyGyro')
                if not bodyGyro then
                    bodyGyro = Instance.new('BodyGyro')
                    bodyGyro.MaxTorque = Vector3.new(900000, 900000, 900000)
                    bodyGyro.P = 30000
                    bodyGyro.Parent = upperTorso
                end
                
                -- Posicionar frente al jugador
                local playerCF = _LocalPlayer2.Character.HumanoidRootPart.CFrame
                local frontPos = playerCF.Position + (playerCF.LookVector * 1.5) + Vector3.new(0, -0.5, 0)
                
                bodyPos.Position = frontPos
                bodyGyro.CFrame = CFrame.new(frontPos) * CFrame.Angles(math.rad(-90), math.pi, 0)
            end)
            
            -- ANIMACI√ìN DE CORTAR CON VELOCIDAD ULTRA R√ÅPIDA (x10)
            local cutAnimId = "126796684932532"
            
            -- LISTA DE MIEMBROS A CORTAR
            local limbsToSever = {
                'RightUpperArm',
                'LeftUpperArm', 
                'LeftUpperLeg',
                'RightUpperLeg'
            }
            
            -- SISTEMA DE EFECTOS DE SANGRE
            local bloodTrail = CreateBloodTrail(knifeVictim)
            
            -- CORTAR MIEMBROS CON VELOCIDAD ULTRA R√ÅPIDA
            for i = 1, 4 do
                -- SONIDO DE CORTE ACELERADO
                local soundId = i == 4 and '91676414066995' or '125856818464345'
                
                if _LocalPlayer2.Character:FindFirstChild('BOOMBOX') then
                    _ReplicatedStorage.MainEvent:FireServer('Boombox', soundId)
                else
                    local cutSound = Instance.new("Sound")
                    cutSound.SoundId = "rbxassetid://" .. soundId
                    cutSound.Volume = 1.0
                    cutSound.Pitch = 2.5  -- Sonido m√°s r√°pido
                    cutSound.Parent = workspace
                    cutSound:Play()
                    game.Debris:AddItem(cutSound, 1.5)
                end
                
                -- ANIMACI√ìN DE CORTE CON VELOCIDAD 10X
                local cutTrack = SyncAnimPlay(cutAnimId, "ultrafast", 0.8, Enum.AnimationPriority.Action)
                
                -- EFECTO DE SANGRE AL CORTAR
                if knifeVictim and knifeVictim.Parent then
                    local part = knifeVictim:FindFirstChild(limbsToSever[i])
                    if part then 
                        -- Crear explosi√≥n de sangre
                        BloodSpray(part.Position, 25)
                        
                        wait(0.05)  -- Espera muy corta por la velocidad
                        part:Destroy()
                    end
                end
                
                -- RISAS ALEATORIAS ACELERADAS
                if math.random(1, 2) == 1 then
                    task.spawn(function()
                        PlayLaugh()
                    end)
                end
                
                wait(0.8)  -- Tiempo reducido para velocidad r√°pida
                SyncAnimStop(cutAnimId)
            end
            
            -- DESTRUIR TRAIL DE SANGRE
            if bloodTrail then
                bloodTrail:Destroy()
            end
            
            -- ESPERAR Y PREPARAR PARA TIRAR CABEZA
            wait(0.5)
            
            -- **DESACOPLAR Y TIRAR LA CABEZA REAL CON EFECTOS DE VELOCIDAD**
            if knifeVictim and knifeVictim.Parent and knifeVictim:FindFirstChild('Head') then
                -- 1. Destruir constraints ANTES de desacoplar
                for _, constraint in ipairs(knifeVictim:GetDescendants()) do
                    if constraint:IsA("WeldConstraint") or constraint:IsA("RopeConstraint") then
                        constraint:Destroy()
                    end
                end
                
                -- 2. Desacoplar cabeza
                local head = DetachHeadAndThrow(knifeVictim)
                
                if head then
                    -- 3. ANIMACI√ìN DE TIRAR LA CABEZA CON VELOCIDAD 10X (10 SEGUNDOS)
                    local throwTrack = SyncAnimPlay("135931094841392", "ultrafast", 10, Enum.AnimationPriority.Action)
                    
                    -- 4. TIRAR LA CABEZA CON FUERZA EXTREMA
                    local throwForce = (_LocalPlayer2.Character.HumanoidRootPart.CFrame.LookVector * 300) + Vector3.new(0, 75, 0)
                    local spinForce = Vector3.new(
                        math.random(-150, 150),
                        math.random(-150, 150),
                        math.random(-150, 150)
                    )
                    
                    head.Velocity = throwForce
                    head.RotVelocity = spinForce
                    
                    -- 5. EFECTO DE SANGRE MASIVO
                    BloodSpray(head.Position, 50)
                    CreateBloodEffect(head.Position, "bloodPool", 8, 25)
                    
                    -- RISAS EN VELOCIDAD R√ÅPIDA
                    task.spawn(function()
                        for i = 1, 3 do
                            PlayLaugh()
                            wait(2)
                        end
                    end)
                    
                    -- 6. NOTIFICACI√ìN
                    game:GetService('StarterGui'):SetCore('SendNotification', {
                        Title = '‚ö° CABEZA LANZADA A VELOCIDAD 10X!',
                        Text = 'Efectos de velocidad extrema activados',
                        Duration = 5
                    })
                    
                    -- 7. ESPERAR A QUE LA CABEZA CAIGA (tiempo reducido)
                    wait(3)
                    
                    -- 8. CAMBIAR A VELOCIDAD EXTREMADAMENTE LENTA PARA LA CA√çDA
                    SyncAnimStop("135931094841392")
                    local slowTrack = SyncAnimPlay("135931094841392", "extremeslow", nil, Enum.AnimationPriority.Action)
                    
                    -- 9. EFECTO DE PANTALLA BLANCA INTENSA EN VELOCIDAD LENTA
                    CreateWhiteScreenEffect(0.7, nil)
                    
                    -- 10. DETECTAR CUANDO EL JUGADOR SE ACERCA
                    local startTime = tick()
                    local playerNearHead = false
                    
                    while tick() - startTime < 25 do
                        if not head or not head.Parent then break end
                        
                        local distance = (_LocalPlayer2.Character.HumanoidRootPart.Position - head.Position).magnitude
                        
                        if distance <= 10 then
                            playerNearHead = true
                            break
                        end
                        task.wait(0.1)
                    end
                    
                    if playerNearHead and head and head.Parent then
                        -- 11. DETENER ANIMACI√ìN LENTA Y EFECTOS
                        SyncAnimStop("135931094841392")
                        RemoveScreenEffect()
                        
                        -- 12. ANIMACI√ìN DE PISAR CON VELOCIDAD NORMAL
                        local stompTrack = SyncAnimPlay("72406748336709", "normal", 2, Enum.AnimationPriority.Action)
                        
                        -- 13. POSICIONAR SOBRE LA CABEZA
                        local headPos = head.Position
                        _LocalPlayer2.Character.HumanoidRootPart.CFrame = CFrame.new(
                            headPos.X, 
                            headPos.Y + 4, 
                            headPos.Z
                        )
                        
                        wait(0.3)
                        
                        -- 14. APLASTAR LA CABEZA
                        local originalWalkSpeed = _LocalPlayer2.Character.Humanoid.WalkSpeed
                        _LocalPlayer2.Character.Humanoid.WalkSpeed = 0
                        
                        head.Velocity = Vector3.new(0, -300, 0)
                        
                        wait(1)
                        
                        -- 15. EXPLOSI√ìN FINAL DE SANGRE
                        BloodSpray(head.Position, 75)
                        
                        -- 16. LANZAR AL VAC√çO CON FUERZA
                        head.Velocity = Vector3.new(0, 400, 0) + Vector3.new(
                            math.random(-75, 75),
                            0,
                            math.random(-75, 75)
                        )
                        
                        wait(1.5)
                        
                        -- 17. RESTAURAR VELOCIDAD Y REMOVER EFECTOS
                        _LocalPlayer2.Character.Humanoid.WalkSpeed = originalWalkSpeed
                        RemoveSpeedEffects()
                        
                        stompTrack:Stop()
                        
                        game:GetService('StarterGui'):SetCore('SendNotification', {
                            Title = 'üíÄ EXECUCI√ìN COMPLETADA CON √âXITO!',
                            Text = victimPlayer.Name .. ' eliminado con efectos √©picos!',
                            Duration = 5
                        })
                    else
                        game:GetService('StarterGui'):SetCore('SendNotification', {
                            Title = 'Grab Knife',
                            Text = 'Tiempo agotado - Efectos desactivados',
                            Duration = 3
                        })
                        RemoveScreenEffect()
                    end
                    
                    -- DETENER ANIMACI√ìN SI SIGUE ACTIVA
                    SyncAnimStop("135931094841392")
                    RemoveSpeedEffects()
                end
            end
            
            -- LIMPIAR
            stopAlign = true
            if alignConnection then alignConnection:Disconnect() end
            
            -- DESTRUIR CONTROLES
            if knifeVictim and knifeVictim.Parent and knifeVictim:FindFirstChild('UpperTorso') then
                local bodyPos = knifeVictim.UpperTorso:FindFirstChild('BodyPosition')
                local bodyGyro = knifeVictim.UpperTorso:FindFirstChild('BodyGyro')
                if bodyPos then bodyPos:Destroy() end
                if bodyGyro then bodyGyro:Destroy() end
            end
            
            -- SOLTAR V√çCTIMA
            game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Grabbing", false)
            
            -- DEVOLVER CUCHILLO
            if knife and knife.Parent == _LocalPlayer2.Character then
                knife.Parent = _LocalPlayer2.Backpack
            end
            
            -- DETENER ANIMACI√ìN DE CONGELAMIENTO Y REMOVER EFECTOS
            if freezeTrack then
                freezeTrack:Stop()
            end
            RemoveScreenEffect()
            RemoveSpeedEffects()
            
            isKnifeActive = false
            
            -- NOTIFICACI√ìN FINAL
            game:GetService('StarterGui'):SetCore('SendNotification', {
                Title = '‚ú® Grab Knife - Ejecuci√≥n √âpica',
                Text = 'Todos los efectos han finalizado',
                Icon = 'rbxthumb://type=Asset&id=7257741890&w=150&h=150',
                Duration = 3
            })
        end)
        
        -- TOOL: IDK - MEJORADO CON VELOCIDADES EXTREMAS
        if _LocalPlayer2.Backpack:FindFirstChild('Idk') then
            _LocalPlayer2.Backpack:FindFirstChild('Idk'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('Idk') then
            _LocalPlayer2.Character:FindFirstChild('Idk'):Destroy()
        end

        wait(0.1)

        local _Tool14 = Instance.new('Tool')
        _Tool14.RequiresHandle = false
        _Tool14.Name = 'Idk'
        _Tool14.Parent = _LocalPlayer2.Backpack

        local isIdkActive = false

        _Tool14.Activated:Connect(function()
            if isIdkActive then return end
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Idk',
                    Text = 'Character not loaded',
                    Duration = 2
                })
                return
            end
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Idk',
                    Text = 'No victim grabbed',
                    Duration = 2
                })
                return
            end
            
            isIdkActive = true
            
            -- REPRODUCIR ANIMACI√ìN CON VELOCIDAD ULTRA R√ÅPIDA (10x)
            local mainTrack = SyncAnimPlay("91859430011682", "ultrafast", 10, Enum.AnimationPriority.Action)
            
            -- ANIMACIONES ADICIONALES TAMBI√âN A VELOCIDAD 10X
            SyncAnimPlay("4784557631", "ultrafast", 10, Enum.AnimationPriority.Action)
            SyncAnimPlay("3119980985", "ultrafast", 10, Enum.AnimationPriority.Action)

            -- VARIABLE PARA CONTROLAR EL LOOP
            getgenv().Furry_ = false
            
            -- CONEXI√ìN PARA DETECTAR CUANDO YA NO HAY V√çCTIMA
            local checkConnection
            checkConnection = RunService.Heartbeat:Connect(function()
                if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects or not _LocalPlayer2.Character.BodyEffects.Grabbed.Value or getgenv().Furry_ == true then
                    -- DETENER ANIMACIONES
                    SyncAnimStop("91859430011682")
                    SyncAnimStop("4784557631")
                    SyncAnimStop("3119980985")
                    
                    -- REMOVER EFECTOS
                    RemoveSpeedEffects()
                    RemoveScreenEffect()
                    
                    -- DESTRUIR CONTROLES
                    if u19 and u19:FindFirstChild('UpperTorso') then
                        Destroy('UpperTorso')
                    end
                    
                    -- DESCONECTAR
                    if checkConnection then
                        checkConnection:Disconnect()
                    end
                    
                    isIdkActive = false
                end
            end)
            
            -- LOOP DE ALINEACI√ìN MEJORADO PARA VELOCIDAD R√ÅPIDA
            task.spawn(function()
                while isIdkActive and _LocalPlayer2.Character and _LocalPlayer2.Character.BodyEffects and _LocalPlayer2.Character.BodyEffects.Grabbed.Value and not getgenv().Furry_ do
                    task.wait()
                    
                    if u19 then
                        local v417 = u19
                        local v418, v419, v420 = pairs(v417:GetDescendants())

                        while true do
                            local v421
                            v420, v421 = v418(v419, v420)
                            if v420 == nil then
                                break
                            end
                            if v421:IsA('BasePart') or (v421:IsA('MeshPart') or v421:IsA('Part')) then
                                v421.Velocity = Vector3.zero
                                v421.AssemblyAngularVelocity = Vector3.zero
                                v421.AssemblyLinearVelocity = Vector3.zero
                                v421.CanCollide = false
                            end
                        end

                        local v422 = tick() * 400  -- Multiplicador aumentado para velocidad r√°pida
                        Align(50000, 1000, 'UpperTorso', 'HumanoidRootPart', 
                              CFrame.new(0, -0.3, -1.2 + 0.1 * math.sin(v422 / 4)),  -- Movimiento m√°s r√°pido
                              CFrame.Angles(math.pi * -0.3, 0, 0))
                    end
                end
            end)
        end)
    end
        
    ToolSSs()
    game.Players.LocalPlayer.CharacterAdded:Connect(function()
        wait(2)
        ToolSSs()
    end)
else
    game:GetService('StarterGui'):SetCore('SendNotification', {
        Title = 'Your Not Buyer',
        Text = 'Please Buy Our Script Our Discord Server (Discord Server Set To Your CilpBoard)',
        Icon = 'rbxthumb://type=Asset&id=7257741890&w=150&h=150',
        Duration = 10,
    })
    setclipboard('https://discord.gg/mFnYMm7e')
end
