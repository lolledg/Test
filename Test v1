    local _LocalPlayer2 = game:GetService('Players').LocalPlayer
    local u19 = nil
    local u20 = nil
    local u21 = nil
    local u22 = {}
    local u23 = nil

    local function u27(p24, p25, p26)
        p24.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
        p24.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
        p24.Velocity = Vector3.new(0, 0, 0)
        p24.CFrame = p25.CFrame * (p26 or CFrame.new())
        p24.CanCollide = false
        p25.CanCollide = false
    end

    function ZeroVelocity___(p38)
        p38.Velocity = Vector3.zero
        p38.AssemblyAngularVelocity = Vector3.zero
        p38.AssemblyLinearVelocity = Vector3.zero
    end

    function Align(p137, p138, p139, p140, p141, p142)
        if _LocalPlayer2.Character.BodyEffects.Grabbed.Value ~= nil then
            if not (u19[p139]:FindFirstChildWhichIsA('BodyPosition') or u19[p139]:FindFirstChildWhichIsA('BodyGyro')) then
                Instance.new('BodyPosition', u19[p139]).MaxForce = Vector3.new(math.huge, math.huge, math.huge)

                local _BodyGyro = Instance.new('BodyGyro', u19[p139])

                _BodyGyro.maxTorque = Vector3.new(9000000000, 9000000000, 9000000000)
                _BodyGyro.P = 10000
            end

            u19[p139].BodyGyro.CFrame = _LocalPlayer2.Character[p140].CFrame * p142
            u19[p139].BodyPosition.Position = _LocalPlayer2.Character[p140].CFrame * p141.Position
            u19[p139].BodyPosition.P = p137
            u19[p139].BodyPosition.D = p138
        end
    end

    function Destroy(p144)
        if u19[p144]:FindFirstChildOfClass('BodyGyro') or u19[p144]:FindFirstChildOfClass('BodyPosition') then
            u19[p144]:FindFirstChildOfClass('BodyGyro'):Destroy()
            u19[p144]:FindFirstChildOfClass('BodyPosition'):Destroy()
        end
    end

    function AnimPlay(p43, p44, p45, p46)
        local v47, v48, v49 = pairs(game.Players.LocalPlayer.Character:WaitForChild('Humanoid'):GetPlayingAnimationTracks())

        while true do
            local v50

            v49, v50 = v47(v48, v49)

            if v49 == nil then
                break
            end
            if v50.Animation.AnimationId:match('rbxassetid://' .. p43) then
                v50:Stop(0.5)
            end
        end

        local _Animation = Instance.new('Animation', game:GetService('Workspace'))

        _Animation.AnimationId = 'rbxassetid://' .. p43
        playing = game.Players.LocalPlayer.Character:WaitForChild('Humanoid'):LoadAnimation(_Animation)
        playing.Priority = Enum.AnimationPriority.Action4

        if tonumber(p46) then
            playing:Play(p46)
        else
            playing:Play()
        end
        if tonumber(p44) then
            playing:AdjustSpeed(p44)
        else
            playing:AdjustSpeed(1)
        end
        if tonumber(p45) then
            playing.TimePosition = p45
        end

        _Animation:Destroy()
    end

    -- ============================================
    -- GRAB KNIFE TOOL - VERSIÓN COMPLETA CORREGIDA
    -- ============================================
    local function CreateGrabKnifeTool()
        -- Eliminar tool existente
        if _LocalPlayer2.Backpack:FindFirstChild('grab knife') then
            _LocalPlayer2.Backpack:FindFirstChild('grab knife'):Destroy()
        end
        
        wait(0.1)
        
        -- Crear nuevo tool
        local tool = Instance.new('Tool')
        tool.RequiresHandle = false
        tool.Name = 'grab knife'
        tool.Parent = _LocalPlayer2.Backpack
        
        -- Variable para controlar ejecución
        local executing = false
        
        -- Función para reproducir sonido
        local function PlayKnifeSound(soundId)
            if _LocalPlayer2.Character:FindFirstChild('BOOMBOX') then
                _ReplicatedStorage.MainEvent:FireServer('Boombox', soundId)
            else
                local sound = Instance.new('Sound')
                sound.SoundId = 'rbxassetid://' .. soundId
                sound.Volume = 1
                sound.Parent = workspace
                sound:Play()
                game.Debris:AddItem(sound, 6)
            end
        end
        
        -- Conectar evento de activación
        tool.Activated:Connect(function()
            if executing then return end
            
            -- Verificar si hay una víctima agarrada
            local grabbedValue = _LocalPlayer2.Character.BodyEffects.Grabbed
            if not grabbedValue.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Grab Knife',
                    Text = 'No victim grabbed',
                    Duration = 3
                })
                return
            end
            
            executing = true
            local victim = grabbedValue.Value
            
            -- 1. Detener animación de agarre
            for _, track in pairs(_LocalPlayer2.Character.Humanoid:GetPlayingAnimationTracks()) do
                if track.Animation and track.Animation.AnimationId == 'rbxassetid://3135389157' then
                    track:Stop()
                end
            end
            
            -- 2. Equipar cuchillo del backpack
            local knifeTool = _LocalPlayer2.Backpack:FindFirstChild('Knife') or 
                             _LocalPlayer2.Backpack:FindFirstChild('knife')
            if knifeTool then
                knifeTool.Parent = _LocalPlayer2.Character
            end
            
            -- 3. Posicionar víctima enfrente de espaldas
            local stopAlign = false
            
            spawn(function()
                while not stopAlign and victim and victim.Parent do
                    if victim:FindFirstChild('UpperTorso') then
                        -- Crear BodyPosition si no existe
                        local bodyPos = victim.UpperTorso:FindFirstChild('BodyPosition')
                        if not bodyPos then
                            bodyPos = Instance.new('BodyPosition')
                            bodyPos.MaxForce = Vector3.new(90000, 90000, 90000)
                            bodyPos.P = 8000
                            bodyPos.D = 800
                            bodyPos.Parent = victim.UpperTorso
                        end
                        
                        -- Crear BodyGyro si no existe
                        local bodyGyro = victim.UpperTorso:FindFirstChild('BodyGyro')
                        if not bodyGyro then
                            bodyGyro = Instance.new('BodyGyro')
                            bodyGyro.MaxTorque = Vector3.new(90000, 90000, 90000)
                            bodyGyro.P = 10000
                            bodyGyro.Parent = victim.UpperTorso
                        end
                        
                        -- Posicionar enfrente (2.5 unidades) mirando al revés
                        local playerCF = _LocalPlayer2.Character.HumanoidRootPart.CFrame
                        local frontPos = playerCF.Position + (playerCF.LookVector * 2.5)
                        
                        bodyPos.Position = frontPos
                        bodyGyro.CFrame = CFrame.new(frontPos) * CFrame.Angles(0, math.pi, 0)
                    end
                    task.wait()
                end
            end)
            
            -- 4. SONIDO INICIAL (91676414066995)
            PlayKnifeSound('91676414066995')
            
            -- 5. ANIMACIÓN CONGELADA EN 1.66 (74694286613254)
            local anim1 = Instance.new('Animation')
            anim1.AnimationId = 'rbxassetid://74694286613254'
            local track1 = _LocalPlayer2.Character.Humanoid:LoadAnimation(anim1)
            track1:Play()
            track1:AdjustSpeed(0) -- Congelar
            track1.TimePosition = 1.66 -- Posición exacta
            
            -- 6. ESPERAR 5 SEGUNDOS
            wait(5)
            
            -- 7. MENSAJE EN CHAT "DIE!!..."
            game:GetService('ReplicatedStorage').DefaultChatSystemChatEvents.SayMessageRequest:FireServer('DIE!!...', 'All')
            
            -- 8. REPETIR ANIMACIÓN DE CORTE 5 VECES
            local cutAnim = Instance.new('Animation')
            cutAnim.AnimationId = 'rbxassetid://126796684932532'
            
            local bodyParts = {
                'RightUpperArm',    -- 1
                'LeftUpperArm',     -- 2
                'LeftUpperLeg',     -- 3
                'RightUpperLeg',    -- 4
                'UpperTorso'        -- 5
            }
            
            for i = 1, 5 do
                -- Sonido de corte (125856818464345) - UNA VEZ POR CORTE
                PlayKnifeSound('125856818464345')
                
                -- Animación de corte
                local cutTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(cutAnim)
                cutTrack:Play()
                
                -- Destruir parte del cuerpo
                if victim and victim.Parent then
                    local part = victim:FindFirstChild(bodyParts[i])
                    if part then
                        part:Destroy()
                    end
                end
                
                -- Esperar 2 segundos
                wait(2)
                
                -- Detener animación
                cutTrack:Stop()
            end
            
            -- 9. DESTRUIR LOWERTORSO TAMBIÉN
            if victim and victim.Parent and victim:FindFirstChild('LowerTorso') then
                victim.LowerTorso:Destroy()
            end
            
            -- 10. MANEJAR LA CABEZA
            if victim and victim.Parent and victim:FindFirstChild('Head') then
                -- Clonar cabeza
                local headClone = victim.Head:Clone()
                headClone.CanCollide = false
                headClone.Anchored = true
                headClone.Parent = workspace
                
                -- Poner en mano derecha
                local rightHand = _LocalPlayer2.Character:FindFirstChild('RightHand') or 
                                 _LocalPlayer2.Character:FindFirstChild('Right Arm')
                if rightHand then
                    headClone.CFrame = rightHand.CFrame
                end
                
                wait(0.5)
                
                -- Tirar al suelo
                headClone.Anchored = false
                headClone.CFrame = _LocalPlayer2.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -5)
                
                -- 11. ANIMACIÓN DE PISAR (72406748336709)
                local stompAnim = Instance.new('Animation')
                stompAnim.AnimationId = 'rbxassetid://72406748336709'
                local stompTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(stompAnim)
                stompTrack:Play()
                
                -- Esperar 2 segundos
                wait(2)
                
                -- 12. HACER DESAPARECER LA CABEZA
                headClone.CFrame = CFrame.new(0, -1000, 0)
                headClone:Destroy()
                
                -- 13. MENSAJE FINAL
                game:GetService('ReplicatedStorage').DefaultChatSystemChatEvents.SayMessageRequest:FireServer('HAHAHAHAHAHAHAHAH', 'All')
            end
            
            -- 14. LIMPIAR Y TERMINAR
            stopAlign = true
            
            -- Limpiar controles de física
            if victim and victim.Parent and victim:FindFirstChild('UpperTorso') then
                local bodyPos = victim.UpperTorso:FindFirstChild('BodyPosition')
                local bodyGyro = victim.UpperTorso:FindFirstChild('BodyGyro')
                if bodyPos then bodyPos:Destroy() end
                if bodyGyro then bodyGyro:Destroy() end
            end
            
            -- Liberar agarre
            _ReplicatedStorage.MainEvent:FireServer('Grabbing')
            
            -- Desequipar cuchillo
            if knifeTool and knifeTool.Parent == _LocalPlayer2.Character then
                knifeTool.Parent = _LocalPlayer2.Backpack
            end
            
            -- Limpiar animaciones
            track1:Stop()
            anim1:Destroy()
            cutAnim:Destroy()
            if stompAnim then stompAnim:Destroy() end
            
            executing = false
        end)
        
        -- Añadir tooltip
        tool.ToolTip = 'Grab Knife - Execute grabbed player'
        
        return tool
    end

    -- FUNCIÓN PARA INICIALIZAR TODAS LAS HERRAMIENTAS
    function InitializeAllTools()
        -- Esperar a que el player esté listo
        repeat wait() until _LocalPlayer2 and _LocalPlayer2.Character and _LocalPlayer2:FindFirstChild("Backpack")
        
        -- Crear herramientas
        CreateGrabKnifeTool()
        
        -- También crear las otras herramientas si existen
        -- ... (aquí iría el código de tus otras herramientas) ...
        
        print("[InitializeAllTools] Todas las herramientas creadas")
    end

    -- INICIALIZAR AL CARGAR
    InitializeAllTools()
    
    -- REINICIALIZAR CUANDO EL PERSONAJE SPAWNEE
    _LocalPlayer2.CharacterAdded:Connect(function(character)
        character:WaitForChild('Humanoid')
        wait(1) -- Esperar a que todo cargue
        InitializeAllTools()
    end)

    print("=== ZERAYA SCRIPT CARGADO ===")
    print("- Grab Knife tool creado en backpack")
    print("- Script listo para usar")
    
else
    game:GetService('StarterGui'):SetCore('SendNotification', {
        Title = 'Not Authorized',
        Text = 'Please purchase the script',
        Icon = 'rbxthumb://type=Asset&id=7257741890&w=150&h=150',
        Duration = 10,
    })
    setclipboard('https://discord.gg/mFnYMm7e')
end
