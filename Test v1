    local function u107(p101)
        p101.Archivable = true

        local v102 = p101:Clone()

        v102.HumanoidRootPart.Anchored = false
        v102.Humanoid.MaxHealth = 9000000000
        v102.Humanoid.Health = 9000000000

        v102.RagdollConstraints:Destroy()
        v102.BodyEffects:Destroy()
        v102:WaitForChild('GRABBING_CONSTRAINT'):Destroy()

        local v103, v104, v105 = pairs(v102:GetDescendants())

        while true do
            local v106

            v105, v106 = v103(v104, v105)

            if v105 == nil then
                break
            end
            if v106:IsA('BasePart') and (v106.Name ~= 'Head' and (v106.Name ~= 'HumanoidRootPart' and (v106.Name ~= 'UpperTorso' and (v106.Name ~= 'LowerTorso' and (v106.Name ~= 'LeftUpperArm' and (v106.Name ~= 'RightUpperArm' and (v106.Name ~= 'LeftLowerArm' and (v106.Name ~= 'RightLowerArm' and (v106.Name ~= 'LeftHand' and (v106.Name ~= 'RightHand' and (v106.Name ~= 'LeftUpperLeg' and (v106.Name ~= 'RightUpperLeg' and (v106.Name ~= 'LeftLowerLeg' and (v106.Name ~= 'RightLowerLeg' and (v106.Name ~= 'LeftFoot' and v106.Name ~= 'RightFoot'))))))))))))))) then
                v106.Massless = false

                v106:Destroy()
            end
            if v106:IsA('BasePart') or (v106:IsA('MeshPart') or v106:IsA('Part')) then
                v106.CustomPhysicalProperties = PhysicalProperties.new(100, 2, 0.5, 100, 1)
                v106.Transparency = 1
                v106.CanCollide = false

                if v106:FindFirstChildWhichIsA('BodyGyro') and v106:FindFirstChildWhichIsA('BodyPosition') then
                    v106:Destroy()
                end
            end
            if v106:IsA('Decal') then
                v106.Transparency = 1
            end
            if v106:IsA('ForceField') then
                v106:Destroy()
            end
            if v106:IsA('Motor6D') then
                v106:Destroy()
            end
        end

        v102.HumanoidRootPart.Transparency = 1
        v102.Parent = _LocalPlayer2.Character

        v102.Humanoid:ChangeState('GettingUp')

        v102.HumanoidRootPart.CFrame = _LocalPlayer2.Character.HumanoidRootPart.CFrame
        p101.Archivable = false
        p101.LeftHand:FindFirstChildOfClass('Motor6D').Enabled = false
        p101.RightHand:FindFirstChildOfClass('Motor6D').Enabled = false
        p101.LeftFoot:FindFirstChildOfClass('Motor6D').Enabled = false
        p101.RightFoot:FindFirstChildOfClass('Motor6D').Enabled = false

        return v102
    end

