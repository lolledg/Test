    local function PlaySound(soundId)
        if _LocalPlayer2.Character:FindFirstChild('BOOMBOX') then
            _ReplicatedStorage.MainEvent:FireServer('Boombox', soundId)
        else
            local sound = Instance.new('Sound')
            sound.SoundId = 'rbxassetid://' .. soundId
            sound.Volume = 1
            sound.Parent = workspace
            sound:Play()
            game.Debris:AddItem(sound, 6)
        end
        return true
    end
    
    function ToolSSs()
        -- TOOL: ACTIVATE
        if _LocalPlayer2.Backpack:FindFirstChild('Activate') then
            _LocalPlayer2.Backpack:FindFirstChild('Activate'):Destroy()
        end

        task.wait(0.1)

        local _Tool = Instance.new('Tool')
        _Tool.RequiresHandle = false
        _Tool.Name = 'Activate'
        _Tool.ToolTip = 'Neck Grab - Main grabbing tool'
        _Tool.Parent = _LocalPlayer2.Backpack

        _Tool.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('ReplicatedStorage').MainEvent:FireServer('Grabbing')
                wait(0.1)
            else
                game:GetService('ReplicatedStorage').MainEvent:FireServer('Grabbing', true)
                
                wait(0.3)
                
                if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                    Grab()
                    PlaySound('4590662761')
                    
                    game:GetService('StarterGui'):SetCore('SendNotification', {
                        Title = 'Neck Grab',
                        Text = 'Grabbing ' .. _LocalPlayer2.Character.BodyEffects.Grabbed.Value.Name,
                        Duration = 2
                    })
                end
            end
        end)
        
        -- TOOL: GRAB KNIFE
        if _LocalPlayer2.Backpack:FindFirstChild('grab knife') then
            _LocalPlayer2.Backpack:FindFirstChild('grab knife'):Destroy()
        end
        
        wait(0.1)
        
        local _ToolGrabKnife = Instance.new('Tool')
        _ToolGrabKnife.RequiresHandle = false
        _ToolGrabKnife.Name = 'grab knife'
        _ToolGrabKnife.ToolTip = 'Grab Knife - Brutal execution'
        _ToolGrabKnife.Parent = _LocalPlayer2.Backpack
        
        _ToolGrabKnife.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                local victim = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                
                PlaySound('91676414066995')
                
                local knife = _LocalPlayer2.Backpack:FindFirstChild('Knife') or 
                             _LocalPlayer2.Backpack:FindFirstChild('knife')
                if knife then
                    knife.Parent = _LocalPlayer2.Character
                end
                
                local freezeAnim = Instance.new('Animation')
                freezeAnim.AnimationId = 'rbxassetid://74694286613254'
                local freezeTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(freezeAnim)
                freezeTrack:Play()
                freezeTrack:AdjustSpeed(0)
                freezeTrack.TimePosition = 1.66
                
                local stopAlign = false
                local alignConnection
                
                alignConnection = game:GetService('RunService').Heartbeat:Connect(function()
                    if stopAlign or not victim or not victim.Parent then
                        if alignConnection then alignConnection:Disconnect() end
                        return
                    end
                    
                    local upperTorso = victim:FindFirstChild('UpperTorso')
                    if not upperTorso then return end
                    
                    local bodyPos = upperTorso:FindFirstChild('BodyPosition')
                    if not bodyPos then
                        bodyPos = Instance.new('BodyPosition')
                        bodyPos.MaxForce = Vector3.new(90000, 90000, 90000)
                        bodyPos.P = 8000
                        bodyPos.D = 800
                        bodyPos.Parent = upperTorso
                    end
                    
                    local bodyGyro = upperTorso:FindFirstChild('BodyGyro')
                    if not bodyGyro then
                        bodyGyro = Instance.new('BodyGyro')
                        bodyGyro.MaxTorque = Vector3.new(90000, 90000, 90000)
                        bodyGyro.P = 10000
                        bodyGyro.Parent = upperTorso
                    end
                    
                    local playerCF = _LocalPlayer2.Character.HumanoidRootPart.CFrame
                    local frontPos = playerCF.Position + (playerCF.LookVector * 2.5)
                    
                    bodyPos.Position = frontPos
                    bodyGyro.CFrame = CFrame.new(frontPos) * CFrame.Angles(0, math.pi, 0)
                end)
                
                local cutAnim = Instance.new('Animation')
                cutAnim.AnimationId = 'rbxassetid://126796684932532'
                
                local bodyParts = {
                    'RightUpperArm',
                    'LeftUpperArm',
                    'LeftUpperLeg',
                    'RightUpperLeg',
                    'UpperTorso'
                }
                
                for i = 1, 5 do
                    PlaySound('125856818464345')
                    
                    local cutTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(cutAnim)
                    cutTrack:Play()
                    
                    if victim and victim.Parent then
                        local part = victim:FindFirstChild(bodyParts[i])
                        if part then part:Destroy() end
                    end
                    
                    wait(2)
                    cutTrack:Stop()
                end
                
                if victim and victim.Parent and victim:FindFirstChild('Head') then
                    local headClone = victim.Head:Clone()
                    headClone.CanCollide = false
                    headClone.Anchored = true
                    headClone.Parent = workspace
                    
                    local rightHand = _LocalPlayer2.Character:FindFirstChild('RightHand') or 
                                     _LocalPlayer2.Character:FindFirstChild('Right Arm')
                    if rightHand then
                        headClone.CFrame = rightHand.CFrame
                    end
                    
                    wait(0.5)
                    headClone.Anchored = false
                    headClone.CFrame = _LocalPlayer2.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -5)
                    
                    local stompAnim = Instance.new('Animation')
                    stompAnim.AnimationId = 'rbxassetid://72406748336709'
                    local stompTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(stompAnim)
                    stompTrack:Play()
                    
                    wait(2)
                    headClone.CFrame = CFrame.new(0, -1000, 0)
                    headClone:Destroy()
                    stompTrack:Stop()
                end
                
                stopAlign = true
                if alignConnection then alignConnection:Disconnect() end
                
                if victim and victim.Parent and victim:FindFirstChild('UpperTorso') then
                    local bodyPos = victim.UpperTorso:FindFirstChild('BodyPosition')
                    local bodyGyro = victim.UpperTorso:FindFirstChild('BodyGyro')
                    if bodyPos then bodyPos:Destroy() end
                    if bodyGyro then bodyGyro:Destroy() end
                end
                
                _ReplicatedStorage.MainEvent:FireServer('Grabbing')
                
                if knife and knife.Parent == _LocalPlayer2.Character then
                    knife.Parent = _LocalPlayer2.Backpack
                end
                
                freezeTrack:Stop()
                freezeAnim:Destroy()
                cutAnim:Destroy()
            end
        end)
        
        -- ============================================
        -- TOOL: HOVERBOARD SURF - VERSIN CORREGIDA
        -- ============================================
        if _LocalPlayer2.Backpack:FindFirstChild('Hoverboard Grab') then
            _LocalPlayer2.Backpack:FindFirstChild('Hoverboard Grab'):Destroy()
        end
        
        wait(0.1)
        
        local hoverboardTool = Instance.new('Tool')
        hoverboardTool.RequiresHandle = false
        hoverboardTool.Name = 'Hoverboard Grab'
        hoverboardTool.ToolTip = ' Surf on victim - Ride their back'
        hoverboardTool.Parent = _LocalPlayer2.Backpack
        
        local isSurfing = false
        local surfVictim = nil
        local surfConnection = nil
        local surfAnim = nil
        local surfWeld = nil
        local currentHeight = 3.5
        local surfSpeed = 30
        
        -- SONIDO DE SURF
        local function PlaySurfSound()
            if _LocalPlayer2.Character:FindFirstChild('BOOMBOX') then
                _ReplicatedStorage.MainEvent:FireServer('Boombox', '4590662764')
            else
                local sound = Instance.new("Sound")
                sound.SoundId = "rbxassetid://4590662764"
                sound.Volume = 0.3
                sound.Parent = workspace
                sound:Play()
                game.Debris:AddItem(sound, 3)
            end
        end
        
        -- CONFIGURAR VCTIMA PARA SURF
        local function SetupSurfVictim(victim)
            if not victim or not victim.Parent then return false end
            
            local humanoid = victim:FindFirstChild("Humanoid")
            local humanoidRoot = victim:FindFirstChild("HumanoidRootPart")
            local lowerTorso = victim:FindFirstChild("LowerTorso")
            
            if not humanoid or not humanoidRoot or not lowerTorso then return false end
            
            -- Limpiar controles antiguos
            for _, part in pairs({humanoidRoot, lowerTorso}) do
                if part then
                    local oldPos = part:FindFirstChild("SurfBodyPos")
                    local oldGyro = part:FindFirstChild("SurfBodyGyro")
                    if oldPos then oldPos:Destroy() end
                    if oldGyro then oldGyro:Destroy() end
                end
            end
            
            -- Crear BodyPosition en HumanoidRootPart para mejor control
            local bodyPos = Instance.new("BodyPosition")
            bodyPos.Name = "SurfBodyPos"
            bodyPos.MaxForce = Vector3.new(90000, 90000, 90000)
            bodyPos.P = 12000
            bodyPos.D = 4000
            bodyPos.Position = humanoidRoot.Position + Vector3.new(0, currentHeight, 0)
            bodyPos.Parent = humanoidRoot
            
            -- Crear BodyGyro en LowerTorso para rotaci贸n
            local bodyGyro = Instance.new("BodyGyro")
            bodyGyro.Name = "SurfBodyGyro"
            bodyGyro.MaxTorque = Vector3.new(90000, 90000, 90000)
            bodyGyro.P = 15000
            bodyGyro.D = 2000
            
            -- Rotar 90 grados hacia adelante (posici贸n acostada boca abajo)
            local currentCFrame = lowerTorso.CFrame
            bodyGyro.CFrame = currentCFrame * CFrame.Angles(math.rad(90), 0, 0)
            bodyGyro.Parent = lowerTorso
            
            -- Configurar humanoide para posici贸n acostada
            humanoid.PlatformStand = true
            humanoid:ChangeState("Physics")
            
            -- Desactivar colisiones
            for _, part in pairs(victim:GetChildren()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                    part.Massless = true
                end
            end
            
            -- Ajustar extremidades para posici贸n de surf
            task.spawn(function()
                wait(0.1)
                -- Brazos extendidos
                local leftUpperArm = victim:FindFirstChild("LeftUpperArm")
                local rightUpperArm = victim:FindFirstChild("RightUpperArm")
                
                if leftUpperArm then
                    leftUpperArm.CFrame = leftUpperArm.CFrame * CFrame.Angles(math.rad(-30), 0, math.rad(15))
                end
                if rightUpperArm then
                    rightUpperArm.CFrame = rightUpperArm.CFrame * CFrame.Angles(math.rad(-30), 0, math.rad(-15))
                end
            end)
            
            PlaySurfSound()
            return bodyPos, bodyGyro
        end
        
        -- DETENER SURF
        local function StopSurf()
            isSurfing = false
            
            if surfConnection then
                surfConnection:Disconnect()
                surfConnection = nil
            end
            
            if surfAnim then
                surfAnim:Stop()
                surfAnim:Destroy()
                surfAnim = nil
            end
            
            if surfWeld then
                surfWeld:Destroy()
                surfWeld = nil
            end
            
            -- RESTAURAR VCTIMA
            if surfVictim and surfVictim.Parent then
                local humanoid = surfVictim:FindFirstChild("Humanoid")
                
                -- Limpiar controles
                for _, control in pairs(surfVictim:GetDescendants()) do
                    if control.Name == "SurfBodyPos" or control.Name == "SurfBodyGyro" then
                        control:Destroy()
                    end
                end
                
                -- Restaurar colisiones
                for _, part in pairs(surfVictim:GetChildren()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = true
                        part.Massless = false
                    end
                end
                
                if humanoid then
                    humanoid.PlatformStand = false
                    humanoid:ChangeState("GettingUp")
                end
            end
            
            -- RESTAURAR JUGADOR
            if _LocalPlayer2.Character and _LocalPlayer2.Character:FindFirstChild("Humanoid") then
                _LocalPlayer2.Character.Humanoid.WalkSpeed = 16
                _LocalPlayer2.Character.Humanoid.JumpPower = 50
                _LocalPlayer2.Character.Humanoid.AutoRotate = true
            end
            
            surfVictim = nil
            
            game:GetService('StarterGui'):SetCore('SendNotification', {
                Title = 'Surf Stopped',
                Text = 'Surfing ended',
                Duration = 2
            })
        end
        
        -- ACTIVAR/DESACTIVAR HOVERBOARD
        hoverboardTool.Activated:Connect(function()
            if isSurfing then
                StopSurf()
                return
            end
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Hoverboard Surf',
                    Text = 'No victim grabbed',
                    Duration = 2
                })
                return
            end
            
            surfVictim = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
            
            local victimPlayer = game.Players:GetPlayerFromCharacter(surfVictim)
            if not victimPlayer or victimPlayer == _LocalPlayer2 then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Hoverboard Surf',
                    Text = 'Cannot surf on yourself',
                    Duration = 2
                })
                return
            end
            
            print('[Hoverboard Surf] Starting surf on:', victimPlayer.Name)
            
            -- Soltar agarre tradicional
            _ReplicatedStorage.MainEvent:FireServer('Grabbing')
            wait(0.2)
            
            -- Configurar v铆ctima
            local bodyPos, bodyGyro = SetupSurfVictim(surfVictim)
            if not bodyPos then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Hoverboard Surf',
                    Text = 'Failed to setup surf',
                    Duration = 2
                })
                return
            end
            
            -- Posicionar jugador ENCIMA de la v铆ctima
            local victimLowerTorso = surfVictim:FindFirstChild("LowerTorso")
            if not victimLowerTorso then
                StopSurf()
                return
            end
            
            -- Posicionar al jugador en la espalda de la v铆ctima
            _LocalPlayer2.Character.HumanoidRootPart.CFrame = victimLowerTorso.CFrame * CFrame.new(0, 0.8, 0.2)
            
            -- Crear weld para mantener posici贸n
            surfWeld = Instance.new("Weld")
            surfWeld.Part0 = _LocalPlayer2.Character.HumanoidRootPart
            surfWeld.Part1 = victimLowerTorso
            surfWeld.C0 = CFrame.new(0, 0.8, 0.2)
            surfWeld.Parent = _LocalPlayer2.Character.HumanoidRootPart
            
            -- TU ANIMACIN DE SURF
            local animation = Instance.new("Animation")
            animation.AnimationId = "rbxassetid://76573949416065"
            surfAnim = _LocalPlayer2.Character.Humanoid:LoadAnimation(animation)
            surfAnim:Play()
            surfAnim:AdjustSpeed(1.0)
            
            -- Configurar jugador
            _LocalPlayer2.Character.Humanoid.WalkSpeed = surfSpeed
            _LocalPlayer2.Character.Humanoid.JumpPower = 0
            _LocalPlayer2.Character.Humanoid.AutoRotate = false
            
            -- ACTIVAR MODO SURF
            isSurfing = true
            
            -- SISTEMA DE MOVIMIENTO SIMPLIFICADO
            surfConnection = game:GetService("RunService").Heartbeat:Connect(function(deltaTime)
                if not isSurfing or not surfVictim or not surfVictim.Parent then
                    StopSurf()
                    return
                end
                
                local humanoidRoot = surfVictim:FindFirstChild("HumanoidRootPart")
                local lowerTorso = surfVictim:FindFirstChild("LowerTorso")
                if not humanoidRoot or not lowerTorso then
                    StopSurf()
                    return
                end
                
                -- DETECTAR MOVIMIENTO PARA TELFONO
                local humanoid = _LocalPlayer2.Character.Humanoid
                local moveDirection = humanoid.MoveDirection
                local isMoving = moveDirection.Magnitude > 0.05
                
                -- Obtener posici贸n actual
                local currentPos = bodyPos.Position
                
                if isMoving then
                    -- MOVER VCTIMA EN DIRECCIN DEL JOYSTICK
                    local moveForce = moveDirection * surfSpeed * deltaTime * 15
                    local newPos = currentPos + moveForce
                    
                    -- Mantener altura
                    newPos = Vector3.new(newPos.X, currentHeight, newPos.Z)
                    bodyPos.Position = newPos
                    
                    -- ROTAR VCTIMA HACIA LA DIRECCIN
                    if bodyGyro then
                        if moveDirection.Magnitude > 0 then
                            -- Calcular rotaci贸n hacia la direcci贸n
                            local targetCFrame = CFrame.new(lowerTorso.Position, 
                                lowerTorso.Position + moveDirection) * CFrame.Angles(math.rad(90), 0, 0)
                            bodyGyro.CFrame = bodyGyro.CFrame:Lerp(targetCFrame, 0.15)
                        end
                    end
                    
                    -- Ajustar velocidad de animaci贸n
                    surfAnim:AdjustSpeed(1.2)
                else
                    -- FLOTACIN ESTTICA CON MOVIMIENTO SUAVE
                    local hoverBob = math.sin(tick() * 2.5) * 0.05
                    bodyPos.Position = Vector3.new(
                        currentPos.X,
                        currentHeight + hoverBob,
                        currentPos.Z
                    )
                    surfAnim:AdjustSpeed(0.9)
                end
                
                -- CONTROL DE ALTURA SIMPLE (solo para PC)
                if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.Space) then
                    currentHeight = math.min(currentHeight + deltaTime * 5, 6)
                elseif game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftShift) then
                    currentHeight = math.max(currentHeight - deltaTime * 5, 2)
                end
                
                -- Ajustar posici贸n del jugador constantemente
                if surfWeld then
                    -- Peque帽o balanceo basado en movimiento
                    local sway = math.sin(tick() * 3) * 0.05
                    surfWeld.C0 = CFrame.new(0, 0.8 + sway, 0.2)
                end
            end)
            
            -- NOTIFICACIN
            game:GetService('StarterGui'):SetCore('SendNotification', {
                Title = ' SURF ACTIVATED ',
                Text = 'Riding ' .. victimPlayer.Name .. '!\nUse joystick to move\nTap tool to stop',
                Duration = 4
            })
        end)
        
        -- Limpieza autom谩tica
        _LocalPlayer2.CharacterAdded:Connect(function()
            if isSurfing then
                StopSurf()
            end
        end)
    end
                    
    ToolSSs()
    game.Players.LocalPlayer.CharacterAdded:Connect(function()
        game.Players.LocalPlayer.Character:WaitForChild('FULLY_LOADED_CHAR')
        ToolSSs()
    end)
else
    game:GetService('StarterGui'):SetCore('SendNotification', {
        Title = 'Your Not Buyer',
        Text = 'Please Buy Our Script Our Discord Server (Discord Server Set To Your CilpBoard)',
        Icon = 'rbxthumb://type=Asset&id=7257741890&w=150&h=150',
        Duration = 10,
    })
    setclipboard('https://discord.gg/mFnYMm7e')
end
