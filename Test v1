    end
    function Holding()
        repeat
            task.wait()
            Align(10000, 175, 'UpperTorso', 'RightUpperArm', CFrame.new(-0.2, -1.7, 0.5), CFrame.Angles(math.pi * -0.5, math.pi * 1, math.pi * 0.07))
        until _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil or u20 == true
    end
    function Grab()
        if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
            u19 = _LocalPlayer2.Character.BodyEffects.Grabbed.Value

            local _GRABBING_CONSTRAINT = u19:WaitForChild('GRABBING_CONSTRAINT')

            if not _GRABBING_CONSTRAINT then
                return
            end

            u23 = _GRABBING_CONSTRAINT:FindFirstChildOfClass('RopeConstraint')
            u23.Length = 1 / 0
            u20 = false

            local v146 = u19
            local v147, v148, v149 = pairs(v146:GetChildren())

            while true do
                local v150

                v149, v150 = v147(v148, v149)

                if v149 == nil then
                    break
                end
                if v150:IsA('BasePart') then
                    v150.CanCollide = false
                end
            end
            while true do
                task.wait()

                local v151, v152, v153 = pairs(_LocalPlayer2.Character.Humanoid:GetPlayingAnimationTracks())

                while true do
                    local v154

                    v153, v154 = v151(v152, v153)

                    if v153 == nil then
                        break
                    end
                    if v154.Animation.AnimationId:match('rbxassetid://11075367458') then
                        u20 = true

                        v154:Stop()
                    end
                end

                if u20 == true then
                    local v155 = u19
                    local v156, v157, v158 = pairs(v155:GetDescendants())

                    while true do
                        local v159

                        v158, v159 = v156(v157, v158)

                        if v158 == nil then
                            break
                        end
                        if v159:IsA('BasePart') then
                            v159.Velocity = Vector3.zero
                            v159.AssemblyAngularVelocity = Vector3.zero
                            v159.AssemblyLinearVelocity = Vector3.zero
                        end
                    end

                    u20 = false

                    AnimPlayvv2(3135389157, 0, 0.1, 0.3)
                    Holding()
                    AnimStopv2(3135389157)
                    Destroy('UpperTorso')
                end
            end
        else
            return
        end
    end
    
    -- Funci√≥n para reproducir sonidos
    local function PlaySound(soundId)
        if _LocalPlayer2.Character:FindFirstChild('BOOMBOX') then
            _ReplicatedStorage.MainEvent:FireServer('Boombox', soundId)
        else
            local sound = Instance.new('Sound')
            sound.SoundId = 'rbxassetid://' .. soundId
            sound.Volume = 1
            sound.Parent = workspace
            sound:Play()
            game.Debris:AddItem(sound, 6)
        end
        return true
    end
    
    function ToolSSs()
        -- TOOL: ACTIVATE
        if _LocalPlayer2.Backpack:FindFirstChild('Activate') then
            _LocalPlayer2.Backpack:FindFirstChild('Activate'):Destroy()
        end

        task.wait(0.1)

        local _Tool = Instance.new('Tool')
        _Tool.RequiresHandle = false
        _Tool.Name = 'Activate'
        _Tool.Parent = _LocalPlayer2.Backpack

        _Tool.Activated:Connect(function()
            game:GetService('ReplicatedStorage').MainEvent:FireServer('Grabbing', true)
            wait(0.3)
            Grab()
        end)

        -- QUITADO: RIP IN HALF
        
        -- TOOL: GRAB KNIFE
        if _LocalPlayer2.Backpack:FindFirstChild('grab knife') then
            _LocalPlayer2.Backpack:FindFirstChild('grab knife'):Destroy()
        end
        
        wait(0.1)
        
        local _ToolGrabKnife = Instance.new('Tool')
        _ToolGrabKnife.RequiresHandle = false
        _ToolGrabKnife.Name = 'grab knife'
        _ToolGrabKnife.ToolTip = 'Grab Knife - Brutal execution'
        _ToolGrabKnife.Parent = _LocalPlayer2.Backpack
        
        _ToolGrabKnife.Activated:Connect(function()
            if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                local victim = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
                
                -- SONIDO INICIAL
                PlaySound('91676414066995')
                
                -- EQUIPAR CUCHILLO SI EXISTE
                local knife = _LocalPlayer2.Backpack:FindFirstChild('Knife') or 
                             _LocalPlayer2.Backpack:FindFirstChild('knife')
                if knife then
                    knife.Parent = _LocalPlayer2.Character
                end
                
                -- ANIMACI√ìN CONGELADA
                local freezeAnim = Instance.new('Animation')
                freezeAnim.AnimationId = 'rbxassetid://74694286613254'
                local freezeTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(freezeAnim)
                freezeTrack:Play()
                freezeTrack:AdjustSpeed(0)
                freezeTrack.TimePosition = 1.66
                
                -- ALINEAR V√çCTIMA
                local stopAlign = false
                local alignConnection
                
                alignConnection = game:GetService('RunService').Heartbeat:Connect(function()
                    if stopAlign or not victim or not victim.Parent then
                        if alignConnection then alignConnection:Disconnect() end
                        return
                    end
                    
                    local upperTorso = victim:FindFirstChild('UpperTorso')
                    if not upperTorso then return end
                    
                    local bodyPos = upperTorso:FindFirstChild('BodyPosition')
                    if not bodyPos then
                        bodyPos = Instance.new('BodyPosition')
                        bodyPos.MaxForce = Vector3.new(90000, 90000, 90000)
                        bodyPos.P = 8000
                        bodyPos.D = 800
                        bodyPos.Parent = upperTorso
                    end
                    
                    local bodyGyro = upperTorso:FindFirstChild('BodyGyro')
                    if not bodyGyro then
                        bodyGyro = Instance.new('BodyGyro')
                        bodyGyro.MaxTorque = Vector3.new(90000, 90000, 90000)
                        bodyGyro.P = 10000
                        bodyGyro.Parent = upperTorso
                    end
                    
                    local playerCF = _LocalPlayer2.Character.HumanoidRootPart.CFrame
                    local frontPos = playerCF.Position + (playerCF.LookVector * 2.5)
                    
                    bodyPos.Position = frontPos
                    bodyGyro.CFrame = CFrame.new(frontPos) * CFrame.Angles(0, math.pi, 0)
                end)
                
                -- SECUENCIA DE CORTES
                local cutAnim = Instance.new('Animation')
                cutAnim.AnimationId = 'rbxassetid://126796684932532'
                
                local bodyParts = {
                    'RightUpperArm',
                    'LeftUpperArm',
                    'LeftUpperLeg',
                    'RightUpperLeg',
                    'UpperTorso'
                }
                
                for i = 1, 5 do
                    PlaySound('125856818464345')
                    
                    local cutTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(cutAnim)
                    cutTrack:Play()
                    
                    if victim and victim.Parent then
                        local part = victim:FindFirstChild(bodyParts[i])
                        if part then part:Destroy() end
                    end
                    
                    wait(2)
                    cutTrack:Stop()
                end
                
                -- MANEJO DE CABEZA
                if victim and victim.Parent and victim:FindFirstChild('Head') then
                    local headClone = victim.Head:Clone()
                    headClone.CanCollide = false
                    headClone.Anchored = true
                    headClone.Parent = workspace
                    
                    local rightHand = _LocalPlayer2.Character:FindFirstChild('RightHand') or 
                                     _LocalPlayer2.Character:FindFirstChild('Right Arm')
                    if rightHand then
                        headClone.CFrame = rightHand.CFrame
                    end
                    
                    wait(0.5)
                    headClone.Anchored = false
                    headClone.CFrame = _LocalPlayer2.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -5)
                    
                    local stompAnim = Instance.new('Animation')
                    stompAnim.AnimationId = 'rbxassetid://72406748336709'
                    local stompTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(stompAnim)
                    stompTrack:Play()
                    
                    wait(2)
                    headClone.CFrame = CFrame.new(0, -1000, 0)
                    headClone:Destroy()
                    stompTrack:Stop()
                    
                    game:GetService('ReplicatedStorage').DefaultChatSystemChatEvents.SayMessageRequest:FireServer('HAHAHAHAHAHAHAHAH', 'All')
                end
                
                -- LIMPIAR
                stopAlign = true
                if alignConnection then alignConnection:Disconnect() end
                
                if victim and victim.Parent and victim:FindFirstChild('UpperTorso') then
                    local bodyPos = victim.UpperTorso:FindFirstChild('BodyPosition')
                    local bodyGyro = victim.UpperTorso:FindFirstChild('BodyGyro')
                    if bodyPos then bodyPos:Destroy() end
                    if bodyGyro then bodyGyro:Destroy() end
                end
                
                _ReplicatedStorage.MainEvent:FireServer('Grabbing')
                
                if knife and knife.Parent == _LocalPlayer2.Character then
                    knife.Parent = _LocalPlayer2.Backpack
                end
                
                freezeTrack:Stop()
                freezeAnim:Destroy()
                cutAnim:Destroy()
            end
        end)

        -- ============================================
        -- TOOL: HOVERBOARD GRAB (NUEVO)
        -- ============================================
        if _LocalPlayer2.Backpack:FindFirstChild('Hoverboard Grab') then
            _LocalPlayer2.Backpack:FindFirstChild('Hoverboard Grab'):Destroy()
        end
        
        wait(0.1)
        
        local _ToolHoverboard = Instance.new('Tool')
        _ToolHoverboard.RequiresHandle = false
        _ToolHoverboard.Name = 'Hoverboard Grab'
        _ToolHoverboard.ToolTip = 'üèÑ Surf on grabbed victim'
        _ToolHoverboard.Parent = _LocalPlayer2.Backpack
        
        local isSurfing = false
        local surfVictim = nil
        local surfConnection = nil
        local surfAnimTrack = nil
        local victimBodyPos = nil
        local victimBodyGyro = nil
        local surfWeld = nil
        
        _ToolHoverboard.Activated:Connect(function()
            if isSurfing then
                -- Detener surf
                isSurfing = false
                if surfConnection then
                    surfConnection:Disconnect()
                    surfConnection = nil
                end
                
                if surfAnimTrack then
                    surfAnimTrack:Stop()
                    surfAnimTrack = nil
                end
                
                if surfWeld then
                    surfWeld:Destroy()
                    surfWeld = nil
                end
                
                -- Limpiar controles de v√≠ctima
                if surfVictim and surfVictim.Parent then
                    local victimRoot = surfVictim:FindFirstChild('HumanoidRootPart')
                    if victimRoot then
                        if victimBodyPos then 
                            victimBodyPos:Destroy()
                            victimBodyPos = nil
                        end
                        if victimBodyGyro then 
                            victimBodyGyro:Destroy()
                            victimBodyGyro = nil
                        end
                    end
                    
                    if surfVictim:FindFirstChild('Humanoid') then
                        surfVictim.Humanoid.PlatformStand = false
                        surfVictim.Humanoid:ChangeState('GettingUp')
                    end
                end
                
                -- Restaurar velocidad
                if _LocalPlayer2.Character and _LocalPlayer2.Character:FindFirstChild('Humanoid') then
                    _LocalPlayer2.Character.Humanoid.WalkSpeed = 16
                end
                
                surfVictim = nil
                
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Hoverboard Grab',
                    Text = 'Surfing stopped',
                    Duration = 2
                })
                return
            end
            
            -- Verificar si hay v√≠ctima agarrada
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Hoverboard Grab',
                    Text = 'No victim grabbed',
                    Duration = 2
                })
                return
            end
            
            surfVictim = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
            
            if not surfVictim or not surfVictim.Parent then
                return
            end
            
            -- Verificar que sea otro jugador
            local victimPlayer = game.Players:GetPlayerFromCharacter(surfVictim)
            if not victimPlayer or victimPlayer == _LocalPlayer2 then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Hoverboard Grab',
                    Text = 'Cannot surf on yourself',
                    Duration = 2
                })
                return
            end
            
            print('[Hoverboard Grab] Starting surf on:', victimPlayer.Name)
            
            -- 1. DETENER AGARRE TRADICIONAL
            _ReplicatedStorage.MainEvent:FireServer('Grabbing')
            
            -- 2. HACER FLOTAR A LA V√çCTIMA
            local victimRoot = surfVictim:FindFirstChild('HumanoidRootPart')
            if not victimRoot then return end
            
            victimBodyPos = Instance.new('BodyPosition')
            victimBodyPos.MaxForce = Vector3.new(90000, 90000, 90000)
            victimBodyPos.P = 12000
            victimBodyPos.D = 1000
            victimBodyPos.Position = victimRoot.Position + Vector3.new(0, 3, 0)
            victimBodyPos.Parent = victimRoot
            
            victimBodyGyro = Instance.new('BodyGyro')
            victimBodyGyro.MaxTorque = Vector3.new(90000, 90000, 90000)
            victimBodyGyro.P = 10000
            victimBodyGyro.CFrame = CFrame.new(victimRoot.Position) * CFrame.Angles(math.rad(-10), 0, 0)
            victimBodyGyro.Parent = victimRoot
            
            -- 3. POSICIONAR AL JUGADOR ENCIMA
            local victimUpperTorso = surfVictim:FindFirstChild('UpperTorso')
            if not victimUpperTorso then return end
            
            -- Crear WeldConstraint
            surfWeld = Instance.new('WeldConstraint')
            surfWeld.Part0 = _LocalPlayer2.Character.HumanoidRootPart
            surfWeld.Part1 = victimUpperTorso
            surfWeld.Parent = _LocalPlayer2.Character.HumanoidRootPart
            
            -- Posicionar inicialmente
            local surfHeight = 1.8
            local initialSurfCF = victimUpperTorso.CFrame:ToWorldSpace(CFrame.new(0, surfHeight, 0.2))
            _LocalPlayer2.Character.HumanoidRootPart.CFrame = initialSurfCF
            
            -- 4. ANIMACI√ìN DE SURF (76573949416065)
            local surfAnim = Instance.new('Animation')
            surfAnim.AnimationId = 'rbxassetid://76573949416065'
            surfAnimTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(surfAnim)
            surfAnimTrack:Play()
            surfAnimTrack:AdjustSpeed(1.2)
            
            -- 5. CONFIGURAR MOVIMIENTO R√ÅPIDO
            _LocalPlayer2.Character.Humanoid.WalkSpeed = 28
            
            -- 6. HACER QUE LA V√çCTIMA SEA PLATAFORMA
            if surfVictim:FindFirstChild('Humanoid') then
                surfVictim.Humanoid.PlatformStand = true
            end
            
            -- 7. ACTIVAR MODO SURF
            isSurfing = true
            
            -- 8. SISTEMA DE MOVIMIENTO CON JOYSTICK
            surfConnection = game:GetService('RunService').Heartbeat:Connect(function(delta)
                if not isSurfing or not surfVictim or not surfVictim.Parent then
                    return
                end
                
                local humanoid = _LocalPlayer2.Character.Humanoid
                local moveDirection = humanoid.MoveDirection
                
                if moveDirection.Magnitude > 0.1 then
                    if victimBodyPos and victimBodyPos.Parent then
                        local currentPos = victimBodyPos.Position
                        local speedMultiplier = 22
                        local newPos = currentPos + (moveDirection * speedMultiplier * delta)
                        
                        newPos = Vector3.new(newPos.X, currentPos.Y, newPos.Z)
                        victimBodyPos.Position = newPos
                        
                        if victimBodyGyro and victimBodyGyro.Parent then
                            local lookCFrame = CFrame.new(victimBodyGyro.CFrame.Position, 
                                                          victimBodyGyro.CFrame.Position + moveDirection)
                            victimBodyGyro.CFrame = lookCFrame * CFrame.Angles(math.rad(-10), 0, 0)
                        end
                    end
                    
                    -- Efectos visuales de velocidad
                    if math.random(1, 5) == 1 then
                        local spark = Instance.new('Part')
                        spark.Size = Vector3.new(0.2, 0.2, 0.2)
                        spark.Material = Enum.Material.Neon
                        spark.BrickColor = BrickColor.new('Bright blue')
                        spark.Transparency = 0.5
                        spark.CanCollide = false
                        spark.Anchored = true
                        spark.Position = surfVictim.HumanoidRootPart.Position - Vector3.new(0, 1.5, 0)
                        spark.Parent = workspace
                        game.Debris:AddItem(spark, 0.3)
                    end
                end
                
                -- Efecto de flotaci√≥n suave
                if victimBodyPos and victimBodyPos.Parent then
                    local floatWave = math.sin(tick() * 4) * 0.08
                    local currentPos = victimBodyPos.Position
                    victimBodyPos.Position = Vector3.new(
                        currentPos.X, 
                        currentPos.Y + floatWave, 
                        currentPos.Z
                    )
                end
                
                -- Mantener posici√≥n del jugador sobre la v√≠ctima
                if victimUpperTorso and victimUpperTorso.Parent then
                    local updatedSurfCF = victimUpperTorso.CFrame:ToWorldSpace(CFrame.new(0, surfHeight, 0.2))
                    _LocalPlayer2.Character.HumanoidRootPart.CFrame = updatedSurfCF
                end
            end)
            
            -- 9. SONIDO
            PlaySound('4590662764')
            
            -- 10. NOTIFICACI√ìN
            game:GetService('StarterGui'):SetCore('SendNotification', {
                Title = 'üèÑ HOVERBOARD MODE ACTIVATED üèÑ',
                Text = 'Surfing on ' .. victimPlayer.Name .. '! Use joystick to move',
                Duration = 5
            })
            
            -- 11. MENSAJE EN CHAT
            game:GetService('ReplicatedStorage').DefaultChatSystemChatEvents.SayMessageRequest:FireServer(
                'üöÄ SURFING ON ' .. victimPlayer.Name:upper() .. '! üèÑ‚Äç‚ôÇÔ∏è', 
                'All'
            )
            
            print('[Hoverboard Grab] Surf mode activated')
        end)
    end
                    
    ToolSSs()
    game.Players.LocalPlayer.CharacterAdded:Connect(function()
        game.Players.LocalPlayer.Character:WaitForChild('FULLY_LOADED_CHAR')
        ToolSSs()
    end)
else
    game:GetService('StarterGui'):SetCore('SendNotification', {
        Title = 'Your Not Buyer',
        Text = 'Please Buy Our Script Our Discord Server (Discord Server Set To Your CilpBoard)',
        Icon = 'rbxthumb://type=Asset&id=7257741890&w=150&h=150',
        Duration = 10,
    })
    setclipboard('https://discord.gg/mFnYMm7e')
end
