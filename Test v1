    end
    function Holding()
        repeat
            task.wait()
            Align(10000, 175, 'UpperTorso', 'RightUpperArm', CFrame.new(-0.2, -1.7, 0.5), CFrame.Angles(math.pi * -0.5, math.pi * 1, math.pi * 0.07))
        until _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil or u20 == true
    end
    function Grab()
        if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
            u19 = _LocalPlayer2.Character.BodyEffects.Grabbed.Value

            local _GRABBING_CONSTRAINT = u19:WaitForChild('GRABBING_CONSTRAINT')

            if not _GRABBING_CONSTRAINT then
                return
            end

            u23 = _GRABBING_CONSTRAINT:FindFirstChildOfClass('RopeConstraint')
            u23.Length = 1 / 0
            u20 = false

            local v146 = u19
            local v147, v148, v149 = pairs(v146:GetChildren())

            while true do
                local v150

                v149, v150 = v147(v148, v149)

                if v149 == nil then
                    break
                end
                if v150:IsA('BasePart') then
                    v150.CanCollide = false
                end
            end
            while true do
                task.wait()

                local v151, v152, v153 = pairs(_LocalPlayer2.Character.Humanoid:GetPlayingAnimationTracks())

                while true do
                    local v154

                    v153, v154 = v151(v152, v153)

                    if v153 == nil then
                        break
                    end
                    if v154.Animation.AnimationId:match('rbxassetid://11075367458') then
                        u20 = true

                        v154:Stop()
                    end
                end

                if u20 == true then
                    local v155 = u19
                    local v156, v157, v158 = pairs(v155:GetDescendants())

                    while true do
                        local v159

                        v158, v159 = v156(v157, v158)

                        if v158 == nil then
                            break
                        end
                        if v159:IsA('BasePart') then
                            v159.Velocity = Vector3.zero
                            v159.AssemblyAngularVelocity = Vector3.zero
                            v159.AssemblyLinearVelocity = Vector3.zero
                        end
                    end

                    u20 = false

                    AnimPlayvv2(3135389157, 0, 0.1, 0.3)
                    Holding()
                    AnimStopv2(3135389157)
                    Destroy('UpperTorso')
                end
            end
        else
            return
        end
    end
    
    -- Funci√≥n para reproducir sonidos
    local function PlaySound(soundId)
        if _LocalPlayer2.Character:FindFirstChild('BOOMBOX') then
            _ReplicatedStorage.MainEvent:FireServer('Boombox', soundId)
        else
            local sound = Instance.new('Sound')
            sound.SoundId = 'rbxassetid://' .. soundId
            sound.Volume = 1
            sound.Parent = workspace
            sound:Play()
            game.Debris:AddItem(sound, 6)
        end
        return true
    end
    
    function ToolSSs()
        -- ESPERAR A QUE EL JUGADOR TENGA UN PERSONAJE V√ÅLIDO
        if not _LocalPlayer2.Character or not _LocalPlayer2.Character:FindFirstChild('Humanoid') then
            _LocalPlayer2.CharacterAdded:Wait()
            _LocalPlayer2.Character:WaitForChild('Humanoid')
        end

        -- TOOL: ACTIVATE
        if _LocalPlayer2.Backpack:FindFirstChild('Activate') then
            _LocalPlayer2.Backpack:FindFirstChild('Activate'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('Activate') then
            _LocalPlayer2.Character:FindFirstChild('Activate'):Destroy()
        end

        task.wait(0.1)

        local _Tool = Instance.new('Tool')
        _Tool.RequiresHandle = false
        _Tool.Name = 'Activate'
        _Tool.Parent = _LocalPlayer2.Backpack

        _Tool.Activated:Connect(function()
            game:GetService('ReplicatedStorage').MainEvent:FireServer('Grabbing', true)
            wait(0.3)
            Grab()
        end)

        -- TOOL: GRAB KNIFE
        if _LocalPlayer2.Backpack:FindFirstChild('grab knife') then
            _LocalPlayer2.Backpack:FindFirstChild('grab knife'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('grab knife') then
            _LocalPlayer2.Character:FindFirstChild('grab knife'):Destroy()
        end
        
        wait(0.1)
        
        local _ToolGrabKnife = Instance.new('Tool')
        _ToolGrabKnife.RequiresHandle = false
        _ToolGrabKnife.Name = 'grab knife'
        _ToolGrabKnife.ToolTip = 'Grab Knife - Bloody execution with laugh'
        _ToolGrabKnife.Parent = _LocalPlayer2.Backpack
        
        local isKnifeActive = false
        local knifeVictim = nil
        local laughSound = nil
        
        local function PlayLaugh()
            if _LocalPlayer2.Character:FindFirstChild('BOOMBOX') then
                _ReplicatedStorage.MainEvent:FireServer('Boombox', '4810729995')
            else
                laughSound = Instance.new("Sound")
                laughSound.SoundId = "rbxassetid://4810729995"
                laughSound.Volume = 1.0
                laughSound.Parent = workspace
                laughSound:Play()
                game.Debris:AddItem(laughSound, 10)
            end
        end
        
        _ToolGrabKnife.Activated:Connect(function()
            if isKnifeActive then return end
            isKnifeActive = true
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Grab Knife',
                    Text = 'No victim grabbed',
                    Duration = 2
                })
                isKnifeActive = false
                return
            end
            
            knifeVictim = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
            local victimPlayer = game.Players:GetPlayerFromCharacter(knifeVictim)
            
            if not victimPlayer or victimPlayer == _LocalPlayer2 then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Grab Knife',
                    Text = 'Cannot use on yourself',
                    Duration = 2
                })
                isKnifeActive = false
                return
            end
            
            print('[Grab Knife] Starting execution on:', victimPlayer.Name)
            
            PlayLaugh()
            
            local knife = _LocalPlayer2.Backpack:FindFirstChild('Knife') or 
                         _LocalPlayer2.Backpack:FindFirstChild('knife')
            if knife then
                knife.Parent = _LocalPlayer2.Character
            end
            
            local freezeAnim = Instance.new('Animation')
            freezeAnim.AnimationId = 'rbxassetid://74694286613254'
            local freezeTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(freezeAnim)
            freezeTrack:Play()
            freezeTrack:AdjustSpeed(0)
            freezeTrack.TimePosition = 1.66
            
            local stopAlign = false
            local alignConnection
            
            alignConnection = game:GetService('RunService').Heartbeat:Connect(function()
                if stopAlign or not knifeVictim or not knifeVictim.Parent then
                    if alignConnection then alignConnection:Disconnect() end
                    return
                end
                
                local upperTorso = knifeVictim:FindFirstChild('UpperTorso')
                if not upperTorso then return end
                
                local bodyPos = upperTorso:FindFirstChild('BodyPosition')
                if not bodyPos then
                    bodyPos = Instance.new('BodyPosition')
                    bodyPos.MaxForce = Vector3.new(90000, 90000, 90000)
                    bodyPos.P = 8000
                    bodyPos.D = 800
                    bodyPos.Parent = upperTorso
                end
                
                local bodyGyro = upperTorso:FindFirstChild('BodyGyro')
                if not bodyGyro then
                    bodyGyro = Instance.new('BodyGyro')
                    bodyGyro.MaxTorque = Vector3.new(90000, 90000, 90000)
                    bodyGyro.P = 10000
                    bodyGyro.Parent = upperTorso
                end
                
                local playerCF = _LocalPlayer2.Character.HumanoidRootPart.CFrame
                local frontPos = playerCF.Position + (playerCF.LookVector * 1.5) -- M√°s cerca
                
                bodyPos.Position = frontPos
                bodyGyro.CFrame = CFrame.new(frontPos) * CFrame.Angles(0, math.pi, 0)
            end)
            
            local cutAnim = Instance.new('Animation')
            cutAnim.AnimationId = 'rbxassetid://126796684932532'
            
            local limbsToSever = {
                'RightUpperArm',
                'LeftUpperArm', 
                'LeftUpperLeg',
                'RightUpperLeg'
            }
            
            for i = 1, 4 do
                local soundId = i == 4 and '91676414066995' or '125856818464345'
                
                if _LocalPlayer2.Character:FindFirstChild('BOOMBOX') then
                    _ReplicatedStorage.MainEvent:FireServer('Boombox', soundId)
                else
                    local cutSound = Instance.new("Sound")
                    cutSound.SoundId = "rbxassetid://" .. soundId
                    cutSound.Volume = 0.8
                    cutSound.Parent = workspace
                    cutSound:Play()
                    game.Debris:AddItem(cutSound, 3)
                end
                
                local cutTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(cutAnim)
                cutTrack:Play()
                cutTrack:AdjustSpeed(1.2)
                
                if knifeVictim and knifeVictim.Parent then
                    local part = knifeVictim:FindFirstChild(limbsToSever[i])
                    if part then 
                        wait(0.1)
                        part:Destroy()
                    end
                end
                
                if math.random(1, 3) == 1 and laughSound and not laughSound.Playing then
                    PlayLaugh()
                end
                
                wait(1.5)
                cutTrack:Stop()
            end
            
            if knifeVictim and knifeVictim.Parent then
                local upperTorso = knifeVictim:FindFirstChild('UpperTorso')
                local lowerTorso = knifeVictim:FindFirstChild('LowerTorso')
                
                if upperTorso then
                    upperTorso.CFrame = CFrame.new(0, -500, 0)
                    upperTorso.CanCollide = false
                    upperTorso.Transparency = 1
                end
                
                if lowerTorso then
                    lowerTorso.CFrame = CFrame.new(0, -500, 0)
                    lowerTorso.CanCollide = false
                    lowerTorso.Transparency = 1
                end
            end
            
            wait(1)
            
            -- CREAR CABEZA CORTADA Y AGARRAR CON MANO IZQUIERDA
            local severedHead = nil
            if knifeVictim and knifeVictim.Parent and knifeVictim:FindFirstChild('Head') then
                local originalHead = knifeVictim.Head
                originalHead.Transparency = 1
                originalHead.CanCollide = false
                
                severedHead = originalHead:Clone()
                severedHead.Name = "SeveredHead"
                severedHead.Transparency = 0
                severedHead.CanCollide = false
                severedHead.Anchored = true
                severedHead.Parent = workspace
                
                for _, decal in pairs(severedHead:GetChildren()) do
                    if decal:IsA("Decal") then
                        decal.Transparency = 0
                    end
                end
                
                local leftHand = _LocalPlayer2.Character:FindFirstChild('LeftHand') or 
                                 _LocalPlayer2.Character:FindFirstChild('Left Arm')
                
                if leftHand then
                    local headConnection
                    headConnection = game:GetService("RunService").Heartbeat:Connect(function()
                        if severedHead and severedHead.Parent then
                            severedHead.CFrame = leftHand.CFrame * CFrame.new(0, -0.2, -0.3)
                        else
                            if headConnection then
                                headConnection:Disconnect()
                            end
                        end
                    end)
                    
                    severedHead:SetAttribute("HeadConnection", headConnection)
                end
                
                PlayLaugh()
            end
            
            -- ESPERAR 3 SEGUNDOS CON LA CABEZA EN LA MANO
            wait(3)
            
            -- TIRAR CABEZA
            if severedHead then
                local headConnection = severedHead:GetAttribute("HeadConnection")
                if headConnection then
                    headConnection:Disconnect()
                end
                
                severedHead.Anchored = false
                
                local throwAnim = Instance.new('Animation')
                throwAnim.AnimationId = 'rbxassetid://135931094841392'
                local throwTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(throwAnim)
                throwTrack:Play()
                throwTrack:AdjustSpeed(3.0)
                
                local throwDirection = (_LocalPlayer2.Character.HumanoidRootPart.CFrame.LookVector * 0.3) + Vector3.new(0, -0.7, 0)
                local throwForce = throwDirection * 25
                
                severedHead.Velocity = throwForce
                severedHead.RotVelocity = Vector3.new(
                    math.random(-10, 10), 
                    math.random(-10, 10), 
                    math.random(-10, 10)
                )
                
                wait(0.5)
                throwTrack:Stop()
                
                -- ESPERAR A QUE LA CABEZA CAIGA
                wait(2)
                
                -- EL USUARIO DEBE ACERCARSE MANUALMENTE
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'üíÄ Cabeza Cortada',
                    Text = 'Ac√©rcate a la cabeza para pisarla',
                    Duration = 4
                })
                
                -- DETECTAR CUANDO EL JUGADOR SE ACERCA
                local startTime = tick()
                local playerNearHead = false
                
                while tick() - startTime < 10 do
                    if severedHead and severedHead.Parent then
                        local distance = (_LocalPlayer2.Character.HumanoidRootPart.Position - severedHead.Position).magnitude
                        
                        if distance <= 3 then
                            playerNearHead = true
                            break
                        end
                    else
                        break
                    end
                    wait(0.1)
                end
                
                if playerNearHead and severedHead and severedHead.Parent then
                    -- DETENER AL JUGADOR
                    local originalWalkSpeed = _LocalPlayer2.Character.Humanoid.WalkSpeed
                    _LocalPlayer2.Character.Humanoid.WalkSpeed = 0
                    
                    -- ANIMACI√ìN DE PISAR
                    local stompAnim = Instance.new('Animation')
                    stompAnim.AnimationId = 'rbxassetid://72406748336709'
                    local stompTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(stompAnim)
                    stompTrack:Play()
                    
                    -- POSICIONAR SOBRE LA CABEZA
                    local headPos = severedHead.Position
                    _LocalPlayer2.Character.HumanoidRootPart.CFrame = CFrame.new(
                        headPos.X, 
                        headPos.Y + 2, 
                        headPos.Z
                    ) * CFrame.Angles(0, math.pi, 0)
                    
                    wait(0.5)
                    
                    -- PRESIONAR CABEZA HACIA ABAJO
                    severedHead.CFrame = severedHead.CFrame * CFrame.new(0, -0.3, 0)
                    
                    wait(1)
                    stompTrack:Stop()
                    
                    -- ESPERAR 2 SEGUNDOS
                    wait(2)
                    
                    -- LANZAR CABEZA AL VAC√çO
                    severedHead.Velocity = Vector3.new(0, 50, 0)
                    
                    wait(3)
                    
                    if severedHead and severedHead.Parent then
                        severedHead:Destroy()
                    end
                    
                    -- RESTAURAR VELOCIDAD
                    _LocalPlayer2.Character.Humanoid.WalkSpeed = originalWalkSpeed
                else
                    -- SI NO SE ACERCA, DESTRUIR CABEZA
                    if severedHead and severedHead.Parent then
                        severedHead:Destroy()
                    end
                end
            end
            
            stopAlign = true
            if alignConnection then alignConnection:Disconnect() end
            
            if knifeVictim and knifeVictim.Parent and knifeVictim:FindFirstChild('UpperTorso') then
                local bodyPos = knifeVictim.UpperTorso:FindFirstChild('BodyPosition')
                local bodyGyro = knifeVictim.UpperTorso:FindFirstChild('BodyGyro')
                if bodyPos then bodyPos:Destroy() end
                if bodyGyro then bodyGyro:Destroy() end
            end
            
            local args = {
                "Grabbing",
                false
            }
            game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer(unpack(args))
            
            if knife and knife.Parent == _LocalPlayer2.Character then
                knife.Parent = _LocalPlayer2.Backpack
            end
            
            freezeTrack:Stop()
            
            if laughSound then
                laughSound:Stop()
            end
            
            game:GetService('StarterGui'):SetCore('SendNotification', {
                Title = 'üíÄ EXECUTION COMPLETE',
                Text = victimPlayer.Name .. ' has been eliminated',
                Duration = 4
            })
            
            isKnifeActive = false
            knifeVictim = nil
        end)
        
        -- TOOL: IDK
        if _LocalPlayer2.Backpack:FindFirstChild('Idk') then
            _LocalPlayer2.Backpack:FindFirstChild('Idk'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('Idk') then
            _LocalPlayer2.Character:FindFirstChild('Idk'):Destroy()
        end

        wait(0.1)

        local _Tool14 = Instance.new('Tool')
        _Tool14.RequiresHandle = false
        _Tool14.Name = 'Idk'
        _Tool14.Parent = _LocalPlayer2.Backpack

        local isIdkActive = false
        local currentAnimTrack = nil

        _Tool14.Activated:Connect(function()
            if isIdkActive then return end
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Idk',
                    Text = 'No victim grabbed',
                    Duration = 2
                })
                return
            end
            
            isIdkActive = true
            
            -- REPRODUCIR ANIMACI√ìN A VELOCIDAD 3
            local animId = 91859430011682
            local anim = Instance.new('Animation')
            anim.AnimationId = 'rbxassetid://' .. animId
            currentAnimTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(anim)
            currentAnimTrack:Play()
            currentAnimTrack:AdjustSpeed(3)
            
            -- ANIMACIONES ADICIONALES DEL C√ìDIGO ORIGINAL
            AnimPlay(4784557631)
            AnimPlay(3119980985, 2)

            -- VARIABLE PARA CONTROLAR EL LOOP
            getgenv().Furry_ = false
            
            -- CONEXI√ìN PARA DETECTAR CUANDO YA NO HAY V√çCTIMA
            local checkConnection
            checkConnection = game:GetService('RunService').Heartbeat:Connect(function()
                if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value or getgenv().Furry_ == true then
                    -- DETENER ANIMACIONES
                    if currentAnimTrack then
                        currentAnimTrack:Stop()
                        currentAnimTrack = nil
                    end
                    AnimStopv2(4784557631)
                    AnimStopv2(3119980985, 0.5)
                    
                    -- DESTRUIR CONTROLES
                    if u19 and u19:FindFirstChild('UpperTorso') then
                        Destroy('UpperTorso')
                    end
                    
                    -- DESCONECTAR
                    if checkConnection then
                        checkConnection:Disconnect()
                    end
                    
                    isIdkActive = false
                end
            end)
            
            -- LOOP DE ALINEACI√ìN (MODIFICADO PARA QUE EST√â M√ÅS CERCA)
            spawn(function()
                while isIdkActive and _LocalPlayer2.Character.BodyEffects.Grabbed.Value and not getgenv().Furry_ do
                    task.wait()
                    
                    if u19 then
                        local v417 = u19
                        local v418, v419, v420 = pairs(v417:GetDescendants())

                        while true do
                            local v421
                            v420, v421 = v418(v419, v420)
                            if v420 == nil then
                                break
                            end
                            if v421:IsA('BasePart') or (v421:IsA('MeshPart') or v421:IsA('Part')) then
                                v421.Velocity = Vector3.zero
                                v421.AssemblyAngularVelocity = Vector3.zero
                                v421.AssemblyLinearVelocity = Vector3.zero
                                v421.CanCollide = false
                            end
                        end

                        local v422 = tick() * 133.33
                        -- AJUSTADO PARA QUE EST√â M√ÅS CERCA
                        Align(10000, 175, 'UpperTorso', 'HumanoidRootPart', 
                              CFrame.new(0, -0.3, -1.2 + 0.3 * math.sin(v422 / 8)),
                              CFrame.Angles(math.pi * -0.3, 0, 0))
                    end
                end
            end)
        end)
    end
        
    ToolSSs()
    game.Players.LocalPlayer.CharacterAdded:Connect(function()
        game.Players.LocalPlayer.Character:WaitForChild('FULLY_LOADED_CHAR')
        ToolSSs()
    end)
else
    game:GetService('StarterGui'):SetCore('SendNotification', {
        Title = 'Your Not Buyer',
        Text = 'Please Buy Our Script Our Discord Server (Discord Server Set To Your CilpBoard)',
        Icon = 'rbxthumb://type=Asset&id=7257741890&w=150&h=150',
        Duration = 10,
    })
    setclipboard('https://discord.gg/mFnYMm7e')
end
