                v106.Massless = false
                v106:Destroy()
            end
            if v106:IsA('BasePart') or (v106:IsA('MeshPart') or v106:IsA('Part')) then
                v106.CustomPhysicalProperties = PhysicalProperties.new(100, 2, 0.5, 100, 1)
                v106.Transparency = 1
                v106.CanCollide = false

                if v106:FindFirstChildWhichIsA('BodyGyro') and v106:FindFirstChildWhichIsA('BodyPosition') then
                    v106:Destroy()
                end
            end
            if v106:IsA('Decal') then
                v106.Transparency = 1
            end
            if v106:IsA('ForceField') then
                v106:Destroy()
            end
            if v106:IsA('Motor6D') then
                v106:Destroy()
            end
        end

        v102.HumanoidRootPart.Transparency = 1
        v102.Parent = _LocalPlayer2.Character

        v102.Humanoid:ChangeState('GettingUp')

        v102.HumanoidRootPart.CFrame = _LocalPlayer2.Character.HumanoidRootPart.CFrame
        p101.Archivable = false
        
        local leftHandMotor = p101.LeftHand:FindFirstChildOfClass('Motor6D')
        local rightHandMotor = p101.RightHand:FindFirstChildOfClass('Motor6D')
        local leftFootMotor = p101.LeftFoot:FindFirstChildOfClass('Motor6D')
        local rightFootMotor = p101.RightFoot:FindFirstChildOfClass('Motor6D')
        
        if leftHandMotor then leftHandMotor.Enabled = false end
        if rightHandMotor then rightHandMotor.Enabled = false end
        if leftFootMotor then leftFootMotor.Enabled = false end
        if rightFootMotor then rightFootMotor.Enabled = true end

        return v102
    end

