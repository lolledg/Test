    function AudioPlay(p61, p62, p63)
        if not _LocalPlayer2.Character or not _LocalPlayer2.Character:FindFirstChild('LowerTorso') then return end
        if _LocalPlayer2.Character.LowerTorso.BOOMBOXSOUND.SoundId == 'rbxassetid://' .. p61 then return end
        u40 = u40 + 1
        game:GetService('ReplicatedStorage'):WaitForChild('MainEvent'):FireServer('Boombox', p61)

        if tonumber(p62) then
            _LocalPlayer2.Character.LowerTorso.BOOMBOXSOUND.Volume = p62
