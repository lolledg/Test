            task.wait()
            Align(10000, 175, 'UpperTorso', 'RightUpperArm', CFrame.new(-0.2, -1.7, 0.5), CFrame.Angles(math.pi * -0.5, math.pi * 1, math.pi * 0.07))
        until not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects.Grabbed.Value or u20 == true
    end
    function Grab()
        if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then return end
        if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
            u19 = _LocalPlayer2.Character.BodyEffects.Grabbed.Value

            local _GRABBING_CONSTRAINT = u19:WaitForChild('GRABBING_CONSTRAINT')

            if not _GRABBING_CONSTRAINT then
                return
            end

            u23 = _GRABBING_CONSTRAINT:FindFirstChildOfClass('RopeConstraint')
            if u23 then
                u23.Length = 1 / 0
            end
            u20 = false

            local v146 = u19
            local v147, v148, v149 = pairs(v146:GetChildren())

            while true do
                local v150

                v149, v150 = v147(v148, v149)

                if v149 == nil then
                    break
                end
                if v150:IsA('BasePart') then
                    v150.CanCollide = false
                end
            end
            while true do
                task.wait()

                if not _LocalPlayer2.Character or not _LocalPlayer2.Character:FindFirstChild('Humanoid') then break end
                local v151, v152, v153 = pairs(_LocalPlayer2.Character.Humanoid:GetPlayingAnimationTracks())

                while true do
                    local v154

                    v153, v154 = v151(v152, v153)

                    if v153 == nil then
                        break
                    end
                    if v154.Animation.AnimationId:match('rbxassetid://11075367458') then
                        u20 = true
                        v154:Stop()
                    end
                end

                if u20 == true then
                    local v155 = u19
                    local v156, v157, v158 = pairs(v155:GetDescendants())

                    while true do
                        local v159

                        v158, v159 = v156(v157, v158)

                        if v158 == nil then
                            break
                        end
                        if v159:IsA('BasePart') then
                            v159.Velocity = Vector3.zero
                            v159.AssemblyAngularVelocity = Vector3.zero
                            v159.AssemblyLinearVelocity = Vector3.zero
                        end
                    end

                    u20 = false

                    AnimPlayvv2(3135389157, 0, 0.1, 0.3)
                    Holding()
                    AnimStopv2(3135389157)
                    Destroy('UpperTorso')
                end
            end
        else
            return
        end
    end
    
    -- Funci칩n para reproducir sonidos
    local function PlaySound(soundId)
        if not _LocalPlayer2.Character then return false end
        if _LocalPlayer2.Character:FindFirstChild('BOOMBOX') then
            _ReplicatedStorage.MainEvent:FireServer('Boombox', soundId)
        else
            local sound = Instance.new('Sound')
            sound.SoundId = 'rbxassetid://' .. soundId
            sound.Volume = 1
            sound.Parent = workspace
            sound:Play()
            game.Debris:AddItem(sound, 6)
        end
        return true
    end
    
    -- ========================================
    -- SISTEMA HOVERBOARD CORREGIDO - VERSION 2.0
    -- ========================================
    local isHoverboarding = false
    local hoverboardVictim = nil
    local hoverboardConnection = nil
    local hoverboardGui = nil
    local cleanupConnection = nil
    
    -- CREAR GUI MEJORADO
    local function CreateHoverboardGui()
        if hoverboardGui then
            hoverboardGui:Destroy()
        end
        
        hoverboardGui = Instance.new("ScreenGui")
        hoverboardGui.Name = "HoverboardGui"
        hoverboardGui.ResetOnSpawn = false
        hoverboardGui.Parent = game.CoreGui
        
        local mainFrame = Instance.new("Frame")
        mainFrame.Size = UDim2.new(0, 220, 0, 110)
        mainFrame.Position = UDim2.new(0.5, -110, 0.1, 0)
        mainFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
        mainFrame.BackgroundTransparency = 0.3
        mainFrame.BorderSizePixel = 0
        mainFrame.Parent = hoverboardGui
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 10)
        corner.Parent = mainFrame
        
        local title = Instance.new("TextLabel")
        title.Size = UDim2.new(1, 0, 0, 35)
        title.Position = UDim2.new(0, 0, 0, 0)
        title.BackgroundTransparency = 1
        title.Text = "游띷 HOVERBOARD ACTIVO"
        title.TextColor3 = Color3.fromRGB(255, 255, 255)
        title.TextSize = 18
        title.Font = Enum.Font.GothamBold
        title.Parent = mainFrame
        
        local turboIndicator = Instance.new("Frame")
        turboIndicator.Size = UDim2.new(0.85, 0, 0, 22)
        turboIndicator.Position = UDim2.new(0.075, 0, 0.45, 0)
        turboIndicator.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
        turboIndicator.BorderSizePixel = 0
        turboIndicator.Parent = mainFrame
        
        local turboCorner = Instance.new("UICorner")
        turboCorner.CornerRadius = UDim.new(0, 5)
        turboCorner.Parent = turboIndicator
        
        local turboFill = Instance.new("Frame")
        turboFill.Size = UDim2.new(0.3, 0, 1, 0)
        turboFill.Position = UDim2.new(0, 0, 0, 0)
        turboFill.BackgroundColor3 = Color3.fromRGB(0, 220, 100)
        turboFill.BorderSizePixel = 0
        turboFill.Name = "TurboFill"
        turboFill.Parent = turboIndicator
        
        local turboFillCorner = Instance.new("UICorner")
        turboFillCorner.CornerRadius = UDim.new(0, 5)
        turboFillCorner.Parent = turboFill
        
        local controlsText = Instance.new("TextLabel")
        controlsText.Size = UDim2.new(1, 0, 0, 30)
        controlsText.Position = UDim2.new(0, 0, 0.72, 0)
        controlsText.BackgroundTransparency = 1
        controlsText.Text = "TURBO: Left Ctrl | VOLAR: Space/Shift"
        controlsText.TextColor3 = Color3.fromRGB(180, 180, 200)
        controlsText.TextSize = 12
        controlsText.Font = Enum.Font.Gotham
        controlsText.Parent = mainFrame
        
        return hoverboardGui
    end
    
    -- ACTUALIZAR BARRA DE TURBO
    local function UpdateTurboBar(isTurbo)
        if hoverboardGui then
            local mainFrame = hoverboardGui:FindFirstChild("Frame")
            if mainFrame then
                local turboIndicator = mainFrame:FindFirstChild("Frame")
                if turboIndicator then
                    local turboFill = turboIndicator:FindFirstChild("TurboFill")
                    if turboFill then
                        if isTurbo then
                            turboFill.BackgroundColor3 = Color3.fromRGB(255, 100, 50)
                            TweenService:Create(turboFill, TweenInfo.new(0.2), {Size = UDim2.new(1, 0, 1, 0)}):Play()
                        else
                            turboFill.BackgroundColor3 = Color3.fromRGB(0, 220, 100)
                            TweenService:Create(turboFill, TweenInfo.new(0.3), {Size = UDim2.new(0.3, 0, 1, 0)}):Play()
                        end
                    end
                end
            end
        end
    end
    
    -- LIMPIAR TODO
    local function CleanupHoverboard()
        isHoverboarding = false
        
        if hoverboardConnection then
            hoverboardConnection:Disconnect()
            hoverboardConnection = nil
        end
        
        if cleanupConnection then
            cleanupConnection:Disconnect()
            cleanupConnection = nil
        end
        
        if hoverboardGui then
            hoverboardGui:Destroy()
            hoverboardGui = nil
        end
        
        hoverboardVictim = nil
    end
    
    -- INICIAR HOVERBOARD
    local function StartHoverboard(victim)
        if isHoverboarding or not victim then return end
        
        if not _LocalPlayer2.Character or not _LocalPlayer2.Character:FindFirstChild("HumanoidRootPart") then
            return
        end
        
        isHoverboarding = true
        hoverboardVictim = victim
        
        -- Crear GUI
        CreateHoverboardGui()
        
        -- Obtener partes de la v칤ctima
        local upperTorso = victim:FindFirstChild("UpperTorso")
        local victimHRP = victim:FindFirstChild("HumanoidRootPart")
        
        if not upperTorso or not victimHRP then
            CleanupHoverboard()
            return
        end
        
        -- CREAR CONTROLES PARA LA V칈CTIMA
        local bodyPos = upperTorso:FindFirstChild("HoverboardPosition") or Instance.new("BodyPosition")
        bodyPos.Name = "HoverboardPosition"
        bodyPos.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
        bodyPos.P = 30000
        bodyPos.D = 3000
        bodyPos.Parent = upperTorso
        
        local bodyGyro = upperTorso:FindFirstChild("HoverboardGyro") or Instance.new("BodyGyro")
        bodyGyro.Name = "HoverboardGyro"
        bodyGyro.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
        bodyGyro.P = 40000
        bodyGyro.D = 2000
        bodyGyro.Parent = upperTorso
        
        -- POSICI칍N Y ROTACI칍N INICIAL CORRECTA
        -- La v칤ctima debe estar BOCA ABAJO (prone) como tabla de surf
        local startCFrame = _LocalPlayer2.Character.HumanoidRootPart.CFrame
        bodyPos.Position = startCFrame.Position + Vector3.new(0, -2, 0)
        bodyGyro.CFrame = startCFrame * CFrame.Angles(math.rad(90), 0, 0) -- Rotaci칩n correcta: boca abajo
        
        -- POSICIONAR JUGADOR ENCIMA DE LA V칈CTIMA
        _LocalPlayer2.Character.HumanoidRootPart.CFrame = CFrame.new(
            upperTorso.Position.X,
            upperTorso.Position.Y + 3,
            upperTorso.Position.Z
        )
        
        -- REPRODUCIR ANIMACI칍N DE HOVERBOARD
        local hoverAnim = Instance.new("Animation")
        hoverAnim.AnimationId = "rbxassetid://13157862997"
        local hoverTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(hoverAnim)
        hoverTrack:Play()
        hoverTrack:AdjustSpeed(1.2)
        
        -- VARIABLES DE CONTROL
        local normalSpeed = 60
        local turboSpeed = 180
        local flySpeed = 40
        local smoothFactor = 0.15
        
        -- SISTEMA DE MOVIMIENTO
        hoverboardConnection = game:GetService("RunService").RenderStepped:Connect(function(delta)
            if not isHoverboarding or not victim or not victim.Parent then
                CleanupHoverboard()
                if hoverTrack then hoverTrack:Stop() end
                return
            end
            
            -- VERIFICAR SI LA V칈CTIMA SALI칍 DEL RAGDOLL
            local victimHumanoid = victim:FindFirstChild("Humanoid")
            if victimHumanoid then
                local bodyEffects = victim:FindFirstChild("BodyEffects")
                if bodyEffects then
                    local ragdoll = bodyEffects:FindFirstChild("Ragdoll")
                    if ragdoll and not ragdoll.Value then
                        -- V칈CTIMA SALI칍 DEL RAGDOLL - LIMPIAR TODO
                        CleanupHoverboard()
                        if hoverTrack then hoverTrack:Stop() end
                        
                        -- Destruir controles de la v칤ctima
                        if upperTorso:FindFirstChild("HoverboardPosition") then
                            upperTorso.HoverboardPosition:Destroy()
                        end
                        if upperTorso:FindFirstChild("HoverboardGyro") then
                            upperTorso.HoverboardGyro:Destroy()
                        end
                        
                        game:GetService('StarterGui'):SetCore('SendNotification', {
                            Title = '游띷 Hoverboard',
                            Text = 'V칤ctima se liber칩',
                            Duration = 3
                        })
                        return
                    end
                end
            end
            
            local userInputService = game:GetService("UserInputService")
            local playerHRP = _LocalPlayer2.Character.HumanoidRootPart
            
            -- DETECTAR TURBO
            local isTurboActive = userInputService:IsKeyDown(Enum.KeyCode.LeftControl)
            UpdateTurboBar(isTurboActive)
            
            -- CALCULAR DIRECCI칍N DE MOVIMIENTO
            local moveDirection = Vector3.zero
            local camera = workspace.CurrentCamera
            local cameraCFrame = camera.CFrame
            
            -- Movimiento basado en la c치mara
            if userInputService:IsKeyDown(Enum.KeyCode.W) then
                moveDirection = moveDirection + cameraCFrame.LookVector
            end
            if userInputService:IsKeyDown(Enum.KeyCode.S) then
                moveDirection = moveDirection - cameraCFrame.LookVector
            end
            if userInputService:IsKeyDown(Enum.KeyCode.A) then
                moveDirection = moveDirection - cameraCFrame.RightVector
            end
            if userInputService:IsKeyDown(Enum.KeyCode.D) then
                moveDirection = moveDirection + cameraCFrame.RightVector
            end
            
            -- Vuelo vertical
            local verticalMove = 0
            if userInputService:IsKeyDown(Enum.KeyCode.Space) then
                verticalMove = 1
            elseif userInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                verticalMove = -1
            end
            
            -- CALCULAR VELOCIDAD
            local currentSpeed = isTurboActive and turboSpeed or normalSpeed
            if verticalMove ~= 0 then
                currentSpeed = flySpeed
            end
            
            -- APLICAR MOVIMIENTO SUAVE
            if moveDirection.Magnitude > 0 then
                moveDirection = (moveDirection.Unit * Vector3.new(1, 0, 1)) + Vector3.new(0, verticalMove, 0)
                
                if moveDirection.Magnitude > 0 then
                    moveDirection = moveDirection.Unit
                end
                
                local targetPos = bodyPos.Position + (moveDirection * currentSpeed * delta)
                bodyPos.Position = bodyPos.Position:Lerp(targetPos, smoothFactor)
                
                -- MANTENER ROTACI칍N CORRECTA (BOCA ABAJO)
                local lookDirection = Vector3.new(moveDirection.X, 0, moveDirection.Z)
                if lookDirection.Magnitude > 0 then
                    local targetCFrame = CFrame.lookAt(upperTorso.Position, upperTorso.Position + lookDirection)
                    bodyGyro.CFrame = targetCFrame * CFrame.Angles(math.rad(90), 0, 0)
                end
            else
                -- Sin movimiento, agregar peque침o movimiento vertical
                bodyPos.Position = bodyPos.Position + Vector3.new(0, verticalMove * flySpeed * delta, 0)
            end
            
            -- MANTENER JUGADOR SOBRE LA V칈CTIMA
            local offsetY = 3
            playerHRP.CFrame = CFrame.new(
                upperTorso.Position.X,
                upperTorso.Position.Y + offsetY,
                upperTorso.Position.Z
            )
            
            -- Mantener velocidad del jugador
            playerHRP.AssemblyLinearVelocity = Vector3.zero
            playerHRP.AssemblyAngularVelocity = Vector3.zero
            
            -- MANTENER ALTURA M칈NIMA SOBRE EL SUELO
            if verticalMove == 0 then
                local ray = Ray.new(upperTorso.Position, Vector3.new(0, -20, 0))
                local hit, position = workspace:FindPartOnRayWithIgnoreList(ray, {victim, _LocalPlayer2.Character})
                
                if hit then
                    local minHeight = position.Y + 1.5
                    if bodyPos.Position.Y < minHeight then
                        bodyPos.Position = Vector3.new(bodyPos.Position.X, minHeight, bodyPos.Position.Z)
                    end
                end
            end
        end)
        
        -- CONEXI칍N PARA DETECTAR CAMBIOS EN RAGDOLL
        cleanupConnection = game:GetService("RunService").Heartbeat:Connect(function()
            if not isHoverboarding or not victim or not victim.Parent then
                CleanupHoverboard()
                if hoverTrack then hoverTrack:Stop() end
                
                -- Limpiar controles
                if upperTorso:FindFirstChild("HoverboardPosition") then
                    upperTorso.HoverboardPosition:Destroy()
                end
                if upperTorso:FindFirstChild("HoverboardGyro") then
                    upperTorso.HoverboardGyro:Destroy()
                end
            end
        end)
        
        -- Notificaci칩n
        game:GetService('StarterGui'):SetCore('SendNotification', {
            Title = '游띷 HOVERBOARD ACTIVADO',
            Text = 'Usa WASD para moverte, Space/Shift para volar, Ctrl para turbo',
            Duration = 5
        })
    end
    
    function ToolSSs()
        -- ESPERAR A QUE EL JUGADOR TENGA UN PERSONAJE V츼LIDO
        if not _LocalPlayer2 then
            _LocalPlayer2 = game:GetService('Players').LocalPlayer
        end
        
        if not _LocalPlayer2.Character then
            _LocalPlayer2.CharacterAdded:Wait()
        end
        
        repeat
            wait()
        until _LocalPlayer2.Character and _LocalPlayer2.Character:FindFirstChild('Humanoid') and _LocalPlayer2.Character:FindFirstChild('BodyEffects')

        -- TOOL: ACTIVATE
        if _LocalPlayer2.Backpack:FindFirstChild('Activate') then
            _LocalPlayer2.Backpack:FindFirstChild('Activate'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('Activate') then
            _LocalPlayer2.Character:FindFirstChild('Activate'):Destroy()
        end

        task.wait(0.1)

        local _Tool = Instance.new('Tool')
        _Tool.RequiresHandle = false
        _Tool.Name = 'Activate'
        _Tool.Parent = _LocalPlayer2.Backpack

        _Tool.Activated:Connect(function()
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character:FindFirstChild('BodyEffects') then
                return
            end
            
            game:GetService('ReplicatedStorage').MainEvent:FireServer('Grabbing', true)
            wait(0.3)
            Grab()
        end)

        -- TOOL: GRAB KNIFE (VERSI칍N SIMPLE SIN LAG)
        if _LocalPlayer2.Backpack:FindFirstChild('grab knife') then
            _LocalPlayer2.Backpack:FindFirstChild('grab knife'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('grab knife') then
            _LocalPlayer2.Character:FindFirstChild('grab knife'):Destroy()
        end
        
        wait(0.1)
        
        local grabKnifeTool = Instance.new('Tool')
        grabKnifeTool.RequiresHandle = false
        grabKnifeTool.Name = 'grab knife'
        grabKnifeTool.ToolTip = 'Grab Knife - Simple sin lag'
        grabKnifeTool.Parent = _LocalPlayer2.Backpack
        
        local isKnifeActive = false
        
        grabKnifeTool.Activated:Connect(function()
            if isKnifeActive then return end
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then
                return
            end
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Grab Knife',
                    Text = 'No victim grabbed',
                    Duration = 2
                })
                return
            end
            
            isKnifeActive = true
            
            local victim = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
            local victimPlayer = game.Players:GetPlayerFromCharacter(victim)
            
            if not victimPlayer or victimPlayer == _LocalPlayer2 then
                isKnifeActive = false
                return
            end
            
            -- PROCESO SIMPLIFICADO
            local freezeTrack = AnimPlayvv2("74694286613254", 3.0, 0, 0.3)
            
            wait(1)
            
            -- SOLTAR V칈CTIMA
            game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Grabbing", false)
            
            if freezeTrack then
                freezeTrack:Stop()
            end
            
            isKnifeActive = false
        end)
        
        -- TOOL: IDK (CON EFECTOS)
        if _LocalPlayer2.Backpack:FindFirstChild('Idk') then
            _LocalPlayer2.Backpack:FindFirstChild('Idk'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('Idk') then
            _LocalPlayer2.Character:FindFirstChild('Idk'):Destroy()
        end

        wait(0.1)

        local idkTool = Instance.new('Tool')
        idkTool.RequiresHandle = false
        idkTool.Name = 'Idk'
        idkTool.ToolTip = 'Idk - Efectos de velocidad'
        idkTool.Parent = _LocalPlayer2.Backpack

        local isIdkActive = false

        idkTool.Activated:Connect(function()
            if isIdkActive then return end
            
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then
                return
            end
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Idk',
                    Text = 'No victim grabbed',
                    Duration = 2
                })
                return
            end
            
            isIdkActive = true
            
            -- ANIMACI칍N CON VELOCIDAD 3x
            local mainTrack = AnimPlayvv2("91859430011682", 3.0, 0, 0.3)
            
            -- CONTROL
            local shouldStop = false
            
            local checkConnection
            checkConnection = game:GetService('RunService').Heartbeat:Connect(function()
                if shouldStop or not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects or 
                   not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                    
                    if mainTrack then
                        mainTrack:Stop()
                    end
                    
                    if checkConnection then
                        checkConnection:Disconnect()
                    end
                    
                    isIdkActive = false
                end
            end)
            
            -- ALINEACI칍N
            task.spawn(function()
                while isIdkActive and _LocalPlayer2.Character and _LocalPlayer2.Character.BodyEffects and 
                      _LocalPlayer2.Character.BodyEffects.Grabbed.Value do
                    task.wait()
                    
                    if u19 then
                        local timeOffset = tick() * 200
                        Align(30000, 800, 'UpperTorso', 'HumanoidRootPart', 
                              CFrame.new(0, -0.3, -1.2 + 0.15 * math.sin(timeOffset / 3)),
                              CFrame.Angles(math.pi * -0.3, 0, 0))
                    end
                end
            end)
            
            -- DETENER AL SOLTAR
            local releaseConnection
            releaseConnection = _LocalPlayer2.Character.BodyEffects.Grabbed:GetPropertyChangedSignal("Value"):Connect(function()
                if _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil then
                    shouldStop = true
                end
            end)
            
            table.insert(u22, releaseConnection)
        end)
        
        -- TOOL: HOVERBOARD (CORREGIDO)
        if _LocalPlayer2.Backpack:FindFirstChild('hoverboard') then
            _LocalPlayer2.Backpack:FindFirstChild('hoverboard'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('hoverboard') then
            _LocalPlayer2.Character:FindFirstChild('hoverboard'):Destroy()
        end
        
        wait(0.1)
        
        local hoverboardTool = Instance.new('Tool')
        hoverboardTool.RequiresHandle = false
        hoverboardTool.Name = 'hoverboard'
        hoverboardTool.ToolTip = 'Hoverboard - Monta v칤ctimas como tabla'
        hoverboardTool.Parent = _LocalPlayer2.Backpack
        
        hoverboardTool.Activated:Connect(function()
            if isHoverboarding then
                -- DETENER HOVERBOARD
                isHoverboarding = false
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = '游띷 Hoverboard',
                    Text = 'Desmontado',
                    Duration = 2
                })
                return
            end
            
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then
                return
            end
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Hoverboard',
                    Text = 'Agarra a alguien primero',
                    Duration = 2
                })
                return
            end
            
            local victim = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
            
            -- SOLTAR PARA PODER MONTAR
            game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Grabbing", false)
            
            -- ESPERAR UN MOMENTO
            wait(0.3)
            
            -- INICIAR HOVERBOARD
            StartHoverboard(victim)
        end)
    end
        
    ToolSSs()
    game.Players.LocalPlayer.CharacterAdded:Connect(function()
        wait(2)
        ToolSSs()
    end)
else
    game:GetService('StarterGui'):SetCore('SendNotification', {
        Title = 'Your Not Buyer',
        Text = 'Please Buy Our Script Our Discord Server (Discord Server Set To Your CilpBoard)',
        Icon = 'rbxthumb://type=Asset&id=7257741890&w=150&h=150',
        Duration = 10,
    })
    setclipboard('https://discord.gg/mFnYMm7e')
end
