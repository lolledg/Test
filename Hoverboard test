                StopAudio()
            end
        end)()
    end
    function AnimPlay(p43, p44, p45, p46)
        if not _LocalPlayer2.Character or not _LocalPlayer2.Character:FindFirstChild('Humanoid') then return end
        local v47, v48, v49 = pairs(_LocalPlayer2.Character:WaitForChild('Humanoid'):GetPlayingAnimationTracks())

        while true do
            local v50

            v49, v50 = v47(v48, v49)

            if v49 == nil then
                break
            end
            if v50.Animation.AnimationId:match('rbxassetid://' .. p43) then
                v50:Stop(0.5)
            end
        end

        local _Animation = Instance.new('Animation', game:GetService('Workspace'))

        _Animation.AnimationId = 'rbxassetid://' .. p43
        playing = _LocalPlayer2.Character:WaitForChild('Humanoid'):LoadAnimation(_Animation)
        playing.Priority = Enum.AnimationPriority.Action4

        if tonumber(p46) then
            playing:Play(p46)
        else
            playing:Play()
        end
        if tonumber(p44) then
            playing:AdjustSpeed(p44)
        else
            playing:AdjustSpeed(1)
        end
        if p45 and type(p45) == 'number' then
            wait(p45)
            playing:Stop()
        elseif p45 and type(p45) == 'boolean' then
        else
            wait(0.7)
            playing:Stop()
        end
        _Animation:Destroy()
    end
    function AnimPlayvv2(p51, p52, p53, p54)
