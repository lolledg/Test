            _LocalPlayer2.Character.LowerTorso.BOOMBOXSOUND.PlaybackSpeed = 1
        end
        stop(p61, u40)
    end

    function Align(p64, p65, p66, p67, p68, p69)
        if u19 and u19:FindFirstChild(p67) and _LocalPlayer2.Character and _LocalPlayer2.Character:FindFirstChild(p66) then
            local u70 = u19:FindFirstChild(p67)
            local u71 = _LocalPlayer2.Character:FindFirstChild(p66)

            u70.Size = Vector3.new(2, 2, 2)
            u70.CanCollide = false
            u71.Size = Vector3.new(2, 2, 2)
            u71.CanCollide = false

            local u72 = u71:FindFirstChildOfClass('AlignPosition') or Instance.new('AlignPosition', u71)

            u72.ApplyAtCenterOfMass = true
            u72.MaxForce = p64
            u72.MaxVelocity = math.huge
            u72.ReactionForceEnabled = false
            u72.Responsiveness = p65
            u72.Attachment1 = u70:FindFirstChildOfClass('Attachment')
            u72.Attachment0 = u71:FindFirstChildOfClass('Attachment')

            local u73 = u71:FindFirstChildOfClass('AlignOrientation') or Instance.new('AlignOrientation', u71)

            u73.MaxAngularVelocity = math.huge
            u73.MaxTorque = p64
            u73.PrimaryAxisOnly = false
            u73.ReactionTorqueEnabled = false
            u73.Responsiveness = p65
            u73.Attachment1 = u70:FindFirstChildOfClass('Attachment')
            u73.Attachment0 = u71:FindFirstChildOfClass('Attachment')
            u73.RigidityEnabled = false

            local u74 = u71:FindFirstChildOfClass('Attachment')

            if p68 then
                u74.Position = p68.Position
            end
            if p69 then
                u71.CFrame = u70.CFrame * p69
            end
        end
    end

    function Grab()
        local v75, v76, v77 = pairs(game:GetService('Players'):GetPlayers())

        while true do
            local v78

            v77, v78 = v75(v76, v77)

            if v77 == nil then
                break
            end
            if v78 ~= _LocalPlayer2 and v78.Character and v78.Character:FindFirstChild('UpperTorso') and v78.Character:FindFirstChild('HumanoidRootPart') and v78.Character.Humanoid.Health > 1 and (v78.Character.HumanoidRootPart.Position - _LocalPlayer2.Character.HumanoidRootPart.Position).Magnitude <= 10 then
                u19 = v78.Character
            end
        end
        if u19 then
            u31(u22, game:GetService('RunService').Heartbeat:Connect(function()
                pcall(function()
                    Align(30000, 200, 'HumanoidRootPart', 'HumanoidRootPart', nil, nil)
                end)
            end))
            u31(u22, u19.Humanoid.Died:Connect(function()
                u19 = nil

                local v79, v80, v81 = pairs(u22)

                while true do
                    local v82

                    v81, v82 = v79(v80, v81)

                    if v81 == nil then
                        break
                    end
                    v82:Disconnect()
                end
                u22 = {}
            end))
        end
    end

    function ToolSSs()
        -- DECLARAR VARIABLE PARA HOVERBOARD
        local isHoverboarding = false
        
        if _LocalPlayer2.Character:FindFirstChild('Activate') then
            _LocalPlayer2.Character:FindFirstChild('Activate'):Destroy()
        end

        task.wait(0.1)

        local _Tool = Instance.new('Tool')
        _Tool.RequiresHandle = false
        _Tool.Name = 'Activate'
        _Tool.Parent = _LocalPlayer2.Backpack

        _Tool.Activated:Connect(function()
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character:FindFirstChild('BodyEffects') then
                return
            end
            
            game:GetService('ReplicatedStorage').MainEvent:FireServer('Grabbing', true)
            wait(0.3)
            Grab()
        end)

        -- TOOL: GRAB KNIFE (VERSIÃ“N SIMPLE SIN LAG)
        if _LocalPlayer2.Backpack:FindFirstChild('grab knife') then
            _LocalPlayer2.Backpack:FindFirstChild('grab knife'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('grab knife') then
            _LocalPlayer2.Character:FindFirstChild('grab knife'):Destroy()
        end
        
        wait(0.1)
        
        local grabKnifeTool = Instance.new('Tool')
        grabKnifeTool.RequiresHandle = false
        grabKnifeTool.Name = 'grab knife'
        grabKnifeTool.ToolTip = 'Grab Knife - Simple sin lag'
        grabKnifeTool.Parent = _LocalPlayer2.Backpack
        
        local isKnifeActive = false
        
        grabKnifeTool.Activated:Connect(function()
            if isKnifeActive then return end
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then
                return
            end
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Grab Knife',
                    Text = 'No victim grabbed',
                    Duration = 2
                })
                return
            end
            
            isKnifeActive = true
            
            local victim = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
            local victimPlayer = game.Players:GetPlayerFromCharacter(victim)
            
            if not victimPlayer or victimPlayer == _LocalPlayer2 then
                isKnifeActive = false
                return
            end
            
            -- PROCESO SIMPLIFICADO
            local freezeTrack = AnimPlayvv2("74694286613254", 3.0, 0, 0.3)
            
            wait(1)
            
            -- SOLTAR VÃCTIMA
            game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Grabbing", false)
            
            if freezeTrack then
                freezeTrack:Stop()
            end
            
            isKnifeActive = false
        end)
        
        -- TOOL: IDK (CON EFECTOS)
        if _LocalPlayer2.Backpack:FindFirstChild('Idk') then
            _LocalPlayer2.Backpack:FindFirstChild('Idk'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('Idk') then
            _LocalPlayer2.Character:FindFirstChild('Idk'):Destroy()
        end

        wait(0.1)

        local idkTool = Instance.new('Tool')
        idkTool.RequiresHandle = false
        idkTool.Name = 'Idk'
        idkTool.ToolTip = 'Idk - Efectos de velocidad'
        idkTool.Parent = _LocalPlayer2.Backpack

        local isIdkActive = false

        idkTool.Activated:Connect(function()
            if isIdkActive then return end
            
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then
                return
            end
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Idk',
                    Text = 'No victim grabbed',
                    Duration = 2
                })
                return
            end
            
            isIdkActive = true
            
            -- ANIMACIÃ“N CON VELOCIDAD 3x
            local mainTrack = AnimPlayvv2("91859430011682", 3.0, 0, 0.3)
            
            -- CONTROL
            local shouldStop = false
            
            local checkConnection
            checkConnection = game:GetService('RunService').Heartbeat:Connect(function()
                if shouldStop or not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects or 
                   not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                    
                    if mainTrack then
                        mainTrack:Stop()
                    end
                    
                    if checkConnection then
                        checkConnection:Disconnect()
                    end
                    
                    isIdkActive = false
                end
            end)
            
            -- ALINEACIÃ“N
            task.spawn(function()
                while isIdkActive and _LocalPlayer2.Character and _LocalPlayer2.Character.BodyEffects and 
                      _LocalPlayer2.Character.BodyEffects.Grabbed.Value do
                    task.wait()
                    
                    if u19 then
                        local timeOffset = tick() * 200
                        Align(30000, 800, 'UpperTorso', 'HumanoidRootPart', 
                              CFrame.new(0, -0.3, -1.2 + 0.15 * math.sin(timeOffset / 3)),
                              CFrame.Angles(math.pi * -0.3, 0, 0))
                    end
                end
            end)
            
            -- DETENER AL SOLTAR
            local releaseConnection
            releaseConnection = _LocalPlayer2.Character.BodyEffects.Grabbed:GetPropertyChangedSignal("Value"):Connect(function()
                if _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil then
                    shouldStop = true
                end
            end)
            
            table.insert(u22, releaseConnection)
        end)
        
        -- TOOL: HOVERBOARD (COMPLETAMENTE CORREGIDO)
        if _LocalPlayer2.Backpack:FindFirstChild('hoverboard') then
            _LocalPlayer2.Backpack:FindFirstChild('hoverboard'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('hoverboard') then
            _LocalPlayer2.Character:FindFirstChild('hoverboard'):Destroy()
        end
        
        wait(0.1)
        
        local hoverboardTool = Instance.new('Tool')
        hoverboardTool.RequiresHandle = false
        hoverboardTool.Name = 'hoverboard'
        hoverboardTool.ToolTip = 'Hoverboard - Monta vÃ­ctimas como tabla'
        hoverboardTool.Parent = _LocalPlayer2.Backpack
        
        hoverboardTool.Activated:Connect(function()
            if isHoverboarding then
                -- DETENER HOVERBOARD
                isHoverboarding = false
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'ðŸ›¹ Hoverboard',
                    Text = 'Desmontado',
                    Duration = 2
                })
                return
            end
            
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then
                return
            end
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Hoverboard',
                    Text = 'Agarra a alguien primero',
                    Duration = 2
                })
                return
            end
            
            local victim = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
            
            if not victim or not victim.Parent then
                return
            end
            
            -- SOLTAR PARA PODER MONTAR
            game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Grabbing", false)
            
            wait(0.3)
            
            -- ACTIVAR HOVERBOARD
            isHoverboarding = true
            
            -- ANIMACIÃ“N DE HOVERBOARD (puedes cambiar el ID por la animaciÃ³n que prefieras)
            local hoverTrack = AnimPlayvv2("3695333486", 1.0, 0, 0.3)
            
            -- ALINEACIÃ“N CONTINUA
            local hoverConnection
            hoverConnection = game:GetService('RunService').Heartbeat:Connect(function()
                if not isHoverboarding or not _LocalPlayer2.Character or not victim or not victim.Parent then
                    if hoverConnection then
                        hoverConnection:Disconnect()
                    end
                    if hoverTrack then
                        hoverTrack:Stop()
                    end
                    isHoverboarding = false
                    return
                end
                
                pcall(function()
                    if victim:FindFirstChild('HumanoidRootPart') and _LocalPlayer2.Character:FindFirstChild('HumanoidRootPart') then
                        local victimRoot = victim:FindFirstChild('HumanoidRootPart')
                        local playerRoot = _LocalPlayer2.Character:FindFirstChild('HumanoidRootPart')
                        
                        -- Posicionar vÃ­ctima debajo como tabla de hoverboard
                        victimRoot.CFrame = playerRoot.CFrame * CFrame.new(0, -3, 0) * CFrame.Angles(math.rad(90), 0, 0)
                        victimRoot.Velocity = Vector3.zero
                        victimRoot.AssemblyLinearVelocity = Vector3.zero
                        victimRoot.AssemblyAngularVelocity = Vector3.zero
                    end
                end)
            end)
            
            -- AÃ±adir a la tabla de conexiones para cleanup
            table.insert(u22, hoverConnection)
            
            game:GetService('StarterGui'):SetCore('SendNotification', {
                Title = 'ðŸ›¹ Hoverboard',
                Text = 'Montado! Click de nuevo para desmontar',
                Duration = 2
            })
        end)
    end
        
    ToolSSs()
    game.Players.LocalPlayer.CharacterAdded:Connect(function()
        wait(2)
        ToolSSs()
    end)
else
    game:GetService('StarterGui'):SetCore('SendNotification', {
        Title = 'Your Not Buyer',
        Text = 'Please Buy Our Script Our Discord Server (Discord Server Set To Your CilpBoard)',
        Icon = 'rbxthumb://type=Asset&id=7257741890&w=150&h=150',
        Duration = 10,
    })
    setclipboard('https://discord.gg/mFnYMm7e')
end
