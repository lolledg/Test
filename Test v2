            if v517 and v517.Character then
                local _Character11 = v517.Character
                local v519, v520, v521 = ipairs({
                    'RightUpperArm',
                    'LeftUpperArm',
                    'RightUpperLeg',
                    'LeftUpperLeg',
                })

                while true do
                    local v522

                    v521, v522 = v519(v520, v521)

                    if v521 == nil then
