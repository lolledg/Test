            end

            -- Animaci칩n de nubes (pulsaci칩n + rotaci칩n)
            task.spawn(function()
                local rot, sc, grow = 0, 1, true
                while cloudsModel and cloudsModel.Parent do
                    rot = rot + 0.015
                    sc  = grow and math.min(sc + 0.004, 1.2) or math.max(sc - 0.004, 0.9)
                    if sc >= 1.2 then grow = false elseif sc <= 0.9 then grow = true end
