t.Activated:Connect(function()if LP2.Character.BodyEffects.Grabbed.Value then for _,tr in pairs(LP2.Character.Humanoid:GetPlayingAnimationTracks())do if tr.Animation.AnimationId=='rbxassetid://3135389157'then tr:Stop()end end local s=false spawn(function()repeat task.wait()Align(8000,800,'UpperTorso','HumanoidRootPart',CFrame.new(0,0,-10),CFrame.Angles(0,math.pi,0))until s end)task.wait(0.6)AnimPlay(10714350889)task.wait(4.5)AnimPlay(10714350889)task.wait(3.4)AnimStop(10714350889)AnimPlay(16281379714)spawn(function()repeat task.wait()Align(8000,800,'UpperTorso','HumanoidRootPart',CFrame.new(0,15,-10),CFrame.Angles(0,math.pi,0))until s end)s=true task.wait(0.2)local vn=tostring(LP2.Character.BodyEffects.Grabbed.Value)if game.Players:FindFirstChild(vn)then local c=game.Players[vn].Character if c and c:FindFirstChild('UpperTorso')then c.UpperTorso.CFrame=CFrame.new(0,-1200,0)end end task.wait(0.3)RS.MainEvent:FireServer('Grabbing')Destroy('UpperTorso')end end)

