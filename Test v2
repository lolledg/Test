        _Tool28.Parent = _LocalPlayer2.Backpack

        _Tool28.Activated:Connect(function()
            Destroy('UpperTorso')
            Destroy('LowerTorso')
            Destroy('RightUpperArm')
            Destroy('LeftUpperArm')
            Destroy('Head')

            local v91, v92, v93 = pairs(u22)

            while true do
                local v94

                v93, v94 = v91(v92, v93)

                if v93 == nil then
                    break
                end
                pcall(v94.Disconnect, v94)
            end

            u22 = {}
        end)

        function AudioPlay(p95, p96, p97, p98, p99, p100)
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character:FindFirstChild('LowerTorso') then return end
            local u101 = u40

            game:GetService('ReplicatedStorage'):WaitForChild('MainEvent'):FireServer('BoomboxStop')

            repeat
                game:GetService('RunService').RenderStepped:Wait()
            until not _LocalPlayer2.Character.LowerTorso:FindFirstChild('BOOMBOXSOUND') or u101 ~= u40

            if u101 ~= u40 then
                return
            end

            local v102 = p95
            local v103 = p96 or 1
            local v104 = p97 or 1
            local v105 = p98 or 5

            task.wait()
            game:GetService('ReplicatedStorage'):WaitForChild('MainEvent'):FireServer('Boombox', v102)
            task.wait(p100 or 0.25)

            local u106 = _LocalPlayer2.Character.LowerTorso:WaitForChild('BOOMBOXSOUND', 5)

            if u106 then
                u106.Volume = v103
                u106.Pitch = v104
                u106.PlaybackSpeed = v105
            end
        end
        
        -- SISTEMA HOVERBOARD ORIGINAL
        local isHoverboarding = false
        local hoverboardVictim = nil
        local hoverMainTrack = nil
        local victimTrack = nil
        local hoverConnection = nil
        local hoverMoveConnection = nil
        local hoverReleaseConnection = nil
        local humanoidDiedConnection = nil

        function CleanupHoverboard()
            isHoverboarding = false
            
            if hoverMainTrack then
                hoverMainTrack:Stop()
                hoverMainTrack = nil
            end
            if victimTrack then
                victimTrack:Stop()
                victimTrack = nil
            end
            if hoverConnection then
                hoverConnection:Disconnect()
                hoverConnection = nil
            end
            if hoverMoveConnection then
                hoverMoveConnection:Disconnect()
                hoverMoveConnection = nil
            end
            if hoverReleaseConnection then
                hoverReleaseConnection:Disconnect()
                hoverReleaseConnection = nil
            end
            if humanoidDiedConnection then
                humanoidDiedConnection:Disconnect()
                humanoidDiedConnection = nil
            end
            
            if hoverboardVictim and hoverboardVictim:FindFirstChild("UpperTorso") then
                Destroy("UpperTorso")
            end
            
            hoverboardVictim = nil
        end

        function StartHoverboard(victim)
            if isHoverboarding then
                CleanupHoverboard()
                return
            end
            
            if not _LocalPlayer2.Character or not victim then return end
            
            isHoverboarding = true
            hoverboardVictim = victim
            
            hoverMainTrack = AnimPlayvv2("77646644732733", 0, 1, 0.3)
            
            local victimAnimId = math.random(1, 2) == 1 and "83734025018597" or "107864399491345"
            local victimHumanoid = victim:FindFirstChild("Humanoid")
            if victimHumanoid then
                local victimAnim = Instance.new("Animation")
                victimAnim.AnimationId = "rbxassetid://" .. victimAnimId
                victimTrack = victimHumanoid:LoadAnimation(victimAnim)
                victimTrack:Play(0, 1, 1.0)
                victimAnim:Destroy()
            end
            
            local speed = 40
            local lastInputTime = tick()
            local currentVelocity = Vector3.zero
            
            hoverConnection = game:GetService("RunService").Heartbeat:Connect(function()
                if not isHoverboarding or not _LocalPlayer2.Character or not hoverboardVictim then return end
                
                local hrp = _LocalPlayer2.Character:FindFirstChild("HumanoidRootPart")
                local victimTorso = hoverboardVictim:FindFirstChild("UpperTorso")
                
                if hrp and victimTorso then
                    Align(30000, 800, "UpperTorso", "HumanoidRootPart", 
                          CFrame.new(0, 1.2, 0), 
                          CFrame.Angles(0, 0, 0))
                end
            end)
            
            local UserInputService = game:GetService("UserInputService")
            
            hoverMoveConnection = game:GetService("RunService").Heartbeat:Connect(function()
                if not isHoverboarding or not _LocalPlayer2.Character then return end
                
                local hrp = _LocalPlayer2.Character:FindFirstChild("HumanoidRootPart")
                if not hrp then return end
                
                local moveDirection = Vector3.new(0, 0, 0)
                local hasInput = false
                
                if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                    moveDirection = moveDirection + (hrp.CFrame.LookVector)
                    hasInput = true
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                    moveDirection = moveDirection - (hrp.CFrame.LookVector)
                    hasInput = true
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                    moveDirection = moveDirection - (hrp.CFrame.RightVector)
                    hasInput = true
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                    moveDirection = moveDirection + (hrp.CFrame.RightVector)
                    hasInput = true
                end
                
                if hasInput then
                    lastInputTime = tick()
                    if moveDirection.Magnitude > 0 then
                        moveDirection = moveDirection.Unit
                    end
                    currentVelocity = moveDirection * speed
                else
                    local timeSinceInput = tick() - lastInputTime
                    if timeSinceInput < 0.5 then
                        currentVelocity = currentVelocity * 0.85
                    else
                        currentVelocity = currentVelocity * 0.7
                    end
                end
                
                hrp.CFrame = hrp.CFrame + currentVelocity * (1/60)
            end)
            
            hoverReleaseConnection = _LocalPlayer2.Character.BodyEffects.Grabbed:GetPropertyChangedSignal("Value"):Connect(function()
                if _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil then
                    task.wait(0.5)
                    if isHoverboarding then
                        CleanupHoverboard()
                    end
                end
            end)
            
            local victimHumanoid = hoverboardVictim:FindFirstChild("Humanoid")
            if victimHumanoid then
                humanoidDiedConnection = victimHumanoid.Died:Connect(function()
                    if isHoverboarding then
                        CleanupHoverboard()
                    end
                end)
            end
            
            game:GetService('StarterGui'):SetCore('SendNotification', {
                Title = 'ðŸ›¹ Hoverboard',
                Text = 'Activado - WASD para moverte',
                Duration = 3
            })
        end
        
        -- SISTEMA HOVERBOARD INVERSE
        local isInverseActive = false
        local inverseVictim = nil
        local inverseMainTrack = nil
        local inverseVictimTrack = nil
        local inverseConnection = nil
        local inverseMoveConnection = nil
        local inverseReleaseConnection = nil
        local inverseHumanoidDiedConnection = nil

        function CleanupInverse()
            isInverseActive = false
            
            if inverseMainTrack then
                inverseMainTrack:Stop()
                inverseMainTrack = nil
            end
            if inverseVictimTrack then
                inverseVictimTrack:Stop()
                inverseVictimTrack = nil
            end
            if inverseConnection then
                inverseConnection:Disconnect()
                inverseConnection = nil
            end
            if inverseMoveConnection then
                inverseMoveConnection:Disconnect()
                inverseMoveConnection = nil
            end
            if inverseReleaseConnection then
                inverseReleaseConnection:Disconnect()
                inverseReleaseConnection = nil
            end
            if inverseHumanoidDiedConnection then
                inverseHumanoidDiedConnection:Disconnect()
                inverseHumanoidDiedConnection = nil
            end
            
            if inverseVictim and inverseVictim:FindFirstChild("UpperTorso") then
                Destroy("UpperTorso")
            end
            
            inverseVictim = nil
        end

        function StartInverse(victim)
            if isInverseActive then
                CleanupInverse()
                return
            end
            
            if not _LocalPlayer2.Character or not victim then return end
            
            isInverseActive = true
            inverseVictim = victim
            
            -- TÃš te acuestas con velocidad 0
            inverseMainTrack = AnimPlayvv2("77646644732733", 0, 1, 0)
            
            -- VÃCTIMA se sienta con una de las 2 animaciones
            local victimAnimId = math.random(1, 2) == 1 and "107864399491345" or "93120341268524"
            local victimHumanoid = victim:FindFirstChild("Humanoid")
            if victimHumanoid then
                local victimAnim = Instance.new("Animation")
                victimAnim.AnimationId = "rbxassetid://" .. victimAnimId
                inverseVictimTrack = victimHumanoid:LoadAnimation(victimAnim)
                inverseVictimTrack:Play(0, 1, 1.0)
                victimAnim:Destroy()
            end
            
            local speed = 40
            local lastInputTime = tick()
            local currentVelocity = Vector3.zero
            
            inverseConnection = game:GetService("RunService").Heartbeat:Connect(function()
                if not isInverseActive or not _LocalPlayer2.Character or not inverseVictim then return end
                
                local hrp = _LocalPlayer2.Character:FindFirstChild("HumanoidRootPart")
                local victimTorso = inverseVictim:FindFirstChild("UpperTorso")
                
                if hrp and victimTorso then
                    -- VÃ­ctima sentada en tu espalda
                    Align(30000, 800, "UpperTorso", "HumanoidRootPart", 
                          CFrame.new(0, -1.2, 0), 
                          CFrame.Angles(math.pi * 0.5, 0, 0))
                end
            end)
            
            local UserInputService = game:GetService("UserInputService")
            
            inverseMoveConnection = game:GetService("RunService").Heartbeat:Connect(function()
                if not isInverseActive or not _LocalPlayer2.Character then return end
                
                local hrp = _LocalPlayer2.Character:FindFirstChild("HumanoidRootPart")
                if not hrp then return end
                
                local moveDirection = Vector3.new(0, 0, 0)
                local hasInput = false
                
                if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                    moveDirection = moveDirection + (hrp.CFrame.LookVector)
                    hasInput = true
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                    moveDirection = moveDirection - (hrp.CFrame.LookVector)
                    hasInput = true
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                    moveDirection = moveDirection - (hrp.CFrame.RightVector)
                    hasInput = true
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                    moveDirection = moveDirection + (hrp.CFrame.RightVector)
                    hasInput = true
                end
                
                if hasInput then
                    lastInputTime = tick()
                    if moveDirection.Magnitude > 0 then
                        moveDirection = moveDirection.Unit
                    end
                    currentVelocity = moveDirection * speed
                else
                    local timeSinceInput = tick() - lastInputTime
                    if timeSinceInput < 0.5 then
                        currentVelocity = currentVelocity * 0.85
                    else
                        currentVelocity = currentVelocity * 0.7
                    end
                end
                
                hrp.CFrame = hrp.CFrame + currentVelocity * (1/60)
            end)
            
            inverseReleaseConnection = _LocalPlayer2.Character.BodyEffects.Grabbed:GetPropertyChangedSignal("Value"):Connect(function()
                if _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil then
                    task.wait(0.5)
                    if isInverseActive then
                        CleanupInverse()
                    end
                end
            end)
            
            local victimHumanoid = inverseVictim:FindFirstChild("Humanoid")
            if victimHumanoid then
                inverseHumanoidDiedConnection = victimHumanoid.Died:Connect(function()
                    if isInverseActive then
                        CleanupInverse()
                    end
                end)
            end
            
            game:GetService('StarterGui'):SetCore('SendNotification', {
                Title = 'ðŸ”„ Hoverboard Inverse',
                Text = 'Activado - VÃ­ctima en tu espalda',
                Duration = 3
            })
        end
        
        -- NUEVO TOOL KNIFE
        local isKnifeActive = false
        
        if _LocalPlayer2.Backpack:FindFirstChild('Knife') then
            _LocalPlayer2.Backpack:FindFirstChild('Knife'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('Knife') then
            _LocalPlayer2.Character:FindFirstChild('Knife'):Destroy()
        end

        wait(0.1)

        local knifeTool = Instance.new('Tool')
        knifeTool.RequiresHandle = false
        knifeTool.Name = 'Knife'
        knifeTool.ToolTip = 'Knife - EjecuciÃ³n brutal'
        knifeTool.Parent = _LocalPlayer2.Backpack
        
        knifeTool.Activated:Connect(function()
            if isKnifeActive then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'ðŸ”ª KNIFE',
                    Text = 'Ya hay una ejecuciÃ³n en progreso',
                    Duration = 2
                })
                return
            end
            
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then
                return
            end
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'ðŸ”ª KNIFE',
                    Text = 'Agarra a alguien primero',
                    Duration = 3
                })
                return
            end
            
            local victim = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
            local victimPlayer = game.Players:GetPlayerFromCharacter(victim)
            
            if not victimPlayer then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'ðŸ”ª KNIFE',
                    Text = 'Error: No se pudo identificar al jugador',
                    Duration = 2
                })
                return
            end
            
            isKnifeActive = true
            
            local knife = _LocalPlayer2.Backpack:FindFirstChild("[Knife]")
            if not knife then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'ðŸ”ª KNIFE',
                    Text = 'Necesitas el item [Knife] en tu inventario',
                    Duration = 3
                })
                isKnifeActive = false
                return
            end
            
            knife.Parent = _LocalPlayer2.Character
            
            game:GetService('StarterGui'):SetCore('SendNotification', {
                Title = 'ðŸ”ª EXECUTION INITIATED',
                Text = 'Preparando ejecuciÃ³n de ' .. victimPlayer.Name,
                Duration = 3
            })
            
            -- Guardar CanCollide original del torso y cabeza
            local originalCanCollide = {}
            if victim then
                for _, part in pairs(victim:GetDescendants()) do
                    if part:IsA("BasePart") and (part.Name == "UpperTorso" or part.Name == "LowerTorso" or part.Name == "Head" or part.Name == "HumanoidRootPart") then
                        originalCanCollide[part] = part.CanCollide
                        part.CanCollide = false
                    end
                end
            end
            
            -- ============================================
            -- FASE 1: ARRODILLAR VÃCTIMA
            -- ============================================
            
            local kneelingTrack = AnimPlayvv2("82213996490288", 0, 1, 0.3)
            
            local bodyPos, bodyGyro
            local alignConnection
            local stopAlign = false
            
            alignConnection = game:GetService("RunService").Heartbeat:Connect(function()
                if stopAlign then return end
                
                if _LocalPlayer2.Character and _LocalPlayer2.Character:FindFirstChild("HumanoidRootPart") and victim and victim:FindFirstChild("UpperTorso") then
                    Align(30000, 800, "UpperTorso", "HumanoidRootPart", 
                          CFrame.new(0, -2.5, -1.5), 
                          CFrame.Angles(math.pi * 0.5, 0, 0))
                end
            end)
            
            wait(1.5)
            
            if kneelingTrack then
                kneelingTrack:Stop()
            end
            
            stopAlign = true
            if alignConnection then alignConnection:Disconnect() end
            
            -- ============================================
            -- FASE 2: DEGOLLAR
            -- ============================================
            
            local cutTrack = AnimPlayvv2("76526969558807", 0, 1, 1.0)
            
            stopAlign = false
            alignConnection = game:GetService("RunService").Heartbeat:Connect(function()
                if stopAlign then return end
                
                if _LocalPlayer2.Character and _LocalPlayer2.Character:FindFirstChild("HumanoidRootPart") and victim and victim:FindFirstChild("UpperTorso") then
                    Align(30000, 800, "UpperTorso", "HumanoidRootPart", 
                          CFrame.new(0, 0.3, -1.2), 
                          CFrame.Angles(math.pi * -0.3, 0, 0))
                end
            end)
            
            if cutTrack then
                wait(1.5)
                cutTrack:Stop()
            end
            
            -- ============================================
            -- FASE 3: TIRAR TORSO (NO AL VACÃO)
            -- ============================================
            
            stopAlign = true
            if alignConnection then alignConnection:Disconnect() end
            
            if bodyPos then bodyPos:Destroy() end
            if bodyGyro then bodyGyro:Destroy() end
            
            -- SOLTAR VÃCTIMA
            game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Grabbing", false)
            
            -- Restaurar CanCollide del torso y cabeza
            if victim and victim.Parent then
                for _, part in pairs(victim:GetDescendants()) do
                    if part:IsA("BasePart") and (part.Name == "UpperTorso" or part.Name == "LowerTorso" or part.Name == "Head" or part.Name == "HumanoidRootPart") then
                        if originalCanCollide[part] ~= nil then
                            part.CanCollide = originalCanCollide[part]
                        end
                    end
                end
            end
            
            if knife and knife.Parent == _LocalPlayer2.Character then
                knife.Parent = _LocalPlayer2.Backpack
            end
            
            game:GetService('StarterGui'):SetCore('SendNotification', {
                Title = 'ðŸ’€ EXECUTION COMPLETE',
                Text = victimPlayer.Name .. ' desmembrado',
                Duration = 4
            })
            
            isKnifeActive = false
        end)
        
        if _LocalPlayer2.Backpack:FindFirstChild('Idk') then
            _LocalPlayer2.Backpack:FindFirstChild('Idk'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('Idk') then
            _LocalPlayer2.Character:FindFirstChild('Idk'):Destroy()
        end

        wait(0.1)

        local idkTool = Instance.new('Tool')
        idkTool.RequiresHandle = false
        idkTool.Name = 'Idk'
        idkTool.ToolTip = 'Idk - Efectos de velocidad'
        idkTool.Parent = _LocalPlayer2.Backpack

        local isIdkActive = false
        local idkMainTrack = nil

        idkTool.Activated:Connect(function()
            if isIdkActive then return end
            
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then
                return
            end
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Idk',
                    Text = 'No victim grabbed',
                    Duration = 2
                })
                return
            end
            
            isIdkActive = true
            
            -- CORREGIDO: AHORA SÃ RETORNA EL TRACK
            idkMainTrack = AnimPlayvv2("91859430011682", 3.0, 0, 0.3)
            
            local shouldStop = false
            local checkConnection
            local alignConnection
            local releaseConnection
            
            checkConnection = game:GetService('RunService').Heartbeat:Connect(function()
                if shouldStop or not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects or 
                   not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                    
                    -- DETENER ANIMACIÃ“N CORRECTAMENTE
                    if idkMainTrack then
                        idkMainTrack:Stop()
                        idkMainTrack = nil
                    end
                    
                    -- DESCONECTAR TODAS LAS CONEXIONES
                    if checkConnection then
                        checkConnection:Disconnect()
                        checkConnection = nil
                    end
                    
                    if alignConnection then
                        alignConnection:Disconnect()
                        alignConnection = nil
                    end
                    
                    if releaseConnection then
                        releaseConnection:Disconnect()
                        releaseConnection = nil
                    end
                    
                    -- LIMPIAR CONTROLES DE LA VÃCTIMA
                    if u19 and u19:FindFirstChild('UpperTorso') then
                        Destroy('UpperTorso')
                    end
                    
                    isIdkActive = false
                    
                    game:GetService('StarterGui'):SetCore('SendNotification', {
                        Title = 'Idk',
                        Text = 'Desactivado - vÃ­ctima liberada',
                        Duration = 2
                    })
                end
            end)
            
            alignConnection = game:GetService('RunService').Heartbeat:Connect(function()
                if isIdkActive and _LocalPlayer2.Character and _LocalPlayer2.Character.BodyEffects and 
                      _LocalPlayer2.Character.BodyEffects.Grabbed.Value and u19 then
                    
                    local timeOffset = tick() * 200
                    Align(30000, 800, 'UpperTorso', 'HumanoidRootPart', 
                          CFrame.new(0, -0.3, -1.2 + 0.15 * math.sin(timeOffset / 3)),
                          CFrame.Angles(math.pi * -0.3, 0, 0))
                end
            end)
            
            releaseConnection = _LocalPlayer2.Character.BodyEffects.Grabbed:GetPropertyChangedSignal("Value"):Connect(function()
                if _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil then
                    shouldStop = true
                end
            end)
            
            table.insert(u22, releaseConnection)
        end)
        
        if _LocalPlayer2.Backpack:FindFirstChild('hoverboard') then
            _LocalPlayer2.Backpack:FindFirstChild('hoverboard'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('hoverboard') then
            _LocalPlayer2.Character:FindFirstChild('hoverboard'):Destroy()
        end
        
        wait(0.1)
        
        local hoverboardTool = Instance.new('Tool')
        hoverboardTool.RequiresHandle = false
        hoverboardTool.Name = 'hoverboard'
        hoverboardTool.ToolTip = 'Hoverboard MÃ³vil - Ultra rÃ¡pido'
        hoverboardTool.Parent = _LocalPlayer2.Backpack
        
        hoverboardTool.Activated:Connect(function()
            if isHoverboarding then
                -- LIMPIAR CONTROLES DE LA VÃCTIMA
                if hoverboardVictim and hoverboardVictim:FindFirstChild("UpperTorso") then
                    local upperTorso = hoverboardVictim:FindFirstChild("UpperTorso")
                    if upperTorso:FindFirstChild("HoverboardPosition") then
                        upperTorso.HoverboardPosition:Destroy()
                    end
                    if upperTorso:FindFirstChild("HoverboardGyro") then
                        upperTorso.HoverboardGyro:Destroy()
                    end
                end
                
                CleanupHoverboard()
                
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'ðŸ›¹ Hoverboard',
                    Text = 'Desmontado',
                    Duration = 2
                })
                return
            end
            
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then
                return
            end
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'ðŸ›¹ Hoverboard',
                    Text = 'Agarra a alguien primero',
                    Duration = 3
                })
                return
            end
            
            local victim = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
            
            game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Grabbing", false)
            
            wait(0.3)
            
            StartHoverboard(victim)
        end)
        
        -- ========================================
        -- NUEVO TOOL: HOVERBOARD INVERSE
        -- ========================================
        
        if _LocalPlayer2.Backpack:FindFirstChild('hoverboard inverse') then
            _LocalPlayer2.Backpack:FindFirstChild('hoverboard inverse'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('hoverboard inverse') then
            _LocalPlayer2.Character:FindFirstChild('hoverboard inverse'):Destroy()
        end
        
        wait(0.1)
        
        local hoverboardInverseTool = Instance.new('Tool')
        hoverboardInverseTool.RequiresHandle = false
        hoverboardInverseTool.Name = 'hoverboard inverse'
        hoverboardInverseTool.ToolTip = 'Hoverboard Inverse - VÃ­ctima en tu espalda'
        hoverboardInverseTool.Parent = _LocalPlayer2.Backpack
        
        hoverboardInverseTool.Activated:Connect(function()
            if isInverseActive then
                -- LIMPIAR CONTROLES DE LA VÃCTIMA
                if inverseVictim and inverseVictim:FindFirstChild("UpperTorso") then
                    local upperTorso = inverseVictim:FindFirstChild("UpperTorso")
                    if upperTorso:FindFirstChild("InversePosition") then
                        upperTorso.InversePosition:Destroy()
                    end
                    if upperTorso:FindFirstChild("InverseGyro") then
                        upperTorso.InverseGyro:Destroy()
                    end
                end
                
                CleanupInverse()
                
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'ðŸ”„ Hoverboard Inverse',
                    Text = 'Desmontado',
                    Duration = 2
                })
                return
            end
            
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then
                return
            end
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'ðŸ”„ Hoverboard Inverse',
                    Text = 'Agarra a alguien primero',
                    Duration = 3
                })
                return
            end
            
            local victim = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
            
            game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Grabbing", false)
            
            wait(0.3)
            
            StartInverse(victim)
        end)
        
        -- ========================================
        -- TOOL DE ACTIVACIÃ“N PARA MÃ“VIL
        -- ========================================
        
        if _LocalPlayer2.Backpack:FindFirstChild('activate') then
            _LocalPlayer2.Backpack:FindFirstChild('activate'):Destroy()
        end
        if _LocalPlayer2.Character:FindFirstChild('activate') then
            _LocalPlayer2.Character:FindFirstChild('activate'):Destroy()
        end

        wait(0.1)

        local activateTool = Instance.new('Tool')
        activateTool.RequiresHandle = false
        activateTool.Name = 'activate'
        activateTool.ToolTip = 'Activar/Desactivar Hoverboards'
        activateTool.Parent = _LocalPlayer2.Backpack

        activateTool.Activated:Connect(function()
            -- Primero verificar si hay algo activo para desactivar
            if isHoverboarding then
                if hoverboardVictim and hoverboardVictim:FindFirstChild("UpperTorso") then
                    local upperTorso = hoverboardVictim:FindFirstChild("UpperTorso")
                    if upperTorso:FindFirstChild("HoverboardPosition") then
                        upperTorso.HoverboardPosition:Destroy()
                    end
                    if upperTorso:FindFirstChild("HoverboardGyro") then
                        upperTorso.HoverboardGyro:Destroy()
                    end
                end
                
                CleanupHoverboard()
                
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'ðŸ›¹ Hoverboard',
                    Text = 'Desmontado',
                    Duration = 2
                })
                return
            end
            
            if isInverseActive then
                if inverseVictim and inverseVictim:FindFirstChild("UpperTorso") then
                    local upperTorso = inverseVictim:FindFirstChild("UpperTorso")
                    if upperTorso:FindFirstChild("InversePosition") then
                        upperTorso.InversePosition:Destroy()
                    end
                    if upperTorso:FindFirstChild("InverseGyro") then
                        upperTorso.InverseGyro:Destroy()
                    end
                end
                
                CleanupInverse()
                
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'ðŸ”„ Hoverboard Inverse',
                    Text = 'Desmontado',
                    Duration = 2
                })
                return
            end
            
            -- Si no hay nada activo, verificar si hay vÃ­ctima agarrada
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then
                return
            end
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'âš¡ Activate',
                    Text = 'Agarra a alguien primero',
                    Duration = 3
                })
                return
            end
            
            local victim = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
            
            -- Verificar quÃ© tool estÃ¡ equipado para decidir cuÃ¡l activar
            local equippedTool = _LocalPlayer2.Character:FindFirstChildOfClass("Tool")
            
            if equippedTool and equippedTool.Name == "hoverboard inverse" then
                -- Activar hoverboard inverse
                game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Grabbing", false)
                wait(0.3)
                StartInverse(victim)
            elseif equippedTool and equippedTool.Name == "hoverboard" then
                -- Activar hoverboard normal
                game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Grabbing", false)
                wait(0.3)
                StartHoverboard(victim)
            else
                -- Si no hay tool equipado, activar hoverboard normal por defecto
                game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Grabbing", false)
                wait(0.3)
                StartHoverboard(victim)
            end
        end)
    end
        
    ToolSSs()
    game.Players.LocalPlayer.CharacterAdded:Connect(function()
        wait(2)
        ToolSSs()
    end)
else
    game:GetService('StarterGui'):SetCore('SendNotification', {
        Title = 'Your Not Buyer',
        Text = 'Please Buy Our Script Our Discord Server (Discord Server Set To Your CilpBoard)',
        Icon = 'rbxthumb://type=Asset&id=7257741890&w=150&h=150',
        Duration = 10,
    })
    setclipboard('https://discord.gg/mFnYMm7e')
end
