t.Activated:Connect(function()if LP2.Character.BodyEffects.Grabbed.Value then for _,tr in pairs(LP2.Character.Humanoid:GetPlayingAnimationTracks())do if tr.Animation.AnimationId=='rbxassetid://3135389157'then tr:Stop()end end local s=false spawn(function()repeat task.wait()Align(3000,400,'UpperTorso','HumanoidRootPart',CFrame.new(0,0,2)*CFrame.Angles(0,math.pi,0),CFrame.Angles(0,math.pi,0))until s end)AnimPlay(126796684932532,0.8)task.wait(5)local vn=tostring(LP2.Character.BodyEffects.Grabbed.Value)if game.Players:FindFirstChild(vn)and game.Players[vn].Character then local c=game.Players[vn].Character for _,limb in ipairs({'RightUpperArm','LeftUpperArm','LeftUpperLeg','RightUpperLeg'})do AnimPlay(126796684932532,1.5)task.wait(1.5)if c:FindFirstChild(limb)then local m=c[limb]:FindFirstChildOfClass('Motor6D')if m then m:Destroy()end c[limb].Velocity=Vector3.new(0,-100,0)game.Debris:AddItem(c[limb],3)end end end s=true task.wait(0.3)RS.MainEvent:FireServer('Grabbing')Destroy('UpperTorso')game:GetService('StarterGui'):SetCore('SendNotification',{Title='EJECUCIÓN COMPLETA',Text='💀',Duration=3})end end)

print("✅ TODOS LOS GRABS CARGADOS - K2 ORDER + ULTRA EXTRAS")
end

