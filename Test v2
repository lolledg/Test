    function Align(p137, p138, p139, p140, p141, p142)
        if _LocalPlayer2.Character.BodyEffects.Grabbed.Value ~= nil then
            if not (u19[p139]:FindFirstChildWhichIsA('BodyPosition') or u19[p139]:FindFirstChildWhichIsA('BodyGyro')) then
                Instance.new('BodyPosition', u19[p139]).MaxForce = Vector3.new(math.huge, math.huge, math.huge)

                local _BodyGyro = Instance.new('BodyGyro', u19[p139])

                _BodyGyro.maxTorque = Vector3.new(9000000000, 9000000000, 9000000000)
                _BodyGyro.P = 10000
            end

            u19[p139].BodyGyro.CFrame = _LocalPlayer2.Character[p140].CFrame * p142
            u19[p139].BodyPosition.Position = _LocalPlayer2.Character[p140].CFrame * p141.Position
            u19[p139].BodyPosition.P = p137
            u19[p139].BodyPosition.D = p138
        end
    end
    function Destroy(p144)
        if u19[p144]:FindFirstChildOfClass('BodyGyro') or u19[p144]:FindFirstChildOfClass('BodyPosition') then
            u19[p144]:FindFirstChildOfClass('BodyGyro'):Destroy()
            u19[p144]:FindFirstChildOfClass('BodyPosition'):Destroy()
        end
    end
    function Holding()
        repeat
            task.wait()
            Align(10000, 175, 'UpperTorso', 'RightUpperArm', CFrame.new(-0.2, -1.7, 0.5), CFrame.Angles(math.pi * -0.5, math.pi * 1, math.pi * 0.07))
        until _LocalPlayer2.Character.BodyEffects.Grabbed.Value == nil or u20 == true
    end
    function Grab()
        if _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
            u19 = _LocalPlayer2.Character.BodyEffects.Grabbed.Value

            local _GRABBING_CONSTRAINT = u19:WaitForChild('GRABBING_CONSTRAINT')

            if not _GRABBING_CONSTRAINT then
                return
            end

            u23 = _GRABBING_CONSTRAINT:FindFirstChildOfClass('RopeConstraint')
            u23.Length = 1 / 0
            u20 = false

            local v146 = u19
            local v147, v148, v149 = pairs(v146:GetChildren())

            while true do
                local v150

                v149, v150 = v147(v148, v149)

                if v149 == nil then
                    break
                end
                if v150:IsA('BasePart') then
                    v150.CanCollide = false
                end
            end
            while true do
                task.wait()

                local v151, v152, v153 = pairs(_LocalPlayer2.Character.Humanoid:GetPlayingAnimationTracks())

                while true do
                    local v154

                    v153, v154 = v151(v152, v153)

                    if v153 == nil then
                        break
                    end
                    if v154.Animation.AnimationId:match('rbxassetid://11075367458') then
                        u20 = true

                        v154:Stop()
                    end
                end

                if u20 == true then
                    local v155 = u19
                    local v156, v157, v158 = pairs(v155:GetDescendants())

                    while true do
                        local v159

                        v158, v159 = v156(v157, v158)

                        if v158 == nil then
                            break
                        end
                        if v159:IsA('BasePart') then
                            v159.Velocity = Vector3.zero
                            v159.AssemblyAngularVelocity = Vector3.zero
                            v159.AssemblyLinearVelocity = Vector3.zero
                        end
                    end

                    u20 = false

                    AnimPlayvv2(3135389157, 0, 0.1, 0.3)
                    Holding()
                    AnimStopv2(3135389157)
                    Destroy('UpperTorso')
                end
            end
        else
            return
        end
    end
    
    local function PlaySound(soundId)
        if not _LocalPlayer2.Character then return false end
        if _LocalPlayer2.Character:FindFirstChild('BOOMBOX') then
            _ReplicatedStorage.MainEvent:FireServer('Boombox', soundId)
        else
            local sound = Instance.new('Sound')
            sound.SoundId = 'rbxassetid://' .. soundId
            sound.Volume = 1
            sound.Parent = workspace
            sound:Play()
            game.Debris:AddItem(sound, 6)
        end
        return true
    end
    
    -- ========================================
    -- SISTEMA HOVERBOARD ULTRA R√ÅPIDO - VERSI√ìN MEJORADA
    -- ========================================
    local isHoverboarding = false
    local hoverboardVictim = nil
    local hoverboardConnection = nil
    local hoverboardGui = nil
    local cleanupConnection = nil
    local animSpeedConnection = nil
    local currentAnimSpeed = 0.5
    local hoverTrack = nil
    
    local function CreateHoverboardGui()
        if hoverboardGui then
            hoverboardGui:Destroy()
        end
        
        hoverboardGui = Instance.new("ScreenGui")
        hoverboardGui.Name = "HoverboardGui"
        hoverboardGui.ResetOnSpawn = false
        hoverboardGui.Parent = game.CoreGui
        
        local mainFrame = Instance.new("Frame")
        mainFrame.Size = UDim2.new(0, 220, 0, 110)
        mainFrame.Position = UDim2.new(0.5, -110, 0.1, 0)
        mainFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
        mainFrame.BackgroundTransparency = 0.3
        mainFrame.BorderSizePixel = 0
        mainFrame.Parent = hoverboardGui
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 10)
        corner.Parent = mainFrame
        
        local title = Instance.new("TextLabel")
        title.Size = UDim2.new(1, 0, 0, 35)
        title.Position = UDim2.new(0, 0, 0, 0)
        title.BackgroundTransparency = 1
        title.Text = "üõπ HOVERBOARD ULTRA"
        title.TextColor3 = Color3.fromRGB(255, 255, 255)
        title.TextSize = 18
        title.Font = Enum.Font.GothamBold
        title.Parent = mainFrame
        
        local speedLabel = Instance.new("TextLabel")
        speedLabel.Name = "SpeedLabel"
        speedLabel.Size = UDim2.new(1, 0, 0, 25)
        speedLabel.Position = UDim2.new(0, 0, 0.35, 0)
        speedLabel.BackgroundTransparency = 1
        speedLabel.Text = "‚ö° Velocidad: Normal"
        speedLabel.TextColor3 = Color3.fromRGB(0, 255, 150)
        speedLabel.TextSize = 14
        speedLabel.Font = Enum.Font.GothamSemibold
        speedLabel.Parent = mainFrame
        
        local animSpeedLabel = Instance.new("TextLabel")
        animSpeedLabel.Name = "AnimSpeedLabel"
        animSpeedLabel.Size = UDim2.new(1, 0, 0, 20)
        animSpeedLabel.Position = UDim2.new(0, 0, 0.55, 0)
        animSpeedLabel.BackgroundTransparency = 1
        animSpeedLabel.Text = "üé¨ Anim: 0.5x"
        animSpeedLabel.TextColor3 = Color3.fromRGB(255, 200, 0)
        animSpeedLabel.TextSize = 12
        animSpeedLabel.Font = Enum.Font.Gotham
        animSpeedLabel.Parent = mainFrame
        
        local controlsText = Instance.new("TextLabel")
        controlsText.Size = UDim2.new(1, 0, 0, 30)
        controlsText.Position = UDim2.new(0, 0, 0.7, 0)
        controlsText.BackgroundTransparency = 1
        controlsText.Text = "Mira hacia donde quieres ir"
        controlsText.TextColor3 = Color3.fromRGB(180, 180, 200)
        controlsText.TextSize = 11
        controlsText.Font = Enum.Font.Gotham
        controlsText.Parent = mainFrame
        
        local buttonFrame = Instance.new("Frame")
        buttonFrame.Name = "ButtonFrame"
        buttonFrame.Size = UDim2.new(0, 200, 0, 200)
        buttonFrame.Position = UDim2.new(0.5, -100, 1, -220)
        buttonFrame.BackgroundTransparency = 1
        buttonFrame.Parent = hoverboardGui
        
        local turboButton = Instance.new("TextButton")
        turboButton.Name = "TurboButton"
        turboButton.Size = UDim2.new(0, 90, 0, 90)
        turboButton.Position = UDim2.new(0.5, -45, 0.5, -45)
        turboButton.BackgroundColor3 = Color3.fromRGB(255, 100, 50)
        turboButton.Text = "üî•\nTURBO"
        turboButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        turboButton.TextSize = 18
        turboButton.Font = Enum.Font.GothamBold
        turboButton.Parent = buttonFrame
        
        local turboCorner = Instance.new("UICorner")
        turboCorner.CornerRadius = UDim.new(1, 0)
        turboCorner.Parent = turboButton
        
        local upButton = Instance.new("TextButton")
        upButton.Name = "UpButton"
        upButton.Size = UDim2.new(0, 60, 0, 60)
        upButton.Position = UDim2.new(0.5, -30, 0, 0)
        upButton.BackgroundColor3 = Color3.fromRGB(50, 150, 255)
        upButton.Text = "‚¨ÜÔ∏è"
        upButton.TextSize = 24
        upButton.Parent = buttonFrame
        
        local upCorner = Instance.new("UICorner")
        upCorner.CornerRadius = UDim.new(1, 0)
        upCorner.Parent = upButton
        
        local downButton = Instance.new("TextButton")
        downButton.Name = "DownButton"
        downButton.Size = UDim2.new(0, 60, 0, 60)
        downButton.Position = UDim2.new(0.5, -30, 1, -60)
        downButton.BackgroundColor3 = Color3.fromRGB(50, 150, 255)
        downButton.Text = "‚¨áÔ∏è"
        downButton.TextSize = 24
        downButton.Parent = buttonFrame
        
        local downCorner = Instance.new("UICorner")
        downCorner.CornerRadius = UDim.new(1, 0)
        downCorner.Parent = downButton
        
        return hoverboardGui
    end
    
    local function UpdateSpeedIndicator(isTurbo, animSpeed)
        if hoverboardGui then
            local mainFrame = hoverboardGui:FindFirstChild("Frame")
            if mainFrame then
                local speedLabel = mainFrame:FindFirstChild("SpeedLabel")
                if speedLabel then
                    if isTurbo then
                        speedLabel.Text = "üî• TURBO M√ÅXIMO"
                        speedLabel.TextColor3 = Color3.fromRGB(255, 80, 80)
                    else
                        speedLabel.Text = "‚ö° Velocidad: Normal"
                        speedLabel.TextColor3 = Color3.fromRGB(0, 255, 150)
                    end
                end
                
                local animSpeedLabel = mainFrame:FindFirstChild("AnimSpeedLabel")
                if animSpeedLabel then
                    animSpeedLabel.Text = string.format("üé¨ Anim: %.2fx", animSpeed)
                end
            end
        end
    end
    
    local function CleanupHoverboard()
        isHoverboarding = false
        
        -- DETENER ANIMACI√ìN
        if hoverTrack then
            hoverTrack:Stop()
            hoverTrack = nil
        end
        
        if hoverboardConnection then
            hoverboardConnection:Disconnect()
            hoverboardConnection = nil
        end
        
        if cleanupConnection then
            cleanupConnection:Disconnect()
            cleanupConnection = nil
        end
        
        if animSpeedConnection then
            animSpeedConnection:Disconnect()
            animSpeedConnection = nil
        end
        
        if hoverboardGui then
            hoverboardGui:Destroy()
            hoverboardGui = nil
        end
        
        currentAnimSpeed = 0.5
        hoverboardVictim = nil
    end
    
    local function StartHoverboard(victim)
        if isHoverboarding or not victim then return end
        
        if not _LocalPlayer2.Character or not _LocalPlayer2.Character:FindFirstChild("HumanoidRootPart") then
            return
        end
        
        isHoverboarding = true
        hoverboardVictim = victim
        currentAnimSpeed = 0.5
        
        CreateHoverboardGui()
        
        local upperTorso = victim:FindFirstChild("UpperTorso")
        local victimHRP = victim:FindFirstChild("HumanoidRootPart")
        
        if not upperTorso or not victimHRP then
            CleanupHoverboard()
            return
        end
        
        -- CONTROLES DE F√çSICA M√ÅS FUERTES
        local bodyPos = upperTorso:FindFirstChild("HoverboardPosition") or Instance.new("BodyPosition")
        bodyPos.Name = "HoverboardPosition"
        bodyPos.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
        bodyPos.P = 50000
        bodyPos.D = 5000
        bodyPos.Parent = upperTorso
        
        local bodyGyro = upperTorso:FindFirstChild("HoverboardGyro") or Instance.new("BodyGyro")
        bodyGyro.Name = "HoverboardGyro"
        bodyGyro.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
        bodyGyro.P = 60000
        bodyGyro.D = 3000
        bodyGyro.Parent = upperTorso
        
        local startCFrame = _LocalPlayer2.Character.HumanoidRootPart.CFrame
        bodyPos.Position = startCFrame.Position + Vector3.new(0, -2, 0)
        bodyGyro.CFrame = startCFrame * CFrame.Angles(math.rad(180), 0, 0)
        
        _LocalPlayer2.Character.HumanoidRootPart.CFrame = CFrame.new(
            upperTorso.Position.X,
            upperTorso.Position.Y + 3,
            upperTorso.Position.Z
        )
        
        -- ANIMACI√ìN NUEVA
        local hoverAnim = Instance.new("Animation")
        hoverAnim.AnimationId = "rbxassetid://76573949416065"
        hoverTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(hoverAnim)
        hoverTrack:Play()
        hoverTrack:AdjustSpeed(currentAnimSpeed)
        
        -- VELOCIDADES ULTRA R√ÅPIDAS
        local normalSpeed = 400
        local turboSpeed = 800
        local flySpeed = 200
        local smoothFactor = 0.25
        
        local isTurboPressed = false
        local isUpPressed = false
        local isDownPressed = false
        
        local buttonFrame = hoverboardGui:FindFirstChild("ButtonFrame")
        if buttonFrame then
            local turboButton = buttonFrame:FindFirstChild("TurboButton")
            if turboButton then
                turboButton.MouseButton1Down:Connect(function()
                    isTurboPressed = true
                    turboButton.BackgroundColor3 = Color3.fromRGB(255, 50, 0)
                end)
                turboButton.MouseButton1Up:Connect(function()
                    isTurboPressed = false
                    turboButton.BackgroundColor3 = Color3.fromRGB(255, 100, 50)
                end)
            end
            
            local upButton = buttonFrame:FindFirstChild("UpButton")
            if upButton then
                upButton.MouseButton1Down:Connect(function()
                    isUpPressed = true
                    upButton.BackgroundColor3 = Color3.fromRGB(30, 100, 200)
                end)
                upButton.MouseButton1Up:Connect(function()
                    isUpPressed = false
                    upButton.BackgroundColor3 = Color3.fromRGB(50, 150, 255)
                end)
            end
            
            local downButton = buttonFrame:FindFirstChild("DownButton")
            if downButton then
                downButton.MouseButton1Down:Connect(function()
                    isDownPressed = true
                    downButton.BackgroundColor3 = Color3.fromRGB(30, 100, 200)
                end)
                downButton.MouseButton1Up:Connect(function()
                    isDownPressed = false
                    downButton.BackgroundColor3 = Color3.fromRGB(50, 150, 255)
                end)
            end
        end
        
        -- SISTEMA DE VELOCIDAD PROGRESIVA DE ANIMACI√ìN
        local timeMoving = 0
        animSpeedConnection = game:GetService("RunService").Heartbeat:Connect(function(dt)
            if not isHoverboarding then return end
            
            local camera = workspace.CurrentCamera
            local cameraCFrame = camera.CFrame
            local moveDirection = cameraCFrame.LookVector
            
            -- Si se est√° moviendo, aumentar velocidad de animaci√≥n
            if moveDirection.Magnitude > 0.1 then
                timeMoving = timeMoving + dt
                -- Aumentar velocidad progresivamente hasta 1.5
                currentAnimSpeed = math.min(0.5 + (timeMoving * 0.3), 1.5)
            else
                timeMoving = math.max(0, timeMoving - dt * 2)
                currentAnimSpeed = math.max(0.5, 0.5 + (timeMoving * 0.3))
            end
            
            if hoverTrack then
                hoverTrack:AdjustSpeed(currentAnimSpeed)
            end
        end)
        
        hoverboardConnection = game:GetService("RunService").RenderStepped:Connect(function(delta)
            -- VERIFICAR SI V√çCTIMA EXISTE
            if not isHoverboarding or not victim or not victim.Parent then
                CleanupHoverboard()
                
                -- LIMPIAR CONTROLES DE LA V√çCTIMA
                if upperTorso:FindFirstChild("HoverboardPosition") then
                    upperTorso.HoverboardPosition:Destroy()
                end
                if upperTorso:FindFirstChild("HoverboardGyro") then
                    upperTorso.HoverboardGyro:Destroy()
                end
                
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'üõπ Hoverboard',
                    Text = 'V√≠ctima perdida - desactivado',
                    Duration = 2
                })
                return
            end
            
            -- VERIFICAR RAGDOLL
            local victimHumanoid = victim:FindFirstChild("Humanoid")
            if victimHumanoid then
                local bodyEffects = victim:FindFirstChild("BodyEffects")
                if bodyEffects then
                    local ragdoll = bodyEffects:FindFirstChild("Ragdoll")
                    if ragdoll and not ragdoll.Value then
                        CleanupHoverboard()
                        
                        if upperTorso:FindFirstChild("HoverboardPosition") then
                            upperTorso.HoverboardPosition:Destroy()
                        end
                        if upperTorso:FindFirstChild("HoverboardGyro") then
                            upperTorso.HoverboardGyro:Destroy()
                        end
                        
                        game:GetService('StarterGui'):SetCore('SendNotification', {
                            Title = 'üõπ Hoverboard',
                            Text = 'V√≠ctima se liber√≥',
                            Duration = 3
                        })
                        return
                    end
                end
            end
            
            local userInputService = game:GetService("UserInputService")
            local playerHRP = _LocalPlayer2.Character.HumanoidRootPart
            
            local isTurboActive = userInputService:IsKeyDown(Enum.KeyCode.LeftControl) or isTurboPressed
            UpdateSpeedIndicator(isTurboActive, currentAnimSpeed)
            
            -- MOVIMIENTO BASADO EN C√ÅMARA (SHIFTLOCK STYLE)
            local camera = workspace.CurrentCamera
            local cameraCFrame = camera.CFrame
            
            -- SIEMPRE AVANZAR HACIA DONDE MIRA LA C√ÅMARA
            local moveDirection = cameraCFrame.LookVector
            
            local verticalMove = 0
            if userInputService:IsKeyDown(Enum.KeyCode.Space) or isUpPressed then
                verticalMove = 1
            elseif userInputService:IsKeyDown(Enum.KeyCode.LeftShift) or isDownPressed then
                verticalMove = -1
            end
            
            local currentSpeed = isTurboActive and turboSpeed or normalSpeed
            if verticalMove ~= 0 then
                currentSpeed = flySpeed
            end
            
            -- MOVIMIENTO ULTRA FLUIDO Y R√ÅPIDO
            if moveDirection.Magnitude > 0 then
                moveDirection = (moveDirection.Unit * Vector3.new(1, 0, 1)) + Vector3.new(0, verticalMove, 0)
                
                if moveDirection.Magnitude > 0 then
                    moveDirection = moveDirection.Unit
                end
                
                local targetPos = bodyPos.Position + (moveDirection * currentSpeed * delta)
                bodyPos.Position = bodyPos.Position:Lerp(targetPos, smoothFactor)
                
                -- ROTAR V√çCTIMA HACIA ADELANTE (BOCA ABAJO MIRANDO ADELANTE)
                local lookDirection = Vector3.new(moveDirection.X, 0, moveDirection.Z)
                if lookDirection.Magnitude > 0 then
                    local targetCFrame = CFrame.lookAt(upperTorso.Position, upperTorso.Position + lookDirection)
                    -- CORREGIDO: V√≠ctima mira ADELANTE boca abajo
                    bodyGyro.CFrame = targetCFrame * CFrame.Angles(math.rad(90), 0, 0)
                end
            else
                bodyPos.Position = bodyPos.Position + Vector3.new(0, verticalMove * flySpeed * delta, 0)
            end
            
            -- MANTENER JUGADOR ENCIMA MIRANDO ADELANTE
            local offsetY = 3
            local lookDirection = Vector3.new(cameraCFrame.LookVector.X, 0, cameraCFrame.LookVector.Z)
            if lookDirection.Magnitude > 0 then
                -- JUGADOR MIRA HACIA ADELANTE (DIRECCI√ìN DE LA C√ÅMARA)
                playerHRP.CFrame = CFrame.new(
                    upperTorso.Position.X,
                    upperTorso.Position.Y + offsetY,
                    upperTorso.Position.Z
                ) * CFrame.lookAt(Vector3.zero, lookDirection)
            else
                playerHRP.CFrame = CFrame.new(
                    upperTorso.Position.X,
                    upperTorso.Position.Y + offsetY,
                    upperTorso.Position.Z
                )
            end
            
            playerHRP.AssemblyLinearVelocity = Vector3.zero
            playerHRP.AssemblyAngularVelocity = Vector3.zero
            
            -- ALTURA M√çNIMA
            if verticalMove == 0 then
                local ray = Ray.new(upperTorso.Position, Vector3.new(0, -20, 0))
                local hit, position = workspace:FindPartOnRayWithIgnoreList(ray, {victim, _LocalPlayer2.Character})
                
                if hit then
                    local minHeight = position.Y + 1.5
                    if bodyPos.Position.Y < minHeight then
                        bodyPos.Position = Vector3.new(bodyPos.Position.X, minHeight, bodyPos.Position.Z)
                    end
                end
            end
        end)
        
        cleanupConnection = game:GetService("RunService").Heartbeat:Connect(function()
            if not isHoverboarding or not victim or not victim.Parent then
                CleanupHoverboard()
                
                if upperTorso:FindFirstChild("HoverboardPosition") then
                    upperTorso.HoverboardPosition:Destroy()
                end
                if upperTorso:FindFirstChild("HoverboardGyro") then
                    upperTorso.HoverboardGyro:Destroy()
                end
            end
        end)
        
        game:GetService('StarterGui'):SetCore('SendNotification', {
            Title = 'üõπ HOVERBOARD ULTRA',
            Text = 'Mira hacia donde quieres ir | Turbo para m√°xima velocidad',
            Duration = 5
        })
    end

    function ToolSSs()
        -- ========================================
        -- TOOL: Activate (NECESARIO PARA EL FUNCIONAMIENTO)
        -- ========================================
        
        if _LocalPlayer2.Backpack:FindFirstChild('Activate') then
            _LocalPlayer2.Backpack:FindFirstChild('Activate'):Destroy()
        end

        task.wait(0.1)

        local _Tool = Instance.new('Tool')
        _Tool.RequiresHandle = false
        _Tool.Name = 'Activate'
        _Tool.Parent = _LocalPlayer2.Backpack

        _Tool.Activated:Connect(function()
            game:GetService('ReplicatedStorage').MainEvent:FireServer('Grabbing', true)
            wait(0.3)
            Grab()
        end)

        -- ========================================
        -- TOOL: GRAB KNIFE (REEMPLAZADO - Versi√≥n mejorada del archivo "Test v2 (1).txt")
        -- ========================================
        
        if _LocalPlayer2.Backpack:FindFirstChild('grab knife') then
            _LocalPlayer2.Backpack:FindFirstChild('grab knife'):Destroy()
        end
        
        wait(0.1)
        
        local grabKnifeTool = Instance.new('Tool')
        grabKnifeTool.RequiresHandle = false
        grabKnifeTool.Name = 'grab knife'
        grabKnifeTool.ToolTip = 'Grab Knife - Remake con corte de boca'
        grabKnifeTool.Parent = _LocalPlayer2.Backpack
        
        local isKnifeActive = false
        
        grabKnifeTool.Activated:Connect(function()
            if isKnifeActive then return end
            if not _LocalPlayer2.Character or not _LocalPlayer2.Character.BodyEffects then
                return
            end
            
            if not _LocalPlayer2.Character.BodyEffects.Grabbed.Value then
                game:GetService('StarterGui'):SetCore('SendNotification', {
                    Title = 'Grab Knife',
                    Text = 'No victim grabbed',
                    Duration = 2
                })
                return
            end
            
            isKnifeActive = true
            
            local victim = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
            local victimPlayer = game.Players:GetPlayerFromCharacter(victim)
            
            if not victimPlayer or victimPlayer == _LocalPlayer2 then
                isKnifeActive = false
                return
            end
            
            -- EQUIPAR CUCHILLO
            local knife = _LocalPlayer2.Backpack:FindFirstChild('Knife') or _LocalPlayer2.Backpack:FindFirstChild('knife')
            if knife then
                knife.Parent = _LocalPlayer2.Character
            end
            
            -- ============================================
            -- FASE 1: AGARRAR POR LA ESPALDA Y CORTAR BOCA
            -- ============================================
            
            game:GetService('StarterGui'):SetCore('SendNotification', {
                Title = 'üî™ Grab Knife',
                Text = 'Fase 1: Corte de boca',
                Duration = 3
            })
            
            -- POSICIONAR V√çCTIMA POR LA ESPALDA (M√ÅS ABAJO) - CORREGIDO
            local stopAlign = false
            local alignConnection
            
            local upperTorso = victim:FindFirstChild("UpperTorso")
            if not upperTorso then
                isKnifeActive = false
                return
            end
            
            -- HACER NOCLIP PARA QUE NO SE BUGEE
            local originalCanCollide = {}
            for _, part in pairs(victim:GetDescendants()) do
                if part:IsA("BasePart") then
                    originalCanCollide[part] = part.CanCollide
                    part.CanCollide = false
                end
            end
            
            -- CORREGIDO: P y D m√°s bajos para evitar que la v√≠ctima salga volando
            local bodyPos = upperTorso:FindFirstChild("GrabKnifePosition") or Instance.new("BodyPosition")
            bodyPos.Name = "GrabKnifePosition"
            bodyPos.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
            bodyPos.P = 3000  -- REDUCIDO de 8000
            bodyPos.D = 400   -- REDUCIDO de 800
            bodyPos.Parent = upperTorso
            
            local bodyGyro = upperTorso:FindFirstChild("GrabKnifeGyro") or Instance.new("BodyGyro")
            bodyGyro.Name = "GrabKnifeGyro"
            bodyGyro.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
            bodyGyro.P = 5000  -- REDUCIDO de 10000
            bodyGyro.Parent = upperTorso
            
            alignConnection = game:GetService("RunService").Heartbeat:Connect(function()
                if stopAlign or not victim or not victim.Parent then
                    if alignConnection then alignConnection:Disconnect() end
                    return
                end
                
                local playerCF = _LocalPlayer2.Character.HumanoidRootPart.CFrame
                -- POSICI√ìN: Por la espalda y M√ÅS ABAJO
                local backPos = playerCF.Position - (playerCF.LookVector * 1) + Vector3.new(0, -1.5, 0)
                
                bodyPos.Position = backPos
                bodyGyro.CFrame = CFrame.new(backPos) * CFrame.Angles(0, math.pi, 0)
            end)
            
            -- ANIMACI√ìN DE CORTE DE BOCA
            local mouthCutAnim = Instance.new('Animation')
            mouthCutAnim.AnimationId = 'rbxassetid://126796684932532'
            local mouthCutTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(mouthCutAnim)
            mouthCutTrack:Play()
            mouthCutTrack:AdjustSpeed(0.8)
            
            -- SONIDO DE CORTE
            if _LocalPlayer2.Character:FindFirstChild('BOOMBOX') then
                _ReplicatedStorage.MainEvent:FireServer('Boombox', '125856818464345')
            else
                local cutSound = Instance.new("Sound")
                cutSound.SoundId = "rbxassetid://125856818464345"
                cutSound.Volume = 0.8
                cutSound.Parent = workspace
                cutSound:Play()
                game.Debris:AddItem(cutSound, 3)
            end
            
            -- ESPERAR 5 SEGUNDOS (CORTE DE BOCA)
            wait(5)
            
            mouthCutTrack:Stop()
            
            -- ============================================
            -- FASE 2: CORTAR BRAZOS Y PIERNAS
            -- ============================================
            
            game:GetService('StarterGui'):SetCore('SendNotification', {
                Title = 'üî™ Grab Knife',
                Text = 'Fase 2: Cortando extremidades',
                Duration = 3
            })
            
            local limbsToSever = {
                'RightUpperArm',
                'LeftUpperArm', 
                'LeftUpperLeg',
                'RightUpperLeg'
            }
            
            for i, limbName in ipairs(limbsToSever) do
                local soundId = '125856818464345'
                
                if _LocalPlayer2.Character:FindFirstChild('BOOMBOX') then
                    _ReplicatedStorage.MainEvent:FireServer('Boombox', soundId)
                else
                    local cutSound = Instance.new("Sound")
                    cutSound.SoundId = "rbxassetid://" .. soundId
                    cutSound.Volume = 0.8
                    cutSound.Parent = workspace
                    cutSound:Play()
                    game.Debris:AddItem(cutSound, 3)
                end
                
                local cutTrack = _LocalPlayer2.Character.Humanoid:LoadAnimation(mouthCutAnim)
                cutTrack:Play()
                cutTrack:AdjustSpeed(1.5)
                
                if victim and victim.Parent then
                    local part = victim:FindFirstChild(limbName)
                    if part then 
                        wait(0.2)
                        
                        -- HACER NOCLIP Y CAER AL VAC√çO
                        part.CanCollide = false
                        part.Massless = true
                        
                        -- Separar del cuerpo
                        for _, motor in pairs(part:GetDescendants()) do
                            if motor:IsA("Motor6D") then
                                motor:Destroy()
                            end
                        end
                        
                        -- Aplicar velocidad hacia abajo para que caiga al vac√≠o
                        part.Velocity = Vector3.new(0, -100, 0)
                        
                        -- Destruir despu√©s de 3 segundos (ya estar√° en el vac√≠o)
                        game.Debris:AddItem(part, 3)
                    end
                end
                
                wait(1.5)
                cutTrack:Stop()
            end
            
            -- ============================================
            -- FASE 3: TIRAR TORSO (NO AL VAC√çO)
            -- ============================================
            
            stopAlign = true
            if alignConnection then alignConnection:Disconnect() end
            
            if bodyPos then bodyPos:Destroy() end
            if bodyGyro then bodyGyro:Destroy() end
            
            -- SOLTAR V√çCTIMA
            game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Grabbing", false)
            
