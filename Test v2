                    end
                end
            end

            task.wait(0.3)
            game:GetService('ReplicatedStorage').MainEvent:FireServer('Grabbing')
            Destroy('UpperTorso')

            u516 = true
        end
    end)

    if _LocalPlayer6.Backpack:FindFirstChild('Control') then
        _LocalPlayer6.Backpack:FindFirstChild('Control'):Destroy()
    end

    task.wait(0.1)

    local _Tool24 = Instance.new('Tool', _LocalPlayer6.Backpack)

    _Tool24.RequiresHandle = false
    _Tool24.Name = 'Control'

    _Tool24.Activated:connect(function()
