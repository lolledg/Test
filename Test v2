                    Title = '游띷 Hoverboard',
                    Text = 'Agarra a alguien primero',
                    Duration = 3
                })
                return
            end
            
            local victim = _LocalPlayer2.Character.BodyEffects.Grabbed.Value
            
            game:GetService("ReplicatedStorage"):WaitForChild("MainEvent"):FireServer("Grabbing", false)
            
            wait(0.3)
            
            StartHoverboard(victim)
        end)
        
        -- ========================================
    if _LocalPlayer6.Backpack:FindFirstChild('Punch') then
        _LocalPlayer6.Backpack:FindFirstChild('Punch'):Destroy()
    end

