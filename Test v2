            local u88, u89 = nil
            local u90 = tick()

            u88 = u37('Heartbeat', function()
                if tick() - u90 > 5 then
                    u88:Disconnect()
                    u89:Disconnect()
                end

                Align(30000, 800, 'UpperTorso', 'HumanoidRootPart', CFrame.new(0, -3, 0), CFrame.Angles(math.pi * 0.5, 0, 0))
            end)
            u89 = u37('RenderStepped', function()
                if _LocalPlayer2.Character.Humanoid.Sit == true and _LocalPlayer2.Character:WaitForChild('Humanoid').FloorMaterial == Enum.Material.Air then
                    _LocalPlayer2.Character.HumanoidRootPart.CFrame = _LocalPlayer2.Character.HumanoidRootPart.CFrame * CFrame.Angles(0, math.rad(50), 0)
