local function sc(cf)local c=clouds:Clone()c.Name='PortalClouds'c.Parent=workspace c.CFrame,c.Anchored=cf,true return c end
t.Activated:Connect(function()if LP2.Character.BodyEffects.Grabbed.Value then task.wait(1)LP2.Character.Portal.Parent=LP2.Backpack stopGrab=true task.wait()stopGrab=false lc()cloneVictim=cloneChar(victim)local cam=workspace.CurrentCamera cam.CameraSubject=cloneVictim addConn(connections,newConn('Heartbeat',function()for _,tr in ipairs(LP2.Character.Humanoid:GetPlayingAnimationTracks())do tr:Stop()end LP2.Character.HumanoidRootPart.CFrame=cloneVictim.HumanoidRootPart.CFrame*CFrame.new(0,-15,0)*CFrame.Angles(math.pi/2,0,0)ZeroVelocity(LP2.Character.HumanoidRootPart)for _,p in ipairs({'Head','HumanoidRootPart','UpperTorso','LowerTorso','LeftUpperArm','LeftLowerArm','RightUpperArm','RightLowerArm','LeftHand','RightHand','LeftUpperLeg','LeftLowerLeg','RightUpperLeg','RightLowerLeg','LeftFoot','RightFoot'})do if victim[p]and cloneVictim:FindFirstChild(p)then align(victim[p],cloneVictim:FindFirstChild(p),CFrame.new())end end cloneVictim.Humanoid.Jump=LP2.Character.Humanoid.Jump cloneVictim.Humanoid:Move(LP2.Character.Humanoid.MoveDirection,false)end))local portal=sc(LP2.Character.HumanoidRootPart.CFrame+Vector3.new(0,9,0))task.wait(3)task.delay(2,function()cam.CameraType=Enum.CameraType.Scriptable local tp=Instance.new('Part',workspace)tp.Size,tp.Anchored,tp.CanCollide,tp.Transparency=Vector3.new(1,1,1),true,false,1 tp.Position=cloneVictim.HumanoidRootPart.Position Instance.new('Humanoid',tp)cam.CameraSubject=tp TS:Create(cam,TweenInfo.new(4,Enum.EasingStyle.Sine,Enum.EasingDirection.InOut),{CFrame=CFrame.new(cloneVictim.HumanoidRootPart.Position+Vector3.new(0,3,15),cloneVictim.HumanoidRootPart.Position)}):Play()task.wait(4)tp:Destroy()TS:Create(cloneVictim.HumanoidRootPart,TweenInfo.new(2,Enum.EasingStyle.Sine,Enum.EasingDirection.Out),{CFrame=cloneVictim.HumanoidRootPart.CFrame+Vector3.new(0,10,0)}):Play()task.wait(1.3)local vn=tostring(LP2.Character.BodyEffects.Grabbed.Value)if game.Players:FindFirstChild(vn)and game.Players[vn].Character then for _,limb in ipairs({'RightUpperLeg','LeftUpperLeg','RightUpperArm','LeftUpperArm','UpperTorso'})do if cloneVictim:FindFirstChild(limb)then cloneVictim[limb].Position=Vector3.new(0,-11000000,0)end if game.Players[vn].Character:FindFirstChild(limb)then game.Players[vn].Character[limb].Position=Vector3.new(0,-11000000,0)end end end task.wait(0.7)RS.MainEvent:FireServer('Grabbing')cam.CameraType=Enum.CameraType.Custom cam.CameraSubject=LP2.Character.Humanoid if portal then portal:Destroy()end end)end end)

