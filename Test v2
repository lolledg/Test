local RS=game:GetService('ReplicatedStorage')
local LP2=game:GetService('Players').LocalPlayer
local victim,stopGrab,cloneVictim,connections,rope=nil,false,nil,{},nil
getgenv().Furry_=false

local function align(p1,p2,p3)if not p1 or not p2 then return end p1.AssemblyLinearVelocity,p1.AssemblyAngularVelocity,p1.Velocity=Vector3.zero,Vector3.zero,Vector3.zero p1.CFrame=p2.CFrame*(p3 or CFrame.new())p1.CanCollide,p2.CanCollide=false,false end
local function addConn(t,c)if type(t)=='table'and c then t[#t+1]=c end end
local function newConn(e,f)if game:GetService('RunService')[e]then local c=nil c=game:GetService('RunService')[e]:Connect(function()local ok,err=pcall(f)if not ok then c:Disconnect()warn(err)end end)return c end end
function ZeroVelocity(p)if not p then return end p.Velocity,p.AssemblyAngularVelocity,p.AssemblyLinearVelocity=Vector3.zero,Vector3.zero,Vector3.zero end

local TS=game:GetService('TweenService')
local audioCounter=0
function StopAudio()RS:WaitForChild('MainEvent'):FireServer('BoomboxStop')end
function PlayId(id)if LP2.Backpack:FindFirstChild('[Boombox]')then LP2.Backpack['[Boombox]'].Parent=LP2.Character RS.MainEvent:FireServer('Boombox',id)LP2.Character['[Boombox]'].RequiresHandle=false LP2.Character['[Boombox]'].Parent=LP2.Backpack LP2.PlayerGui.MainScreenGui.BoomboxFrame.Visible=false end end
function AnimPlay(id,sp,tp,ft)if not LP2.Character or not LP2.Character:FindFirstChild('Humanoid')then return end for _,t in pairs(LP2.Character.Humanoid:GetPlayingAnimationTracks())do if t.Animation.AnimationId:match('rbxassetid://'..id)then t:Stop(0.5)end end local a=Instance.new('Animation')a.AnimationId='rbxassetid://'..id local p=LP2.Character.Humanoid:LoadAnimation(a)p.Priority=Enum.AnimationPriority.Action4 if ft then p:Play(ft)else p:Play()end if sp then p:AdjustSpeed(sp)end if tp then p.TimePosition=tp end a:Destroy()return p end
function AnimStop(id,ft)if not LP2.Character or not LP2.Character:FindFirstChild('Humanoid')then return end for _,t in pairs(LP2.Character.Humanoid:GetPlayingAnimationTracks())do if t.Animation.AnimationId:match('rbxassetid://'..id)then if ft then t:Stop(ft)else t:Stop()end end end end

local function cloneChar(char)if not char then return nil end char.Archivable=true local c=char:Clone()c.HumanoidRootPart.Anchored=false c.Humanoid.MaxHealth,c.Humanoid.Health=9e9,9e9 if c:FindFirstChild('RagdollConstraints')then c.RagdollConstraints:Destroy()end if c:FindFirstChild('BodyEffects')then c.BodyEffects:Destroy()end if c:FindFirstChild('GRABBING_CONSTRAINT')then c.GRABBING_CONSTRAINT:Destroy()end for _,p in pairs(c:GetDescendants())do if p:IsA('BasePart')and not table.find({'Head','HumanoidRootPart','UpperTorso','LowerTorso','LeftUpperArm','RightUpperArm','LeftLowerArm','RightLowerArm','LeftHand','RightHand','LeftUpperLeg','RightUpperLeg','LeftLowerLeg','RightLowerLeg','LeftFoot','RightFoot'},p.Name)then p:Destroy()end if p:IsA('BasePart')then p.CustomPhysicalProperties=PhysicalProperties.new(100,2,0.5,100,1)p.Transparency,p.CanCollide=1,false end if p:IsA('Decal')then p.Transparency=1 end if p:IsA('ForceField')or p:IsA('Motor6D')then p:Destroy()end end c.HumanoidRootPart.Transparency=1 c.Parent=LP2.Character c.Humanoid:ChangeState('GettingUp')c.HumanoidRootPart.CFrame=LP2.Character.HumanoidRootPart.CFrame char.Archivable=false for _,m in pairs({'LeftHand','RightHand','LeftFoot','RightFoot'})do local mt=char[m]:FindFirstChildOfClass('Motor6D')if mt then mt.Enabled=false end end return c end

game.Workspace.FallenPartsDestroyHeight=-math.huge
function Align(p,d,part,ref,pos,ang)if not LP2.Character or not LP2.Character.BodyEffects.Grabbed.Value or not victim or not victim[part]then return end if not(victim[part]:FindFirstChildWhichIsA('BodyPosition')or victim[part]:FindFirstChildWhichIsA('BodyGyro'))then local bp=Instance.new('BodyPosition',victim[part])bp.MaxForce=Vector3.new(math.huge,math.huge,math.huge)local bg=Instance.new('BodyGyro',victim[part])bg.maxTorque=Vector3.new(9e9,9e9,9e9)bg.P=10000 end if LP2.Character[ref]then victim[part].BodyGyro.CFrame=LP2.Character[ref].CFrame*ang victim[part].BodyPosition.Position=LP2.Character[ref].CFrame*pos.Position victim[part].BodyPosition.P,victim[part].BodyPosition.D=p,d end end
function Destroy(part)if not victim or not victim[part]then return end if victim[part]:FindFirstChildOfClass('BodyGyro')then victim[part]:FindFirstChildOfClass('BodyGyro'):Destroy()end if victim[part]:FindFirstChildOfClass('BodyPosition')then victim[part]:FindFirstChildOfClass('BodyPosition'):Destroy()end end
function Holding()repeat task.wait()Align(10000,175,'UpperTorso','RightUpperArm',CFrame.new(-0.2,-1.7,0.5),CFrame.Angles(math.pi*-0.5,math.pi,math.pi*0.07))until not LP2.Character or not LP2.Character.BodyEffects.Grabbed.Value or stopGrab end

function Grab()if not LP2.Character or not LP2.Character.BodyEffects then return end if LP2.Character.BodyEffects.Grabbed.Value then victim=LP2.Character.BodyEffects.Grabbed.Value local gc=victim:WaitForChild('GRABBING_CONSTRAINT')if not gc then return end rope=gc:FindFirstChildOfClass('RopeConstraint')if rope then rope.Length=math.huge end stopGrab=false for _,p in pairs(victim:GetChildren())do if p:IsA('BasePart')then p.CanCollide=false end end while true do task.wait()if not LP2.Character or not LP2.Character:FindFirstChild('Humanoid')then break end for _,t in pairs(LP2.Character.Humanoid:GetPlayingAnimationTracks())do if t.Animation.AnimationId:match('rbxassetid://11075367458')then stopGrab=true t:Stop()end end if stopGrab then for _,p in pairs(victim:GetDescendants())do if p:IsA('BasePart')then p.Velocity,p.AssemblyAngularVelocity,p.AssemblyLinearVelocity=Vector3.zero,Vector3.zero,Vector3.zero end end stopGrab=false AnimPlay(3135389157,0,0.1,0.3)Holding()AnimStop(3135389157)Destroy('UpperTorso')end end end end

function ToolSSs()

-- ══════════════════ 1. ACTIVATE ══════════════════
